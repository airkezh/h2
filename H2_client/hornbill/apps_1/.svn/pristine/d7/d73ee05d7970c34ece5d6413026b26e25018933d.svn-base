	}
	p.label{
		.h(15px);.bgc(#f7f7f7);.mt(20px);
		//2px #ff6699
		border-top:1px solid #d2d2d2;border-left:1px solid #d2d2d2;border-right:1px solid #d2d2d2;
		padding:10px 0 10px 20px;font-size: 14px;
	}
	margin:auto;
	.w(980px);
	#coupon{
	}
	.addr{
		.addressErrorMessage{
			.mt(4px);
			.ml(70px);
			.red;
		}
		.good,.bad,strong{
			.i_block;
			.w(15px);
			.h(15px);
			.ml(10px);
			padding-top: 4px;
		}
		.good,.bad{
			background:url("@{pic_url}/images/doota/validate_1.png") no-repeat bottom left;
			*background-position:0;
		}
		.good{
			background-position:-15px bottom;
			*background-position:-15px 0;
		}
		border-top:1px solid #d2d2d2;
		border-left: 1px solid #d2d2d2;
		border-right: 1px solid #d2d2d2;
		border-bottom: 1px solid #d2d2d2;

		.bgc(#ffffff);
