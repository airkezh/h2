	.id-card{
		.con{
			position: relative;
			border: 1px solid #d2d2d2;
			padding: 16px 20px 9px;
			line-height: 3.0;
			.h(46px);
			

			.advice{
				//color:#f69;
				color:#ff6699;
				margin-left: 5px;
			}
			.change{
				cursor: pointer;
				color: #f69;
			}
			.id-card-change{
				color: #f69;
				cursor: pointer;
			}
			.id-card-edit{
				//display: none;
				.aid-hidden{
					display: none;
				}
				.id-card-input{
					width:168px;
					border: 1px solid #ddd;
					color: #999;
					height: 34px;
					line-height: 20px;
					padding: 0 5px;
					box-shadow:inset 0 1px 2px rgba(0,0,0,.1);
				}
				.bad-reasons{
					color:#f69;
					margin-left: 5px;
				}
				.addressBtn{
					cursor: pointer;
				}

				#id-card-save{
					-webkit-border-radius: 3px;
					background: #f69;
					color: #fff;
					border: none;
					font-size: 12px;
					padding: 0;
					width: 68px;
					height: 34px;
					text-align: center;
					color: white !important;
					margin-left: 10px;
				}

				#id-card-cancel{
					-webkit-border-radius: 3px;
					background: #ccc;
					color: #fff;
					border: none;
					font-size: 12px;
					padding: 0;
					width: 68px;
					height: 34px;
					text-align: center;
					color: white !important;
					margin-left: 5px;
				}

				.bad{
					background: url("http://i.meilishuo.net/css/images/doota/validate_1.png") no-repeat bottom left;
					display: inline-block;
					
					width:15px;
					height: 15px;
				}

				.good{
					background: url("http://i.meilishuo.net/css/images/doota/validate_1.png") no-repeat bottom left;
					background-position: -15px bottom;
					display: inline-block;

					width:15px;
					height: 15px;
				}
			}
		}
	}

	.order{
		#coupon{
			margin-left:5px;
			margin-right:10px;
		}
		border-top:1px solid #d2d2d2;
		.mb(50px);
        .x-highlight { color:#f69; }
		td,th{
			.bgc(#ffffff);
		}
		th{
			.l(43px);
		}
