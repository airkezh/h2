				.l(normal);
				.clearfix();
				vertical-align: top;
				label{
					.left();
					position: relative;
					top: 10px;
				}
				textarea.comment{
					margin: 6px 0px;
					.left();
					border:1px solid #d2d2d2;
					font-size:12px;
					.w(285px);
					.h(15px);
					.l(15px);
					padding:3px 5px;
					&:hover{
						border-color: @red;
						outline: 0;
						box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075), 0 0 8px fadeout(@red, 40%);					
					}
					&:focus{
						border-color: @red;
						outline: 0;
						box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075), 0 0 8px fadeout(@red, 40%);
						height: 30px;
					}
				}
			}
		}
		td.content{
			.content-link{
				.pr();.block;.clearfix();
			}
			img,span{
				.i_block;
				.left;
				text-align:left;
			}
			img{
				.w(68px);
				.h(68px);
				.mr(22px);
				.ml(25px);
				border:1px solid #e6e6e6;
			}
			.presale-btn{
				.pa();.block();.h(16px);.w(68px);.bgc(#ff6699);.c();.l(16px);.fc(#ffffff);top:69px;left:26px;
			}
			span.title{
				.l(20px);
				.w(238px);
				.h(58px);
			}
			span.props{.w(238px);
				span{
					.gray;
					.mr(26px);
				}
			}
		}
		tbody.goods td{
			border-bottom:1px dotted #bfbfbf;
			.presale-time{
				.block;.l(24px);
			}
		}
		tbody.goods td.first,tbody.goods td.last{
			border-bottom:0;
		}
		thead th{
			border-bottom:1px solid #d2d2d2;
		}
		thead th.first,tbody td.first{
			border-left:1px solid #d2d2d2;
		}
		thead th.last,tbody td.last{
			border-right:1px solid #d2d2d2;
		}
		tbody tr.first td{
			border-top:1px solid #d2d2d2;
		}
		tbody tr.last td,.total td{
			border-bottom:1px solid #d2d2d2 !important;
		}
		tbody.empty td{
			background:none;
			.h(55px);
			border:0 !important;
			&.c{
				.c;
			}
		}
		tbody.empty .blank{
			width: 10px;
			height: 20px;
		}

		tbody.empty td#top-empty{
			background:none;
			.h(15px);
			border:0 !important;
			&.c{
				.c;
			}
		}
		tfoot td{
			.h(52px);
			.l(52px);
			.ie6_left{
				float:left;
				display:block;
				*padding:15px 0;
			}
			border-top:1px solid @red;
			border-bottom:1px solid @red;
			
			&.first{
				border-left:1px solid @red;
			};
			&.last{
				border-right:1px solid @red;
			}
			span.right{
				.i_block;
				.w(420px);
				.right;
				.h(52px);
				.tr; .pr(190px);
				position:relative;
				.cart{
					.red;
					.mr(30px);
				}
				.price{
					.f(20px);
					.red;
				}
				.go_charge{
					position:absolute;
					top:-1px;
					right:0;
					.w(150px);
					.h(54px);
					.i_block;
					background:url("@{pic_url}/images/doota/save_order.png");
				}
			}
		}
		tfoot .init_msg{
			td{
				border:none;
				.init_msg_content{
					font-size: 12px;line-height: 18px;
					text-align: right;
					em{
						display: inline-block;zoom:1;font-weight: bold;
						color:#ffffff;background: #ff83ad;padding:0px 3px;margin-right: 10px;
					}
				}
			}
		}
	}

