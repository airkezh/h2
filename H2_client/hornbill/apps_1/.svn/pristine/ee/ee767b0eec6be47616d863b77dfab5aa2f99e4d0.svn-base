@import "storeAtom.less";
.page;
.pageNav{ .page > .pageRed; }
.nav_bar;
.page_title {.h3_title;.fb;.border-1(#e6e6e6);
	span {.left;}
	.query { margin: 10px 15px; .f(12px); .h(25px); .l(25px); .right; .bgc(#ff679a); .fc(#fff); .c; .none; .border-r;.w(65px); border: none; 
		&:hover{.cursor}}
}
.post{.w(738px);.bgc(#fff);.border-1(#e6e6e6);.pl(20px);.pr(20px);.mt(-1px);
	.content_wrap{.pt(20px);.pb(20px);.l(30px);.f(16px);
		.back_detail{.f(12px);.red;.ml(10px);}
	}
}
.tab_wrap{.mt(10px);border-bottom: 2px #f69 solid;
	a{.left;.w(112px);.h(38px);.l(38px);.c;.fb;.red;.bgwhite;}
	.selected{.bgred;.white;}
}
.list_wrap{.w(738px);.bgc(#fff);.border-1(#e6e6e6);.mt(-1px);padding: 20px;
	.search_wrap{
		.pb(20px);.mb(20px);.border-bc(dotted);
		.goods_title{.w(348px);}
		.input_txt{.mr(20px);}
		.search_sub{.w(70px);.h(25px);.bgred;.white;border: 0;.border-r;.cursor;}
	}
	.list_table{.w(100%);border-collapse: collapse;
		.blank_tr{border: 0;
			td{.h(10px);}
		}
		tr{.border-1(#e6e6e6);
			&.title_tr{.h(48px);}
			td{.c;
				&.goods_td{.w(280px);.c;.pt(19px);.pb(19px);.pl(25px);.l(22px);
					div{.vm;.hidden;}
					img{.w(60px);.h(60px);.left;}
					span{.w(180px);.tl;.ml(9px);.left;}
				}
				.apply_btn{.bgred;.white;padding: 5px 10px;.border-r(3px);}
				.unable{.gray;}
			}
		}
	}
}