/* 弹窗 */
.calendar_box{
	.pf;
	top:8%;
	left:3%;
	.w(94%);
	.pt(124%);
	z-index:99;
}

.calendar_close{
	.pa;
	.block;
	.w(10%);
	.pt(10%);
	top:-3%;
	right:-1%;
	z-index:999;
	background:url("http://d02.res.meilishuo.net/pic/_o/65/ea/aa1428023719379bc2db1f93d773_49_49.ch.png") no-repeat;
	background-size:100%;
}

.layer_shade{
	.pf;
	top:0;
	left:0;
	.w(100%);
	.h(100%);
	z-index:10;
	background:rgba(0,0,0,.5)
}

.slide{
	.pr;
	.hidden;
	.w(600*@w);
	margin:20*@w auto;
}

.slide_wrap{
	.pr;
	.hidden;
	-webkit-backface-visibility:hidden;
	-moz-backface-visibility:hidden;
	-ms-backface-visibility:hidden;
	-o-backface-visibility:hidden;
	backface-visibility:hidden;
	/* Fix of Webkit flickering */
	z-index:2;
}

.slide_wrap > div{
	.pr;
	.left;
	.w(100%);
}

.slide_pic{
	.pr;
	.hidden;
	.w(100%);
	.bgc(#fff);
}

.slide_pic img{
	.w(100%);
	min-height:120px;
}

.list_pics{
	clear:both;
	overflow:hidden;
	*zoom:1;
}

.list_pics li{
	.tl;
	.left;
	.inline;
	.w(31.2%);
	.mr(3.2%);
}

.list_pics li img{
	.w(100%);
}

.list_pics li:nth-child(3n){
	.mr(0);
}

/* 组图 */
.swipe{
	.pr;
	.c;
	.hidden;
	min-height:240*@px;
}

.swipe_wrap{
	.pr;
	.hidden;
	-webkit-backface-visibility:hidden;
	-moz-backface-visibility:hidden;
	-ms-backface-visibility:hidden;
	-o-backface-visibility:hidden;
	backface-visibility:hidden;
	/* Fix of Webkit flickering */
	z-index:1;
}

.swipe_wrap > div{
	.pr;
	.left;
	.w(100%);
}

.swipe_pic{
	.pr;
	.hidden;
	.w(100%);
	.bgc(#fff);
	min-height:240*@px;
}

.swipe_pic img{
	.w(100%);
	min-height:240*@px;
}

.swipe_num{
	.pa;
	left:0;
	.w(100%);
	bottom:8px;
	z-index:9;

	li{
		.vm;
		.w(5px);
		.h(5px);
		.mr(2px);
		.i_block;
		.bgc(#fff);
		float:none;
		.border-r(50%);
	}

	li.active{
		border:1px solid #fff;
		background-color:transparent;
	}
}

.wrapperIndexActivity{
	.pr;
	.block;
	.hidden;
	.w(620*@w);
	.h(100%);
	margin:20*@w 0 20*@w 20*@w;
	z-index:1;

	.scrollerActivity{
		.left;
		.hidden;
		.h(100%);
		padding:0;

		ul li{
			.left;
			.block;
			.w(90px);
			.mr(10px);
			box-sizing:border-box;
			-webkit-box-sizing:border-box;
			&:last-child{
				.mr(0);
			}
		}  
	} 
}

@media screen and (min-width:321px){
	.wrapperIndexActivity .scrollerActivity ul li{
		.w(100px);
	}
}
