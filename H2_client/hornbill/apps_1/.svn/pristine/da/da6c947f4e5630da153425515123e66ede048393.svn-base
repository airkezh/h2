@import "../../block.less";

html{
	.f(10*@f);
}

body{
	.bgc(#fff);
}

img{
	.block;
	.w(100%);
}

a{
	.block;
	outline:none;
}

/* 0.5的水平线 */
.line-h05(@lw){
	width:@lw;
	height:1px;
	-webkit-transform:scaleY(.5);
	background-color:rgb(213,213,213);
}

.lhp100{
	.line-h05(100%);
}

.banner_wrap{
	.pr;
	.w(100%);
	.pt(160*@w);
	background:url("http://d02.res.meilishuo.net/pic/_o/b1/fd/b97ace4b94d05539be119caf96e5_640_160.ch.jpg") no-repeat;
	background-size:100%;

	.sign_bonus{
		.pa;
		.c;
		.w(20%);
		top:56%;
		left:10%;
		.f(2.5rem);
		.fc(#ff5f53);
	}

	.sign_txt{
		.pa;
		top:14%;
		left:35%;

		p{
			.l(1.6);
	    	.f(1.2rem);
			.fc(#fff);

			a{
				.fc(#fff);
			}

			em{
				.fc(#ffe814);
				font-style:normal;
			}
		}

		p.tit{
			.f(1.6rem);
		}
	}

	.icon_date{
		.pa;
		top:20%;
		right:1rem;
		.w(76*@w);
		.pt(93*@w);
		background:url("http://d03.res.meilishuo.net/pic/_o/95/3b/634d541bf3248f1491eb7dc5170d_76_93.cg.png") no-repeat;
		background-size:100%;
	}

	.icon_animate{
		-webkit-animation:shake 1s .2s ease both;
		-moz-animation:shake 1s .2s ease both;
		@-webkit-keyframes shake{
			0%,100%{-webkit-transform:translateX(0);}
			10%,30%,50%,70%,90%{-webkit-transform:translateX(-10px);}
			20%,40%,60%,80%{-webkit-transform:translateX(10px);}
		}
	}
}

.coupon_wrap{
	.l(1.5);
	.f(1.2rem);
	.hidden;
	clear:both;
	padding:.6rem 1rem;
	border-bottom:1px solid #dcdcdc;

	.title{
		.left;
		.fc(#333);
		.i_block;
		.pl(32px);
		background:url("http://d03.res.meilishuo.net/pic/_o/5e/8c/b4b502a907417ae3a5ba403c8cd7_51_34.cg.png") no-repeat;
		background-size:auto 17px;
	}

	.text{
		.right;
		.fc(#ff5f53);
	}

	.text_box{
		.right;
		.fc(#b6b6b6);

		span{
			.i_block;
			.ml(.4rem);
			.bgc(#fff);
			.fc(#ff5f53);
			.border-r(5px);
			padding:0 1rem;
			border:1px solid #ff5f53;
			
		}
	}
}

.update_tips{
	.l(1.5);
	.hidden;
	clear:both;
	.f(1.2rem);
	.fc(#ff5f53);
	.bgc(#fff8f7);
	padding:.4rem;
	border-bottom:1px solid #ffdbd8;

	span{
		.right;
	}
}

.head_wrap{
	.c;
	margin:1.3rem auto;

	.head{
		.vm;
		.w(10%);
		.h(10%);
		.i_block;
		.mr(1rem);
		.border-r(50%);
		overflow:hidden;

		img{
			border:0;
			.border-r(50%);
		}
	}

	.award{
		.vm;
		.f(1.5rem);
		.fc(#ff5f53);
	}
}

.desc_wrap{
	margin:8px 10px;

	p{
		.l(1.6);
		.f(1.2rem);
		.fc(#666);
	}

	p.title{
		.f(1.5rem);
		.fc(#ff5f53);
	}
}

.nav_box{
	.w(100%);
	min-height:49px;
	overflow:hidden;

	.nav{
		.c;
		.w(100%);
		.bgc(#fff);
		clear:both;

		li{
			.c;
			.l(1.8);
			.w(22%);
			.i_block;
			margin:8px 0 8px;

			a{
				.block;
				.f(1.4rem);
				.fc(#80737b);
			}
		}

		li.curr{
			a{
				.f(1.5rem);
				.fc(#f69);
			}

			border-bottom:solid 1px #f69;
		}
	}
}

.shadow{
	box-shadow:7px 2px 7px #e6e6e6;
	/*for ie6,7,8*/		
	filter:progid:DXImageTransform.Microsoft.Shadow(Strength=7, Direction=180, Color='#e6e6e6');
}

.scrollin{
	.pf;
	top:-1px;
	left:0;
	right:0;
	margin:0 auto;
	z-index:100;
}

.scrollout{
	.pr;
}

.banner_box{
	.hidden;
	margin:0 1rem;

	li{
		.left;
		.w(100%);
		.mt(1rem);
	}
}

.recommend_box{
	.hidden;
	margin:0 1rem 1rem;

	li{
		.left;
		.w(48.3%);
		.mt(1rem);
	}

	.odd{
		li:nth-child(odd){.mr(3.4%);}
	}
}

.container{
	.hidden;
	margin:20*@px;
	min-height:700px;

	.even{
		li:nth-child(odd){.mr(3%);}
	}

	.odd{
		li:nth-child(odd){.mr(3%);}
	}
}

.goods_wall{
	.clearfix;

	li{
		.left;
		.mr(0);
		.w(48.5%);

		.link_box{
			.block;
		}

		.hjfpos{
			.pr;

			.ownh{
				height:61px;
			}

			.adenter{
				.pa;
				top:0;
				left:0;
				.h(100%);
			}
		}

		figure{
			.pr;

			.soldout{
				.pa;
				top:50%;
				left:50%;
				z-index:10;
				.w(298*@w);
				.pt(298*@w);
				margin:-149*@w 0 0 -149*@w;
				background:url("http://d04.res.meilishuo.net/pic/_o/1a/d7/70bb562514b6d8f86d9dcf7699d9_149_149.ch.png") no-repeat center;
				background-size:100%;
			}

			span.bs_load_img{
				.block;
				.w(100%);
				.pt(140%);
				opacity:.75;
				background-image:url("http://d01.res.meilishuo.net/pic/_o/b9/0a/fd965c6f94c2fd9df704e481098f_304_425.cg.png");
				/*background-image:url("http://d05.res.meilishuo.net/pic/_o/33/cf/a618bae2cdf43bc03289e82cb53f_290_290.cg.png");*/
				background-size:cover;
			}

			span.df_show_img{
				.block;
				.w(100%);
				.pt(140%);
				opacity:.75;
				background-image:url("http://d01.res.meilishuo.net/pic/_o/b9/0a/fd965c6f94c2fd9df704e481098f_304_425.cg.png");
				/*background-image:url("http://d05.res.meilishuo.net/pic/_o/33/cf/a618bae2cdf43bc03289e82cb53f_290_290.cg.png");*/
				background-size:cover;
			}

			div.new_png{
				.pa;
				.w(100%);

				div{
					.left;
					.ml(6/290*100%);

					img{
						.border-r(0 0 0 0);
					}
				}
			}

			.lave{
				.pa;
				.c;
			    .w(100%);
			    .fc(#fff);
			    .f(1rem);
			    bottom:0;
			    z-index:90;
			    padding:4px 0;
		    }

		    .progress{
		    	.pa;
				.block;
				.f(10px);
				.w(100%);
				.h(12px);
				bottom:0;
				opacity:.6;
				padding:4px 0;
		    }

		    .rpbgc{
		    	.bgc(#dd8200);
		    }

		    .gpbgc{
		    	.bgc(#80737b);
		    }

		    .ypbgc{
		    	.bgc(#ffb414);
		    }
		}

		figcaption{
			.bgwhite;
			padding:8px 0 12px;

			.pro{
				&Title{
					.hidden;
					.l(1.2);
					.f(1.2rem);
					.fc(#666);
					text-overflow:ellipsis;
					display:-webkit-box;
					-webkit-line-clamp:1;
					-webkit-box-orient:vertical;
					clear:both;
				}

				&Price {
					.l(2);
					.i_block;
					.fc(#f69);
					.f(32*@px);

					i{
						font-style:normal;
					}

					.discount{
						.fc(#fff);
						.i_block;
						.f(1.0rem);
						.bgc(#ffaa1d);
						padding:0px 6px;
						.border-r(12px);
						.vm;
						.ml(2px);
						.mt(-4px); 
						.l(18px);
					}

					.word { 
						.fc(#999);
						.f(1.0rem);
					}
				}
			}
		}
	}
}

.more_bar{
	.c;
	.none;
	.w(90%);
	.l(1.4);
	.f(1.3rem);
	.fc(#595056);
	.border-r(4px);
	padding:8px 0;
	margin:10px auto;
	border:solid 1px #595056;
}

.pullUp{
	.pullUpDown > .common;.pullUpDown > .loading;
	&:before{
		.nowrap;
		.ml(-10px);
	}
	&[status="tap"]{
		background:0 none;	
	}
	&[status="stop"]{
		background:0 none;	
	}
}

.gotop_wrap{
	.pf;
	.none;
	.w(82*@w);
	.pt(82*@w);
	z-index:10;
	right:10px;
	bottom:40px;

	.gotop{
		.block;
		.w(100%);
	}

	img{
		.w(100%);
	}
}

