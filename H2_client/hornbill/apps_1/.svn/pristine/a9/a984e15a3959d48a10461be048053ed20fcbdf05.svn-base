<%#head.html%>
<div id="start" class="page">
	<div class="gap">
		<span id="start_play"></span>
	</div>
</div>
<div id="play" class="page">
	<div class="cities"></div>
	<div class="inputs">
		<input type="text" readonly id="score" value="0"/>
		<input type="text" readonly id="remain_time" value="30"/>
	</div>
	<div class="btn">
		<span id="click"></span>
		<span id="calculating"></span>
	</div>
</div>
<div id="final" class="float">
	<div class="score">
		<strong id="result_score">0</strong>
	</div>
	<div class="rank">
		<strong id="result_rank">0</strong>
	</div>
	<div class="share">
		<span id="share"></span>
	</div>
	<div class="watch_live">
		<a id="watch_live" href="/goto/open/?url=meilishuo%3A%2F%2Fcircle.meilishuo%3Fjson_params%3D%257B%2522circle_id%2522%253A%252235143%2522%257D"></a>
	</div>
	<div class="replay">
		<span id="replay"></span>
	</div>

</div>
<div class="cover"></div>
<%! $_ENGINE_SELF.callFn("useModule")("<<<_use:
	fml.use('wap/luhan/index');
_use:", this._JSstack , this._JSmods ); %>
<%#foot_simple.html%>
