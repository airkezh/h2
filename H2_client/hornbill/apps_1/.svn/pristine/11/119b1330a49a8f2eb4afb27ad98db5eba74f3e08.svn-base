.hotDeals {padding:0 1px 20px;
	.hotCon {.left;.w(228px);margin:20px 12px 0 0;}
	.hotImg {.w(226px);border:1px solid #FFF;
		img {.w(226px);.block;}
		}
	.hotCon:hover {
		.hotImg {border-color:#F69;}
		a {.red;}
		div{.red;}
		}
	.hotTitle {.pt(10px);.l(18px);
		span {padding:0 10px;}
		div{.red;}
		text-overflow: ellipsis;
		white-space: nowrap;
		width: 211px;
		overflow: hidden;
		}
	.hotTitle1 {.pt(10px);.l(18px);
		span {padding:0 10px;}
		div{color: #999;.f(12px);}
		text-overflow: ellipsis;
		white-space: nowrap;
		width: 228px;
		overflow: hidden;
		.f(14px);
		// font-family: "Hiragino Sans GB", "Microsoft YaHei", sans-serif;
		-webkit-font-smoothing: antialiased;
		}
}
