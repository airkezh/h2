}
.top_mlsslide{
	.w(100%); .border-bc(solid); .pr; .h(340px);
	.mlsslide_img{
		li{
		 .w(100%); .h(340px); .pa; .cursor; 
		 background-position: center top; background-repeat: no-repeat; opacity: 0; filter:alpha(opacity=0);
		 .none;
			a{.block; .w(100%); .h(340px);
		}
	}
}
	.mlsslide_tit{ .w(950px); .auto; top: 308px; z-index: 300; .pr; .h(32px); .hidden; .nowrap;
		li{.left; .l(32px); .f(12px); .fc(#fff); .c; .w(316px); .cursor; .ml(1px); background:rgba(0,0,0,0.5); filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#80000000', endColorstr='#80000000');}
		.slide_tit_active{background:rgba(255,43,114,0.5); filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#80ff2b72', endColorstr='#80ff2b72');}
	  .firLi{ .ml(0);}
  }
}
