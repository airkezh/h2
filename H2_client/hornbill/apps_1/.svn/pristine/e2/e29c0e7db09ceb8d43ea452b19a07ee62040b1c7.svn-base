@import "../../block.less";
@import "../tuanbase.less";
@import "../imgSlide.import.less";

html,body{
	.bgc(#fff);
	.w(100%);
	.f(10px);
}
.main{
	.pr;
	.hidden;
	min-width: 320px;
	margin: 0 auto;
	max-width: 640px;
}
.slideWraper{
	.pr;
}
.slide;
img{
	.w(100%);
	.block;
}
ul{list-style:none;}
i{font-style:normal;}
.line{
	.pa; 
	top: 30px;
}
.f_14{.f(1.4rem);}
.f_15{.f(1.5rem);}
.f_30{.f(3rem);}
.f_23{.f(2.3rem);}
.f_25{.f(2.5rem);}
.red_f1{.fc(#f66);}
.top-bnr{.pr;.hidden;}
.a-desc{
	.none;
	.w(20%);
	.pa;
	right: 0;
	bottom: 20px;
}
.movelr{
	-webkit-animation: movelr 1s .2s 2 ease both;
}
.rbclose{
	.w(50%);
	.h(50%);
    .pa;
    top: 10px;
    right: 10px;
}
.nav_wap{
	min-height: 3.6rem;
}
.navbox{
	.none;
	.w(100%);
	.f(1.4rem);
	.fc(#80737b);
	.bgc(#f5f5f5);
}
.scrollin{
	.pf;top:-1px;left: 0; margin: 0 auto; z-index:100;
}
.scrollout{
	.pr;
}
.lhp100{
	.line-h05(100%);
}
.act-tip{
	.pt(10px);
	.pb(10px);
	.f(1rem);
	.bgc(#f5f5f5);
	.split{
		.i_block;
		.w(1px);
		.h(.9rem);
		.ml(14*@w);
		.mr(14*@w);
		.bgc(#dadada);
	}
	.t-c{
		.right;
		.mr(.9rem);
	}
}
.lhp33{
	.line-h05(33%);
}
.navbl{
	.mt(-16px);
}
.navtip{
	.left;
	.ml(8px);
	.w(60px);
	background: url('@{pic_url}/images/wap/tuan/navtip.png') center center no-repeat;
	background-size: 104% 70%;
	text-indent: -999px;
}
.points {
	position: relative;
	overflow: hidden;
	.w(100%);
	margin: 0 auto;
	height: 58px;
}
.points s {
	width: 52px;
	height: 58px;
	position: absolute;
	left: 50%;
	margin-left: -26px;
	background: url('http://d01.res.meilishuo.net/pic/_o/9e/07/e796f0e2feed9aca188e16f3ffad_104_91.cf.png') top center no-repeat;
	background-size: 125%;
	z-index: 10;
}

.points ul {
	list-style: none;
	-webkit-transform: translate3d(0,0,0);
	-webkit-transition: all .3s ease-in-out;
	width: 1000px;
	overflow: hidden;
	position: relative;
	z-index: 10;
}
.points ul li {
	float: left;
	line-height: 17px;
}
.points ul li a {
	.fc(#a6a4a5);
	.f(1.4rem);
	display: block;
	//width: 63px;
	margin-top: 5px;
	text-align: center;
	text-decoration: none;
	font-weight: 700;
	outline: 0;
	font-weight: 400;
	font-family: "Helvetica Neue";
	label{
		.block;
		.f(1rem);
	}
}
.points ul .curr a{
	.fc(#fff);
}
#content_box{
	.pl(14*@w);.pr(14*@w);
	
}
.zhengdian{			
	.mt(14px);.pr;.bgc(#fff);min-height:140px;
	>figure{.w(45%);.i_block;.pr;.mt(2px);
	}
	.desc{.w(55%);.right;
		li{.ml(7px);.mr(7px);.pa;.w(53%);}
		li.li-a{top:1%; overflow: hidden; white-space: nowrap;}
		li.li-b{top:19%;}
		li.li-c{top:45%;}
		li.li-d{bottom:3px;}
		
	}
	.now_price{
		.ml(-6px);
	}
	.old_price{.ml(-4px); .f(1rem); .fc(#b3acb0);}
	.gcata{
		display: inline-block;
		line-height: 15px;
		width: 38px;
		i{
			.f(0.9rem);
			.fc(#fff);
			padding: 1px 5px;
			.ml(-1px);
		}
	}
	.t_count{
		.f(1rem);
		.fc(#80737b);
	}
	.red_bg{ .bgc(#f66);}
	.yel_bg{
		.bgc(#ffb414);
	}
	.mark{
		position: absolute;
		top: -10px;
		left: -10px;
		.w(45px);
		.h(45px);
		background: url('@{pic_url}/images/wap/tuan/mflag.png') center center no-repeat;
		background-size: 100% 100%;
	}
	.saleinfo{
		p{
			.fc(#80737b);
			height: 1.5rem;
			.f(1rem);
			
			line-height: 1.5rem;
			
		}
		.scale{
			border: solid 1px rgba(255, 232, 20, .6);
			text-align: center;
			position: relative;
			z-index: 5;
		}
		.progress{
			display: block;
			height: 1.6rem;
			opacity: .6;
			.bgc(#ffe814);
			margin-top: -1.6rem;
			margin-left: 1px;
		}
	}
}
.status_btn {
	.pt(10px);
	.pb(10px);
	.f(1.4rem);
	.fc(#fff);
	.block;
	text-align: center;
	background-color: #ffac5c;
}
span.over_btn,li.over_btn{.bgc(#ccc;);}
span.reminded_btn,li.reminded_btn{background:#ccc;}
span.empty_btn,li.empty_btn{.bgc(#b6b6b6);} 
span.buy_btn,li.buy_btn{
	.bgc(#da333e); 
	a{.block;.w(100%);
		.h(100%); 
		.fc(#fff);
	}
}
/*dialog*/
.dialog();
#dialogLayer .dialogTitle .close{.none;}
.dialog_box{
	.pt(1rem);.c;
	p{ .l(2rem); .f(1.4rem); }
	.mask_btn{.white; .border-r(40*@px); .border-n; .f(1.4rem); padding: 1% 1rem;.w(32%); .left;.ml(4%);.mt(20px);
		}
	.r_btn{.right;.mr(5%);background:#f69;}
	.l_btn{background:#ffad5c;}
}
/* 分享 */
.starBox { .star_box; }
@-webkit-keyframes movelr{
	0%{-webkit-transform:translate(5px,0);}
	50%{-webkit-transform:translate(0px,0);}
	100%{-webkit-transform:translate(5px,0);}
}
@media screen and (min-width: 360px){
	html,body {
	.f(12px);
	}
}