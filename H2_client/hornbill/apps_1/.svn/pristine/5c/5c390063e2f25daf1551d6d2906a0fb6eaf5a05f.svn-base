@import "common.import.less";
@font-face {font-family: 'historyIcon';
  src : url('data:application/font-woff;base64,') format('woff');
}
body{background-color: #FAFAFA}
@bubbleBorderColor:#e6e6e6;
@mBgColor:#fa6ea1;
@uBgColor:#fff;
.LinkBlue(){.fc(#45b9fc);}
.LineBg(){.pr;.c;.h(24px);
    &:after{.pa;content:'';left: 40px;right:40px;.h(0);top:37%;z-index: 1;
      border-top:1px solid #c2c2c2;
    }
	span{background-color: #fafafa;.pr;z-index: 2;padding:0px 10px 6px 10px;.fc(#5a5557);}

}
.ConCommon(@bgColor:#fff){
	.hidden;
    .mb(10px);
	.con{.pr;max-width: 80%;
		.bubble{
          border:1px solid #ff6699;
          .bgc(@bgColor);
          word-wrap:break-word;
          word-break:break-all;
			.border-r(5px);.l(18px);
          padding: 8px;
          min-height: 18px;
          a{.LinkBlue();}
          .status_wrapper{.pa;}
		}
		.shap{.pa;.w(7px);.h(38px);}
		svg{
			.shap_path{
				stroke:#ff6699;
				fill:@bgColor;
				stroke-width:1px;
			}
		}
	}
	.ctime{.pb(10px);.fc(#ccc);.c;}
}
.main{
	.history_btn{.cursor;.auto;.w(200px);.h(25px);.border-r(15px);.c;.mt(10px);.mb(10px);.f(12px);.fc(#1677df);
		&:after{content:'查看历史消息';}
		&.loading{cursor: auto;
			&:after{content:'正在获取历史消息...';}
		}
      span {
        font-family:"historyIcon" !important;
        font-size:16px;
        font-style:normal;
        -webkit-font-smoothing: antialiased;
        -webkit-text-stroke-width: 0.2px;
        -moz-osx-font-smoothing: grayscale;
      }
	}
}
.userInfo{
	.w(170px);
	img{
		.w(40px);
		.h(40px);
	}
	span{
		.pl(10px);
		.pr(10px);
		.w(110px);
		.l(40px);
		.h(40px);
		.block;
	}
}
.d_{
	&u{ //other
		.con{.left;
          img {left:10px;}
        }
        svg{
          .shap_path{
            stroke:#dedede !important;

          }
        }
		.ConCommon(@uBgColor);
		.bubble{.left;border: 1px solid #dedede !important;
          .fc(#444);
          margin-left: 54px;
        }
		.shap{.Transform(rotateY(180deg));}
		.shap1{left:0px;}
		.shap2{left:48px; top: 11px}
		.userInfo{.left;
			span{
				.left;
			}
			img{
				.right;
			}
		}
        .con .bubble-no-avatar{
          margin-left: 16px;
        }
        .con .svg-no-avatar {
          left : 10px;
        }
	}
	&m{ //mine
		.userInfo{.right;
			span{
				.right;
			}
			img{
				.left;
			}
		}
		.con{.right;
          img {right:10px}
        }
      svg{
        .shap_path{
          stroke:#dbdbdb !important;
          fill:#fff2f6 !important;
        }
        .shap_path_file {
          fill:#fff !important;
        }
      }
		.ConCommon(@mBgColor);
        .con .bubble{.right;.mr(55px);
            .fc(#666);
              background-color: #fff2f6;
              border: 1px solid #dbdbdb;
			.Wait(){opacity: 0.7;}
			&.wait{.Wait();
				& + .shap{
					.shap_path{.Wait();}
				}
			}
			&.fail{.cursor;
				.status_wrapper{left: -65px;bottom: 0;.bgc(#f00);.white;.border-r(5px);padding:0 3px; &:after{content:'发送失败'}}
				&:hover{border-color: #f00;
					& + .shap{
						.shap_path{stroke:#f00;}
					}
				}
			}
			&.succ{}
		}
        .file_bubble {
          background-color: #fff !important;
        }
        .con .bubble-no-avatar{
          margin-right: 16px;
        }
        .con .svg-no-avatar {
          right : 10px;
        }
		.shap1{right:0px;}
		.shap2{
          right:49px;
          top: 11px;
        }
	}
	&trans{.pl(20px);.pr(20px);
		.line_start,.line_end{.LineBg();}
		.trans_msg{.mb(5px);.l(150%);
			.t_user{.LinkBlue();}
			.t_time{.ml(36px);}
			.t_service{.red;}
			p{
				a{.LinkBlue();}
			}
		}
	}
	&history{.none;
		.history_list{}
		.history_line{.LineBg();}
	}
	&sys{.c;.mb(10px);
		.sys_msg{.l(18px);.c;.bgc(#e9ebec);.border-r(18px);.fc(#666);.i_block;padding: 6px 12px;}
	}
}
.offline_tip{
  .pr;
	z-index: 2;
	.l(26px);.bgc(#fef7d6);text-align: left;.w(100%);top: 0;.none;.f(12px); .fc(#333); text-indent: 34px;
    .offline-tips-icon{
      .pa;
      top:0px;
      left: 10px;
    }
    .offline-tips-close {
      .pa;
      right:8px;
      text-indent: 0;
      cursor: pointer;
      top: 1px;
    }

}

.file_left{.left;
	img{}
}
.file_middle{.left;.l(16px);.ml(5px);
	.file_name{.w(132px);.hidden;white-space: nowrap;text-overflow:ellipsis;}
	.file_size{.fc(#999);}
}
.file_right{.left;.ml(5px);.w(63px);
	.file_status{.red;.c;}
	.file_btn{.bgred;.white;.cursor;.c;.w(58px);.auto;.border-r(2px);}
}

.facetableSetxy {
  width:36px;
}
