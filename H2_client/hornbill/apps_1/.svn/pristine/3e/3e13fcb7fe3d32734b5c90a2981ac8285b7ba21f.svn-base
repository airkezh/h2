fml.define('app/logstatics', ['jquery'], function(require, exports){
	var $ = require('jquery');
	
	exports.clicklog = function(url) {
		var logWorker = new Image;
		logWorker.src = url;
	//	$('body').append('<img width="0" height="0" src="'+url+'"/>');
	}
	exports.clickgetlog = function(url) {
		$.get(url);
	}
});
