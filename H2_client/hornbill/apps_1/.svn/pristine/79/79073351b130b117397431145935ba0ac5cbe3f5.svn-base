@import "../common/block.less";
.mt18_f{ .mt(18px);}
.comment_title{.mt(21px);.mb(21px);.w(978px);.auto;
	.comment_icon{.w(114px);.h(32px); background:url("@{pic_url}/images/shop/comment_title.png") no-repeat;}
}
.comment_nav{.w(978px);.auto;}
.comment-main{ .w(978px); .auto; border-style: solid; border-color: #f69 #e6e6e6 #e6e6e6 #e6e6e6; border-width: 2px 1px 1px 1px; .bgc(#fff);.mt(10px);
   .goods-list{ padding: 20px; .clearfix;
      .goods-info{ .w(461px); .left; .pt(7px);
         .item-detail{ .block; .h(70px); .hidden; .mb(40px);
            .pic{ .left; .w(70px); .h(70px);}
            .title{ .ml(90px); .mb(24px); .w(350px); .ellipsis;}
            .prop{ .ml(90px); .l(12px); .dgray; .h(12px);
               span{ .i_block; .w(160px); .tl; .ellipsis;
                  em{.gray;}
               }
            }
         }
         .order-info{ 
            em{ .gray;}
         }
         .shop-info{ .mt(32px);
            .shop-name{ .f(14px); .mb(10px);}
            .goqq { .i_block; .cursor; .h(23px); .l(23px); .f(12px); .c; .red; .w(86px);
               .qqico { background: url("@{pic_url}/images/doota/doota_im1.gif") no-repeat; .pt(2px); .pb(3px); .pl(18px); }
            }
         }
      }
      .comment-cont{ .left; .pl(20px); .pb(20px); .w(425px); 
         .com-item{
            span{ .left; .w(60px); .l(30px);}
            .com-item-list{ .ml(60px); .clearfix;}
            .flower{ .c; .left; .w(70px); .h(30px); .l(30px); .mr(10px); .tl;
               i{ .i_block; vertical-align: middle; .w(19px); .h(30px); .mr(5px); background: url("@{pic_url}/images/doota/sale1501201617.png") no-repeat;}
            }
            .flower.active{ text-decoration: underline;}
            .h-flower{ .fc(#fb8080); i{.bp(-249px, -140px);}}
            .n-flower{ .fc(#ffbe58); i{.bp(-272px, -140px);}}
            .c-flower{ .fc(#b3a7ae); i{.bp(-294px, -140px);}}
            .feature{ .w(240px);}
            .rate-tag{ .w(70px); .h(30px); .mr(10px); .mb(10px); .l(30px); .bgc(#f2f0f0); .left; .c;}
            //a.rate-tag:hover{ .bgred; .white;}
            .rate-tag.active{ .bgred; .white;}
            .txtarea{.pr; .w(360px);}
            textarea{.border-1(#e6e6e6); .fc(#999); .h(86px); .w(348px); resize:none; overflow: auto; outline: none; .left;}
            .placeholder{ .pa; left:4px; top:4px; .w(100%); .fc(#999); cursor:text;}
            .comment_notice{ .h(32px); .l(32px); .red; .none;
               .wrong_tip{ background: url("@{pic_url}/images/doota/@{doota_sale}") no-repeat; padding: 1px 12px; .bp(0,-115px);}
               b{.fn;}
            }
         }
      }
      .starItem{.h(26px); .l(25px);
         span , ul{.left;}
         ul{ list-style: none; padding: 0; margin: 0; .mr(20px); .mt(3px);}
         .yellow_star ,.gray_star{ .h(16px); .w(23px); .left; .cursor;}
         .yellow_star{ background:url("@{pic_url}/images/shop/star-c1.png") no-repeat 6px;}
         .gray_star{ background:url("@{pic_url}/images/shop/star-c2.png") no-repeat 6px;}
         .point{.red;}
         span em{.pr(5px); .red;}
      }
   }
   .submit_btn{ .pl(400px); margin: 20px auto 30px; .h(47px);
      .com_btn{ .w(155px); .h(47px); outline: none; border: 0; padding: 0; .bgred; .white; .f(18px); .l(46px); .c; .mr(20px); .cursor;}
      .check_box{ .w(13px); .h(13px); outline: none;}
      label{ .pl(6px); .cursor;}
   }
}

.tipbubble{ .pa; top: 30px; left: -9999px; .block; .w(229px); background-color:transparent; *border:1px solid #666;
   s{ .pa; top: -15px; *top: -15px; left: 110px; .block; .h(0); .w(0); .f(0); .l(0); border-color:transparent transparent #e6e6e6 transparent;border-style:dashed dashed solid dashed;border-width:8px;}
   i{.pa; top: -6px; *top: -6px; left: -7px; .block;.h(0);.w(0);.f(0);.l(0);border-color:transparent transparent #fff transparent;border-style:dashed dashed solid dashed;border-width:7px;}
   .tipcontent{.border-1(#e6e6e6);-moz-border-radius:3px;-webkit-border-radius:3px;.pa;.bgc(#fff);.w(100%);.h(auto);*top:-2px;*border-top:1px solid #666;*border-top:1px solid #666;*border-left:none;*border-right:none;*height:102px;padding:10px;
      span{.red;}
      p{ .mt(3px); .l(16px);}
   }
}
.placeholder{}

.shop_wrap{ padding: 20px;}
.order-info{ 
   em{ .gray;}
}
.shop-info{ .mt(32px);
   .shop-name{ .f(14px); .mb(10px);}
   .goqq { .i_block; .cursor; .h(23px); .l(23px); .f(12px); .c; .red; .w(86px);
      .qqico { background: url("@{pic_url}/images/doota/doota_im1.gif") no-repeat; .pt(2px); .pb(3px); .pl(18px); }
   }
}
.starItem{.h(26px); .l(25px);
   span , ul{.left;}
   ul{ list-style: none; padding: 0; margin: 0; .mr(20px); .mt(3px);}
   .yellow_star ,.gray_star{ .h(16px); .w(23px); .left; .cursor;}
   .yellow_star{ background:url("@{pic_url}/images/shop/star-c1.png") no-repeat 6px;}
   .gray_star{ background:url("@{pic_url}/images/shop/star-c2.png") no-repeat 6px;}
   .point{.red;}
   span em{.pr(5px); .red;}
}
.dash_border{ .w(95%); .border-tc(dashed); margin:2px auto;}
      
