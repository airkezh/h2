<%# head.html%>
<script src="http://res.wx.qq.com/open/js/jweixin-1.0.0.js"></script>
<div class="wrap">
	<div class="gameWrap">
		<div class="jjl_box" id="jjlBox">
			<div class="basket none">
			</div>
			<div class="none">
				<span class="scoreAlert">+1</span>
			</div>
			
			<div class="none">
				<img src="http://d01.res.meilishuo.net/pic/_o/de/05/b8e131512a1052b7c8d3d03f517e_94_94.cf.png" class="gift_init gift gift_bg">
				<img src="http://d01.res.meilishuo.net/pic/_o/74/20/98c84ac1c6b0cc994b84345e67d6_118_118.ch.png" class="gift_init gift gift_gz">
				<img src="http://d03.res.meilishuo.net/pic/_o/7c/7a/7ecd8613c25ee3c94e981bab5327_68_66.cf.png"  class="gift_init gift gift_ku">
				<img src="http://d01.res.meilishuo.net/pic/_o/de/05/b8e131512a1052b7c8d3d03f517e_94_94.cf.png" class="gift_init gift gift_tg">
				<img src="http://d01.res.meilishuo.net/pic/_o/74/20/98c84ac1c6b0cc994b84345e67d6_118_118.ch.png"  class="gift_init gift gift_xh">
				<img src="http://d01.res.meilishuo.net/pic/_o/de/05/b8e131512a1052b7c8d3d03f517e_94_94.cf.png" class="gift_init gift gift_tg">
			</div>
		</div>

		<div class="jjl_head">
			<p class="score">0</p>
			<div class="jjl_progress">
				<i></i>
			</div>
			<div class="jjl_time">
				<h6 class="jjl_time_tit">TIME</h6>
				<p class="jjl_time_con"><span>0</span>s</p>
			</div>
		</div>

		<div class="jjl_msgAlert">
			<img src="http://d05.res.meilishuo.net/pic/_o/9f/cc/081f35a407927cf21c9d3fac09bb_554_891.cf.png" />
		</div>
		<div class="begin_btn" >
			开始
		</div>
	</div>

	<div class="result none_f">
		<div class="mark">
			<div class="info">
				<span id="getScore">32</span>分
			</div>
			<h2 id="tips">你是练过葵花点穴手吗？ </h2>
		</div>
		<a href="javascript:;" class="again btn">不服再战</a>
		<a href="/activity/music/userInfo/" class="get btn">我要门票</a>
	</div>
</div>

<script>
	fml.vars.mlsHost = '<%=this.mlsHost%>';
</script>

<%! $_ENGINE_SELF.callFn("useModule")("<<<_use:
	fml.use('wap/page/music/game');
_use:", this._JSstack, this._JSmods); %>
<%# foot.html%>