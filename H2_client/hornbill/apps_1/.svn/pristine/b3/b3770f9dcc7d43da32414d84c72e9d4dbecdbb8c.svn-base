.shopBanner(){
	#slider{height:600px;
		a,span{.block;height:600px;.bgwhite;.w(100%);
			img{.block;.auto;}
		}
	}
	.slider{position:relative;height:600px;overflow:hidden;width:100%}
	.round { .w(948px); position:relative; .auto;
		.adType{ .tr; .pa; .w(948px); top: -275px; z-index:10;
			a {.w(50px);.h(50px);float:left;background:url("@{pic_url}/images/doota/slider_arrow.png");opacity:.2;filter:alpha(opacity=20);border-radius:5px;}
			a.next{background-position:-50px;float:right}
		}
	}
	.user_module{
		background:url("@{pic_url}/images/doota/module_gray_line.png") repeat-x;.w(948px);margin:auto;.c;.mb(20px);
		.name{.i_block;padding:15px 30px;.bgwhite;.f(14px);}
		.pics{
			.pic{.mt(10px);
				img{.w(948px);}
			}
			.goods{
				border:1px solid #e6e6e6;.h(385px);.left;.w(230px);.mr(6px);.mb(6px);
				img{.w(230px)}
				.title{margin:10px;.tl;.l(16px);.h(32px);overflow:hidden;}
			} 
			overflow:hidden;
			.w(948px);
		}
	}
}

.itemShopHeader(){
	.shop_contit { .h(48px); .l(48px);
		.shop_title{.w(948px); .auto;
			.shop_info_rate{ .left; .pr; z-index: 13;}
			.rate_info{.left; .ml(10px); .mr(10px);
				b{.fn;}
				.ml2_f{ .ml(2px);}
				.fRed{.fc(@red);}
				.fGreen{.fc(#b6cc00);}
			}
			.shop_name{ .left;.fc(#666); .f(16px); .fb; background: url("@{pic_url}/images/shop/shop_tle.png") no-repeat 10px 16px; .pl(40px);}
			.shop_arrows{.w(10px); .h(6px);.left; .mt(22px); background: url("@{pic_url}/images/header/@{mls_ico}") no-repeat -20px -70px;}
			.shop_arrows_active{background: url("@{pic_url}/images/header/@{mls_ico}") no-repeat -30px -70px;}	
			.shop_con{ .none; .pa; .bgc(#fff); border:1px solid #e6e6e6; .ml(-1px); padding: 50px 30px 20px; z-index: 12; .l(23px);
				span{.ml(-5px);}
				.shop_con_l{.pr(38px);
				 	span{ .fc(#f69); .fb;}
				}
				.shop_con_r{.pl(34px); border-left:1px dotted #e6e6e6;}
				.shop_con_m{.pr(25px); 
				 	.redlevbtn, .grenlevbtn, .yellevbtn{.white; .left; .h(19px); .l(19px); .ml(0); .pl(4px); .pr(4px); cursor: auto; .mr(10px); .common_button; .common_button > .common;}
				 	.redlevbtn{ .bgc(#ff4040);}
				 	.grenlevbtn{ .bgc(#b6cc00);}
				 	.yellevbtn{ .bgc(#f93);}
				}
				.goqq{.right;}
				.btnforDetail, .btnforShop{.w(88px); .border-1(@red); .c; .mt(18px); .h(26px); .l(26px);}
				.btnforShop{.bgc(@red); .ml(130px);}

				.icon-aptitude {  
				  .w(76px); 
				  .h(18px);
				  .i_block;
				  .vm; 
				  background-image : url('http://d01.res.meilishuo.net/pic/_o/7b/9e/92c6886fb374e8318ce9b4e58def_76_18.ch.gif'); 
				}
				.icon-money10000,.icon-money30000{
				  .w(58px);
				  .h(18px);
				  .i_block;
				  .vm;
				}
				.icon-money10000{
				  background-image : url('http://d04.res.meilishuo.net/pic/_o/bc/86/64f9aef7d6b095af5fcc4ff4adcb_58_18.cg.gif');
				}
				.icon-money30000{
				  background-image : url('http://d02.res.meilishuo.net/pic/_o/84/10/1b03a582c4d3e2cc624df0dc93d1_58_18.cf.gif');
				}
			}
		}
	}
}
.mt12{ .mt(12px);}
.refundGoodsInfo(){
	.bought_goods {.bgwhite;
		table {.w(100%);.border-1(@color:#e6e6e6);border-width:0 1px 1px;border-collapse:collapse;}
		th, td {padding:15px 0;border-bottom:1px solid #e6e6e6;}
	}
	.seller_info {
		span {.pl(20px);}
		.getserviceqq {padding:6px 42px;.ml(10px);
						background:url('@{pic_url}/images/serviceqq/service_qq.gif') no-repeat;
				}
	}
	.order_goods {.c;
		td {border-bottom:1px dotted #bfbfbf;}
		.blank_row {border-bottom-color:#FFF;}
		.tl {.tl;}
		.goods {.tl;}
		.pic {.left;.w(68px);.h(68px);.border-1(@color:#e6e6e6);.mr(10px);}
		.des {.left;.w(240px);}
		.title {.h(40px);.l(18px);.mb(15px);.hidden;}
		.spec {
			span {.gray;.mr(20px);}
			}
		.status,.operate {
			.border-1(@color:#e6e6e6);border-width:0 0 1px 1px;
			}
		}
	.order_time {.border-1(@color:#e6e6e6);border-width:0 1px 1px;padding:15px 0;
		span {padding:0 20px;}
	}
	.total {overflow:hidden;.border-1(@color:#e6e6e6);border-width:0 1px 1px;.l(20px);padding:15px 20px;
		p {.right;
			span {.mr(70px);}
			.last {.mr(0);}
			b {.red;.f(20px);}
		}
	}
}
