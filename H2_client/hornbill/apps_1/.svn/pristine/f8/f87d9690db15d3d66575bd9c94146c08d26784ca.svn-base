	.pp-footer-bd{width:985px;margin:0 auto;padding:20px 0}
	.pp-footer-services{border-bottom:1px solid #ddd;padding-bottom:30px;margin-left:auto;margin-right:auto;margin-bottom:15px;line-height:1.8}
	.pp-footer-services li{float:left;width:24.99%}
	.pp-footer-services dl{padding-left:52px}
	.pp-footer-services dt{height:25px;margin-bottom:10px;font-size:16px;font-family:"\5fae\8f6f\96c5\9ed1";position:relative;margin-left:-2px}
	.pp-footer-services u{position:absolute;width:100%;height:100%;display:block;background-repeat:no-repeat;top:0;left:-40px}
	.pp-footer-services .newbie u{background-image:url(http://static.paipaiimg.com/common/sprite/pp-footer.png?max_age=19830211&d=20120828170221);background-position:-64px -32px;width:124px}
	.pp-footer-services .safe u{background-image:url(http://static.paipaiimg.com/common/sprite/pp-footer.png?max_age=19830211&d=20120828170221);background-position:-156px 0;width:170px}
	.pp-footer-services .payment u{background-image:url(http://static.paipaiimg.com/common/sprite/pp-footer.png?max_age=19830211&d=20120828170221);background-position:-189px -26px;width:136px}
	.pp-footer-services .sale u{background-image:url(http://static.paipaiimg.com/common/sprite/pp-footer.png?max_age=19830211&d=20120828170221);background-position:-326px -26px;width:136px}
	.pp-footer-sitelink{padding-bottom:10px}
	.pp-footer-sitelink .paipai{float:left;margin-left:5px;_display:inline;color:#ddd}
	.pp-footer-sitelink .tencent,.pp-footer-copyright{float:right;width:560px;*width:570px}
	.pp-footer-sitelink .paipai a{margin:0 7px 0 5px}
	.pp-footer-sitelink a{margin:0 3px;*margin:0 2px}
	.pp-footer-logo{float:left;_display:inline;margin-left:10px;margin-top:10px}
	.pp-footer-logo a{float:left;margin-right:20px;display:block;overflow:hidden}
	.pp-footer-logo .paipai{width:88px;height:24px}
	.pp-footer-logo .qqshop{width:62px;height:22px}
	.pp-footer-logo .buyqq{width:74px;height:24px;margin-top:-2px}
	.pp-footer-logo u{width:100%;height:100%;display:block;position:static}
	.pp-footer-logo .paipai u{background-image:url(http://static.paipaiimg.com/common/sprite/pp-footer.png?max_age=19830211&d=20120828170221);background-position:-327px 0}
	.pp-footer-logo .qqshop u{background-image:url(http://static.paipaiimg.com/common/sprite/pp-footer.png?max_age=19830211&d=20120828170221);background-position:0 -37px}
	.pp-footer-logo .buyqq u{background-image:url(http://static.paipaiimg.com/common/sprite/pp-footer.png?max_age=19830211&d=20120828170221);background-position:-416px 0}
	.pp-footer-logo a u{cursor:pointer}
	.pp-footer-copyright p{line-height:1.8}
	.pp-footer-copyright a{color:#aaa}
	.pp-footer-police{display:block;float:left;margin-right:15px;margin-top:5px;width:63px;height:36px;overflow:hidden}
	.pp-footer-police u{width:100%;height:100%;display:block;position:static;cursor:pointer;background-image:url(http://static.paipaiimg.com/common/sprite/pp-footer.png?max_age=19830211&d=20120828170221);background-position:0 0}
	.pp-footer-trust-eshop{display:block;float:right;margin-top:5px;width:91px;height:31px;overflow:hidden}
	.pp-footer-trust-eshop u{width:100%;height:100%;display:block;position:static;cursor:pointer;background-image:url(http://static.paipaiimg.com/common/sprite/pp-footer.png?max_age=19830211&d=20120828170221);background-position:-64px 0}
	.pp-toolbar a,.pp-header a,.pp-footer a,a{color:#666}
	.pp-toolbar a:hover,.pp-header a:hover,.pp-footer a:hover,a:hover{color:#c00}
