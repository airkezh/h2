.footer{
	.mt(14px);
	a:link, a:visited{color:#3b3b3b;}
	a:hover{color:#bf2f28;text-decoration:underline;}
	.groups{*zoom:1}
	.groups:after{content:'\20';display:block;clear:both;height:0}
	.pp-footer{margin:0 auto;padding-bottom:40px;border-top:1px solid #ddd;color:#aaa;font:12px/1.5 arial;background-color:#fff;-webkit-text-size-adjust:none;*zoom:1;clear:both;text-align:left}
