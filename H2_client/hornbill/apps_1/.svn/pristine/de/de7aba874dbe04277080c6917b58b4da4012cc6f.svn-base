.vali_err{.red;text-indent: 12px;}
.createShop_top {.h(70px);background:#f7f6f6;.pl(20px);.border-1(@color:#e6e6e6);.hidden;
	.createShop_title {.f(24px);.red;.left;.mt(23px);.fn;}
}
.boxCommon(){.mb(10px);.bgwhite;.border-1(#e6e6e6);padding:0 20px 10px;}
.tle(){.f(12px);.fn;.h(54px);.l(54px);border-bottom:1px dotted #bfbfbf;.mb(10px);
