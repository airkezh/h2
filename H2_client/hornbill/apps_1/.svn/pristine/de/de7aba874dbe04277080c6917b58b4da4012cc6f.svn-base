		li {.left;.w(130px);.c;}
		.icon2, {.w(60px);.h(60px);.mb(10px);.block;.auto;background:url('@{pic_url}/images/doota/sale131022.png') no-repeat;.bp(-71px,0); }
		.icon1,.icon3,.icon4{.w(60px);.h(60px);.mb(10px);.block;.auto;background:url('@{pic_url}/images/store/icon_1117.png') no-repeat; }
		.icon1 {.bp(0,0);}
		.icon3 {.bp(0, -130px);}
		.icon4 {.bp(0, -257px);}
		}
