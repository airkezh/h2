}
.createShop_input() {.h(23px);padding:0 5px;.border-1(@color:#e6e6e6);color:#666;}
.shop_contact {.boxCommon();.mt(10px);
	h6{.tle();}
	table{.l(45px);
		th{.w(70px);white-space: nowrap; font-weight: normal;}
		.radio_wrapper{.mr(10px);}
		input[type=text]{.createShop_input();}
	}
}
.strength();
.strength{.boxCommon();
	h6{.tle();}
}
.warning{.warning();}
.identityCard,.company_credentials {.boxCommon();
	h6{.tle();}
	th {.w(140px);}
	.id_num,.id_name {.h(42px);
		input {.w(150px);.createShop_input;}
	}
	.id_valid {.h(42px);
		.year {.w(88px);.createShop_input;}
		.month,.day {.w(38px);.createShop_input;}
		span {padding:0 10px;}
	}
	.id_frontPho, .id_backPho,.id_headPho,.id_halfPho,.company_license {
		th,td {padding:10px 0;}
		img {.w(98px);.h(98px);.border-1(@color:#e6e6e6);.block;.mb(10px);}
		.upload_pho {.left;.w(100px);}
		.btn_upload {.redBtn;.w(100px);padding:0;.c;.cursor;}
		.example_pho {.w(480px);.left;.ml(30px);}
			span,img {.left;}
			span {.pr(20px);}
			.exp {.left;.ml(30px);.l(20px);}
		}
	.company_name {.h(42px);
			input {.createShop_input;.w(280px);}
		}
}
/*资质认证*/
.approve();
