@import "block";
.about_us() {
  .auto; .mt(14px); .w(948px);
  .content { .bgwhite; .box-shadow; .left; .mr(5px); height:auto !important; .h(442px);min-height:506px; .p20; .w(668px);
    a { .red;}
    ol { .f(14px); .l(22px); .pl(30px); .pr(30px);
      li { list-style:decimal; }
    }
    .media { .f(14px); .l(22px);
      li{
        a{ color:#666;
          &:hover{color:#ff6699;}
        }
        span{.right;}
      }
    }
    table { .f(14px); .l(24px); }
  }
  h2 { .f(24px); }
  p { .f(14px); .l(22px); .mt(10px);}
  .intro_CEO { .left; .w(380px);}
  .brand_group img { .border-1; .mr(14px); .mb(7px); .mt(7px); }
  .jobs {.f(14px); .l(20px); .mt(10px);
    table { .w(100%); }
    th { .tl;}
  }
  .desc { .f(14px); .l(20px); .mt(10px);
    span { .block;.mb(5px);.mt(5px); }
  }
  .app { .left; .mt(15px); .w(320px);
    img { .left; .mr(10px);}
    div { .f(14px);.l(20px); .mt(5px);
      span { .f(12px); .p05; .red; }
    }
  }
  .cpc_adv { .left; .f(14px); .l(22px); .ml(14px); .inline; .mt(10px); .w(320px); .h(160px); }
  .cost table { border-collapse:collapse; .w(600px);
    td { .border-1c; .p05; .h(24px); .l(24px); }
  }
  .kw_adv { .left; .w(160px); .mt(14px);.pl(30px); .pr(20px); }
}

.mb10_f {
  margin-bottom: 10px;
}
.adv1, .adv2, .adv3, .adv4, .adv5, .adv6, .process { background:url("@{pic_url}/images/about/cpc_20140715_01.png"); .mb(10px); .w(265px); .h(87px); }
.adv1 { background-position:-10px 0; }
.adv2 { background-position:-364px 0; }
.adv3 { background-position:-10px -93px; }
.adv4 { background-position:-364px -93px; }
.adv5 { background-position:-10px -203px; }
.adv6 { background-position:-364px -203px; }
.process { .mt(40px); .mb(30px); background-position:0 -300px; .w(640px); .h(92px);}
.contact { .ml(30px); }
.k_icon1, .k_icon2, .k_icon3, .buy_process { background:url("@{pic_url}/images/about/kw_1.png"); .w(100px); .h(100px); .auto; }
.k_icon1 { background-position:0 0; }
.k_icon2 { background-position:-212px -0px; }
.k_icon3 { background-position:right 0; }
.buy_process { background-position: 0 -120px; .w(500px); .h(290px); }
.aboutus {.pr;.about_us;}

.korea-shop {
  .list-icons {
    .item {
      min-height: 60px;
    }

    .wrap {
      float: left;
      height: 60px;
    }
  }
  .icon {
    display: block;
    position: relative;
    top: 10px;
    width: 45px;
    height: 45px;
    margin-right: 15px;
    background-image: url("@{pic_url}/images/doota/korean-shop-20140815.png");
  }

  .icon-1 {
    background-position: 0 -62px;
  }

  .icon-2 {
    background-position: -48px -62px;
  }

  .icon-3 {
    background-position: -96px -62px;
  }

  .icon-4 {
    background-position: -144px -62px;
  }

  .icon-5 {
    background-position: -192px -62px;
  }

  .title {
    font-weight: bold;
  }

  .list li {
    list-style: initial;
  }

  #sheet {
    margin-top: 40px;
    padding-top: 15px;
    border-top: 1px dashed;
  }

  .proclaim {
    margin-top: 20px;
    padding-bottom: 10px;
    line-height: inherit;
    text-align: center;
  }

  thead, tr {
    text-align: center;
  }

  .name {
    padding-left: 50px;
    text-align: left;
  }

  .first .name {
    padding-left: 0;
    text-align: center;
  }
}

.list-show i, .show-pos, .cps-process {
  background-image: url("@{pic_url}/images/about/cpc_20140715_03.png");
}

.list-show {
  margin-top: 50px;

  li {
    font-size: 14px;
    line-height: 70px;
    margin-bottom: 35px;
    overflow: hidden;
  }

  i {
    float: left;
    width: 70px;
    height: 70px;
    margin-right: 15px;
  }

  .icon-2 {
    background-position: -85px 0;
  }

  .icon-3 {
    background-position: -164px 0;
  }
}

.show-pos {
  width: 544px;
  height: 310px;
  background-position: 0 -77px;
}

.cps-process {
  width: 452px;
  height: 360px;
  margin: 60px 0 80px 60px;
  background-position: 0 -398px;
}

@cptImage: url("@{pic_url}/images/about/cpt_20140722_01.png");

/* CPT */
.cpt {
  .ico {
    background-image: @cptImage;
  }

  .ico-trend, .ico-time, .ico-fans {
    float: left;
    width: 70px;
    height: 70px;
    margin-left: 10px;
    margin-right: 30px;
  }

  .ico-crown, .ico-shop, .ico-btn {
    display: inline-block;
    width: 32px;
    height: 32px;
    margin: 0 5px;
    vertical-align: middle;
  }

  .ico-time {
    background-position: -70px 0;
  }

  .ico-fans {
    background-position: -140px 0;
  }

  .ico-crown {
    background-position: -210px 0;
  }

  .ico-shop {
    background-position: -257px 0;
  }

  .ico-btn {
    width: 100px;
    background-position: -300px -2px;
  }

  .pos {
    margin-left: 16px;
    margin-top: 10px;

    h5 {
      font-size: 14px;
    }

    li {
      margin-top: 10px;
      list-style: initial;
    }
  }
}

.cpt-goodness {
  li {
    height: 80px;
    padding: 25px 0;
    overflow: hidden;
    font-size: 14px;
  }

  .middle {
    border-width: 1px 0;
    border-style: dotted;
  }

  .last {
    h4 {
      margin-bottom: 10px;
    }
  }

  .split {
    float: left;
    height: 30px;
    width: 1px;
    margin: 20px 30px 0 0;
    background-color: #666;
  }

  h4 {
    margin-bottom: 15px;
    color: #f69;
    font-weight: bold;
  }

  .intro {
    margin-top: 10px;
  }
}

.cpt-process {
  width: 640px;
  height: 92px;
  margin-top: 20px;
  margin-bottom: 50px;
  background-image: @cptImage;
  background-position: 0 -86px;
}


