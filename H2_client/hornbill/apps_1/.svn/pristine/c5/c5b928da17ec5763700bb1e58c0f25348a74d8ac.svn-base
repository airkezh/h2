//join meilishuo style
.empty{
  text-align: center;
  height: 330px;
  line-height: 330px;
  font-size: 18px;
  font-weight: bold;
}
.fweight{
  font-weight:bold;
}
.header{
  h3{
    float:left;
  }
}
.xiao{
  width:100%;
  height:140px;
  img{
    display: block;
    width:100%;
    height:56px;
    position: relative;
    top:15px;
  }
  a{
    position: relative;
    top:-20px;
    left:76%;
    text-decoration: underline;
    color: #666 !important;
  }
  a:hover{
    opacity:0.8;
  }
}

.nav{
  width:100%;
  overflow: hidden;
  color:#666;
  font-size: 14px;
  font-weight: 550;
  .nav_head{
    float: left;
    text-align: right;
    line-height: 50px;
    color:#888;
  }
  .nav_content{
    overflow: hidden;
    width:568px;
    margin: 9px 0 9px 0;
    color:#ff6599;
    .nav_list{
      display: block;
      overflow: hidden;
      li{
        cursor:pointer;
        float: left;
        height:25px;
        margin-left:20px;
        line-height: 25px;
        padding-left: 5px;
        padding-right: 5px;
        text-align: center;
        border-radius: 2px;
        -webkit-border-radius:2px;
        -moz-border-radius:2px;
        margin:4px;
      }
      li:hover{
        background:#ffdfea;
      }
    }
  }
  .nav_content:after{
    display: block;
    clear: both;
  }
}
.aclr{
  background:#ffdfea;
}
.job_lists{
  .job_header{
    width:668px;
    height:40px;
    font-size: 14px;
    font-weight: 500;
    background: #ff6599;
    color: #fff;
    ul{
      height:100%;
      li{
        float: left;
        height: 40px;
        line-height: 40px;
      }
      .job_name{
        padding:0 10px;
        width:230px;
      }
      .job_type,.job_place,.job_num{
        padding:0 10px;
        width:64px;
      }
      .job_updaTime{
        width:100px;
        text-align: center;
      }
      .job_tu{
        padding:0 10px;
        width:46px;
      }
    }
  }
  .job_content{
    width:668px;
    height:100%;
    font-size: 14px;
    font-weight: 500;
    background: #fff;
    .even{
      background: #fff1f6;
    }
    .list_ul{
      height:40px;
      cursor: pointer;
      li{
        float: left;
        height: 40px;
        line-height: 40px;
      }
      .job_name{
        padding:0 10px;
        width:230px;
        white-space: nowrap;
        overflow: hidden;
        text-overflow:ellipsis;
      }
      .job_type,.job_place,.job_num{
        padding:0 10px;
        width:64px;
        white-space: nowrap;
        overflow: hidden;
        text-overflow:ellipsis;
      }
      .job_num{
        text-align: center;
      }
      .job_updaTime{
        width:100px;
        text-align: center;
        white-space: nowrap;
        overflow: hidden;
        text-overflow:ellipsis;
      }
      .job_tu{
        padding:0 10px;
        width:46px;
        img{
          cursor: pointer;
          width:22px;
          height:22px;
          display: block;
          position: relative;
          top:50%;
          margin-top:-25%;
          left:50%;
          margin-left: -25%;
        }
      }
    }
  }
}
.job_desc{
  padding:10px 20px;
  display: none;
  p{
    span{
      color:#777 !important;
    }   
  }
  .email_header{
    width:100%;
    height:21px;
    font-size: 14px;
    font-weight: bold;
    color:#333;
    margin-top: 10px;
  }
  .email_share{
    width:250px;
    height:33px;
    position: relative;
    top:0px;
    left:375px;
    .email{
      font-size: 14px;
      font-weight: bold;
      width:200px;
      float: left;
      color:#ff6599;
    }
    .apply{
      background: #f69;
      width:100px;
      height:33px;
      float: right;
      text-align: center;
      line-height: 33px;
      a{
        color:#fff;
        font-weight: bold;
      }
    }
    .share{
      width:123px;
      height:33px;
      line-height: 33px;
      float: left;
      color:#ff6599;
      span{
        font-size: 14px;
        font-weight: bold;
        display: block;
        float: left;
      }
      ul{
        height:17px;
        width:70px;
