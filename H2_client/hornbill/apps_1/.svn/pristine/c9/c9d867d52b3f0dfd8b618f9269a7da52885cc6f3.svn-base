<%#head.html%>
<%
if(this.b_banner && this.b_banner.mob_tuan_big_banner&&this.b_banner.mob_tuan_big_banner.length){
	var bigbanners=this.b_banner.mob_tuan_big_banner;%>
<div id="topbanner">
	<a href="<%= bigbanners[0].link%>">
		<img src="<%= bigbanners[0].show_pic%>" alt="<%= bigbanners[0].tab_name%>" />
	</a>
</div>
<%}%>
<%if(this.s_banner&& this.s_banner.mob_tuan_pin_banner_right&&this.s_banner.mob_tuan_pin_banner_right.length){
	var smallbanners=this.s_banner.mob_tuan_pin_banner_right,
	pinbanner=this.m_banner&& this.m_banner.mob_tuan_pin_banner&& this.m_banner.mob_tuan_pin_banner?this.m_banner.mob_tuan_pin_banner:null;%>
<div class="bannerbox">
	<%if(pinbanner){%>
		<figure><a href="<%= pinbanner[0].link%>"><img src="<%= pinbanner[0].show_pic%>" alt="<%= pinbanner[0].tab_name%>" /></a></figure>
	<%}%>
	<%smallbanners.forEach(function(data){%>
       <figure><a href="<%= data.link%>"><img src="<%= data.show_pic%>" alt="<%= data.tab_name%>" /></a></figure>
	<%})%>
</div>
<%}%>
<section class="container">
	<%if(this.tuanhome){%>
	<hgroup>
		<h3><img src="<%= this.PICTURE_URL%>images/wap/tuan/t_free.png" alt="" /></h3>
		<span>每日10:00上新</span>
		<div class="clear_f"></div>
	</hgroup>
	<%}%>
	<div class="content">
		<div class="goods_wall">
		</div>
		<div class="pullUp" status="loading"></div>
	</div>
</section>
