<%#head.html%>
<style type="text/css">
	.dailytpl { margin: 0 auto; width: 948px; overflow: hidden; }
	.pic_box { margin-top: 14px; }
	.pic_box img { width: 948px; }
</style>
<div class="dailytpl">
	<% if(this.pic){ %>
		<div class="pic_box"><img src="<%= this.pic.top_pic%>" /></div>
		<div class="pic_box"><img src="<%= this.pic.bot_pic%>" /></div>
	<% } %>
</div>

<%#foot.html%>
