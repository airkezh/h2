.item-selected {
  background: #fff2f7;
  *background-color: #fff2f7;

}

.auto-cur {
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  position: absolute;
  bottom: 140px;
  z-index: 9990;
  left: 30px;
  width: 16px;
  height: 11px;
  display: none;
}

.auto-menu {
  width: 560px;
  border: 1px solid #dedede;
  background-color: white;
  position: absolute;
  bottom: 150px;
  padding: 11px 0px;
  left: 20px;
  display: none;
  left: 10px;
  & li {
    height: 30px;
    line-height: 30px;
    list-style: none;
    padding-left: 20px;
    font-size: 12px;
    overflow: hidden;
    word-break: break-all;
    word-wrap: break-word;
    text-overflow: ellipsis;
  }

}