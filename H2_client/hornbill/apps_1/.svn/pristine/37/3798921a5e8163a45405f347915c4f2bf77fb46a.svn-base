@import "../block.less";
body{.bgc(#fff)}
.main{
  position: relative;
  width: 100%;
  height: auto;
  z-index: 1;
  margin: 0 auto;
}

.litetooltip-hotspot-wrapper {
    position: relative;
    width: 100%;
    height: auto;
    z-index: 1;
    margin:0 auto;
}

.litetooltip-hotspot-container {
    position: relative;
    height: 0;
}

.litetooltip-hotspot-container .hotspot {
  position: absolute;
  z-index: 3;
  cursor: pointer;
}

.data-container {width: 100%;height:100%;
  a{display: block;width: 100%;height: 100%}
}

img {
    display: block;
    width: 100%;
}


.box{.pa;left:30%;top:65%;.w(62.5%);.h(35%);}
