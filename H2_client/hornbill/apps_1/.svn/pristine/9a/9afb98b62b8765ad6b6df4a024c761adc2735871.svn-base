/*社区宝贝样式*/
.goodsWrap{
	background: #F2F0F0;.mt(20px);.l(20px);width: 352px;
	.imgWrap{
		.hidden;.block;height: 400px;.border-1(#f6f4f4);.bgc(#fff);
	}
	.titleWrap{
		.bgc(#F6F4F4);.pr;.hidden;
	}
	.goodstitle{.f(14px);.block;padding: 20px 140px 20px 20px;.left;text-align: justify;word-break: break-all;word-wrap: break-word;.red;}
	span{.f(16px);.fb;.pa;right: 20px;top: 20px;}
}
/*回复框部分*/
.reply_block{
	.pl(88px);.pb(20px);.pr;.left;.w(618px);
	.separation{.ml(-88px);}
}
.replay_triangle{.w(13px);.h(2px);.pa;right: 48px;top: -9px;.hidden;padding: 4px;}
.inner{
	background: #fff;.h(10px);.w(10px);border:1px solid #e5e5e5;-moz-transform: rotate(45deg);-o-transform: rotate(45deg);-webkit-transform: rotate(45deg);transform: rotate(45deg);
	/* IE6 and 7 */
	filter: "progid:DXImageTransform.Microsoft.Matrix( M11=0.7071067811865474, M12=-0.7071067811865477, M21=0.7071067811865477, M22=0.7071067811865474, SizingMethod='auto expand')";
/* IE8+ - must be on one line, unfortunately */
	-ms-filter: "progid:DXImageTransform.Microsoft.Matrix(M11=0.7071067811865474, M12=-0.7071067811865477, M21=0.7071067811865477, M22=0.7071067811865474, SizingMethod='auto expand')";
}
.reply_sg { .h(100px); .w(540px);.f(12px);.gray; padding: 13px; .border-1c; .mt(20px); }
.reply_bar { .h(22px); .f(14px); .mr(50px);
	.share_smileys, .reply_img, .reply_goods{.left;}
	.img_wraper { z-index: 99; }
	.holder { background: #FFF; .border-1; padding:8px; .box-shadow; .pr; 
		.tedit_icon { .h(20px); 
			.close_z { .mt(0); }
		}
	}
	.goods_holder { .w(310px);
		.goodspic, .goodspic img { .h(150px); .w(150px); .hidden;}
		.goodspic_r { .right; .w(150px); 
			h3 { .l(20px); .f(14px); .fb;}
			.goods_price { .mt(14px); .f(22px); }
		}
	}
	.reply_goods { .ml(20px); }
	.upload_pic_holder { .w(150px); }
}
/* 图片上传 */
