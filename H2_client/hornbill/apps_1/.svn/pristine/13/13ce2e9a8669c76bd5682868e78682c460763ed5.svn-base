<%# head_simple.html%>
<div class="show-main show-main-1">
	<img src="http://img.meilishuo.net/css/images/wap/activity/draw/back-1.png" />
	<div class="show-banner show-banner-1">
		<img src="http://img.meilishuo.net/css/images/wap/activity/draw/star.png" />
	</div>
</div>
<div class="show-detail show-detail-1">
	<p>星光ID：<input id="star" value="<%= this.url.data.id || ' '%>" type="text" readonly="readonly" /></p>
	<div>
		<input class="back" id="name" type="text" placeholder="姓&nbsp;&nbsp;名："/>
	</div>
	<div>
		<input class="back" id="tel" type="text" placeholder="手机号：" />
	</div>
	<a id="btn" href="<%= this.url.data.url %>">
		<img src="http://img.meilishuo.net/css/images/wap/activity/draw/btn-1.png" />
	</a>
</div>
<%! $_ENGINE_SELF.callFn("useModule")("<<<_use:
	fml.use('wap/page/activity/draw');
_use:", this._JSstack , this._JSmods ); %>
<%# foot_simple.html%>