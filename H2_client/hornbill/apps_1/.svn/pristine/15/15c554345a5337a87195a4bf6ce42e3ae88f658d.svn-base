<%#shop/settled/settle_head.html%>
<div class="container">
	<h3 class="page_title">公告列表</h3>
	<ul class="post_list">
		<% this.post_list && this.post_list.list && this.post_list.list.forEach(function(item){ %>
		<li>
			<a href="/shop/settled/post?id=<%=item.id %>"><%=item.title %><span class="date"><%=item.date %></span></a>
		</li>
		<% });%>
	</ul>
</div>
<%#foot.html%>