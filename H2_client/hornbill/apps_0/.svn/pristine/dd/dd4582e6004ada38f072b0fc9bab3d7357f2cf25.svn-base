<%#shop/settled/settle_head.html%>
<div class="container">
	<div class="top_content"><%=this.odds.info.top %></div>
	<div class="odds_list">
		<% this.odds.info && this.odds.info.list && this.odds.info.list.forEach(function(item,index){ %>
		<div class="c_item">
			<h4>
				<img src="<%=item.icon %>">
				<%=item.title %>
			</h4>
			<p><%=item.desc %></p>
		</div>
		<%});%>
		<div class="clear_f"></div>
	</div>
</div>
<%#foot.html%>