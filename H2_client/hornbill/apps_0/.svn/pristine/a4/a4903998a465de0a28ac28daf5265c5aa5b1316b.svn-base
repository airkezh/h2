				background-size:53px auto;
			}

			.level2{
				.pl(108*@px);
				background:url("http://d03.res.meilishuo.net/pic/_o/24/60/689ee7bdc0bae2254c017cc40d83_106_34.ch.png") no-repeat;
				background-size:53px auto;
			}

			.level3{
				.pl(108*@px);
				background:url("http://d02.res.meilishuo.net/pic/_o/46/cd/6e703ef3cea736f83d99db23c016_106_34.cf.png") no-repeat;
				background-size:53px auto;
			}

			.level4{
				.pl(108*@px);
				background:url("http://d03.res.meilishuo.net/pic/_o/55/10/220858554cf771f8eb7df6542ff9_106_34.cg.png") no-repeat;
				background-size:53px auto;
			}


			.name{
				.l(1.4);
				.f(1.6rem);
				.fc(#fff);
			}

			.beauty{
				.mt(6%);
				.f(1.2rem);
				.fc(#fee503);

				.desc{
					.ml(10px);
					.fc(#fff);
				}
			}

			.progress_wrap{
				.c;
				.mt(4%);

				.level_text{
					.left;
					.i_block;
					.w(8%);
					.f(1.3rem);
					.fc(#fff);
				}

				.text_left{
					.tl;
				}

				.text_right{
					.tr;
				}

				.outer_progress_bar{
					.left;
					.i_block;
					.w(84%);
					.h(13px);
					.bgc(#755f76);
					.border-r(6px);
					-moz-box-shadow:1px 0 5px rgba(0, 0, 0, .1) inset;            
                    -webkit-box-shadow:1px 0 5px rgba(0, 0, 0, .1) inset;         
                    box-shadow:1px 0 5px rgba(0, 0, 0, .1) inset;
				}

				.inner_progress_bar{
					.w(80%);
					.h(100%);
					.f(.9rem);
					.fc(#fff);
					.border-r(6px);
					/*border:1px solid rgba(66,61,66,.2);*/
					/* Firefox 3.6+ */
 					background:-moz-linear-gradient(left, #fe6054, #fe6054 25%, #ffe347);
 					/* Safari 4-5, Chrome 1-9 */
 					background:-webkit-gradient(linear, left top, right top, from(#ace), color-stop(0.25, #fe6054), to(#ffe347));
 					/* Safari 5.1+, Chrome 10+ */
 					background:-webkit-linear-gradient(left, #fe6054, #fe6054 25%, #ffe347);
 					/* Opera 11.10+ */
 					background:-o-linear-gradient(left, #fe6054, #fe6054 25%, #ffe347);
				}

				.percent{
					padding:2px 0;
				}
			}
		}
	}

	.beauty_tips{
		.pa;
		.l(1.2);
		.f(1.2rem);
		top:14%;
		right:30*@w;

		.mark{
			.pl(24*@px);
			background:url("http://d05.res.meilishuo.net/pic/_o/c9/d1/7a8e0ab8763a9c0cf856ca57a44d_22_22.cg.png") no-repeat;
			background-size:12px auto;
		}
	}
}

