/* 返回顶部 */
.gotop_wrap{
	.pf;
	.none;
	.w(82*@w);
	.pt(82*@w);
	z-index:10;
	right:10px;
	bottom:40px;

	.gotop{
		.block;
		.w(100%);
	}

	img{
		.w(100%);
	}
}

/* 弹层 */
.layer_shade{
	.pf;
	top:0;
	left:0;
	.w(100%);
	.h(100%);
	z-index:999;
	background:rgba(0,0,0,.85);
}

.layer_main{
	.pf;
	top:10%;
	left:0;
	.w(100%);
	.h(auto);
	z-index:999;

	.m{
		.pr;

		img{
			.w(500*@w);
			margin:0 auto;
		}

		.content{
			overflow:auto;
			max-height:360px;
			box-sizing:border-box;
		}

		.layer_top{
			.pa;
			top:-3%;
			left:70*@w;
			.w(500*@w);
			.pt(38*@w);
			background:url("http://d03.res.meilishuo.net/pic/_o/ae/29/5cc8367e1a1a8e6a37be74768aa0_500_38.ch.png") no-repeat;
			background-size:100%;
		}

		.know_btn{
			.pa;
			.w(500*@w);
			.h(15%);
			left:11%;
			bottom:0;
		}

		.close_btn{
			.pa;
			bottom:-8%;
			left:70*@w;
			.w(500*@w);
			.pt(88*@w);
			background:url("http://d05.res.meilishuo.net/pic/_o/ba/34/bd8578644f2d79436af1fe83809b_500_88.cg.png") no-repeat;
			background-size:100%;
		}
	}
}

/* 组图 */
.swipe{
	.c;
	.pr;
	.hidden;
}

.swipe_wrap{
	.pr;
	.hidden;
	-webkit-backface-visibility:hidden;
	-moz-backface-visibility:hidden;
	-ms-backface-visibility:hidden;
	-o-backface-visibility:hidden;
	backface-visibility:hidden;
	/* Fix of Webkit flickering */
	z-index:1;
}

.swipe_wrap > div{
	.pr;
	.left;
	.w(100%);
}

.swipe_pic{
	.pr;
	.hidden;
	.w(100%);
	.bgc(#fff);
}

.swipe_pic img{
	.w(100%);
}

.swipe_num{
	.pa;
	left:0;
	.w(100%);
	bottom:8px;
	z-index:9;

	li{
		.vm;
		.w(5px);
		.h(5px);
		.mr(2px);
		.i_block;
		.bgc(#fff);
		float:none;
		.border-r(50%);
	}

	li.active{
		border:1px solid #fff;
		background-color:transparent;
	}
}

