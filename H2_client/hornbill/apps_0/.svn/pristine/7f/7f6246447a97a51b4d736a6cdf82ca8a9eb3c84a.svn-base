/*放大缩小*/
@keyframes move-big{
    0%{
        -webkit-transform: scale(1);
        transform: scale(1);
    }
    50%{
        -webkit-transform: scale(1.3);
        transform: scale(1.3);
    }
    100%{
        -webkit-transform: scale(1);
        transform: scale(1);
    }
}
@-webkit-keyframes move-big{
    0%{
        -webkit-transform: scale(1);
        transform: scale(1);
    }
    50%{
        -webkit-transform: scale(1.3);
        transform: scale(1.3);
    }
    100%{
        -webkit-transform: scale(1);
        transform: scale(1);
    }
}
/*左右摇摆*/
@keyframes shaked{
    0%{
        -webkit-transform: rotate(-20deg);
        transform: rotate(-20deg);
    }
    100%{
        -webkit-transform: rotate(20deg);
        transform: rotate(20deg);
    }
}
@-webkit-keyframes shaked{
    0%{
        -webkit-transform: rotate(-20deg);
        transform: rotate(-20deg);
    }
    100%{
        -webkit-transform: rotate(20deg);
        transform: rotate(20deg);
    }
}
