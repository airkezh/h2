@import "storeAtom.less";
.container{.w(960px);.bgwhite;.auto;border-top: 2px solid @red;.mb(20px);
	.order_detail{padding: 0 20px;.l(56px);.border-1(#e6e6e6);.mt(-1px);
		span{.mr(20px);}
	}
	.bank_list{.border-1(#e6e6e6);.mt(-1px);padding: 0 20px;
		.desc{.l(56px);}
		.b_list{
			li{.pb(20px);.hidden();.left;.w(220px);.mr(9px);
				.b_radio,.b_img{.left;}
				.b_radio{.mt(15px);}
				.b_img{.w(130px);.h(40px);.border-1(#e6e6e6);}
			}
		}
	}
	.btn_wrapper{.mt(10px);.border-1(@red);.l(48px);
		.cancel_pay{.left;.ml(20px);.red;}
		.pay_btn{.right;.h(100%);padding: 0 35px;.f(20px);.white;.bgred;.cursor;}
	}
}