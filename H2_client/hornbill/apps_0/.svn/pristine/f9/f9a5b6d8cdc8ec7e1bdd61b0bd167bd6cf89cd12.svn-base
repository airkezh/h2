.foot_login(){
	.foot_login{
		.mt(20*@px);
		.common-box;
		.pt(20*@px);
		.mb(120*@px);
		ul{
			.mb(20*@px);.clearfix;
			li{
				.w(50%);.left;
				a{
					.bgred;.c;.border-r(@radius:3px);.l(2);.ml(10*@px);.mr(10*@px);
					.white;.block;.f(1.8rem);
				}
			}
		}
		p{
			.f(1.4rem);.l(1.4);
			.mb(20*@px);
			a, span{.red;.mr(70*@px);}
			a{.tunder;}
		
		}
		
	}	
	
}
