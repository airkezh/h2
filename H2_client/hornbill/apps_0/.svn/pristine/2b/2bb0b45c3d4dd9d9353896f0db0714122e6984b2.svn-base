.add_action{animation: jump 1s linear infinite;
			-webkit-animation: jump 1s linear infinite;
			-moz-animation: jump 1s linear infinite;
			-o-animation: jump 1s linear infinite;
			-ms-animation: jump 1s linear infinite;
	@-webkit-keyframes jump{
		0%{.mt(0px);transform: scale(1,1);-webkit-transform: scale(1,1);-moz-transform: scale(1,1);-o-transform: scale(1,1);-ms-transform: scale(1,1);}
		10% {.mt(0px);transform: scale(1,1);-webkit-transform: scale(1,1);-moz-transform: scale(1,1);-o-transform: scale(1,1);-ms-transform: scale(1,1);}
		50% {.mt(-20px);transform: scale(1,1);-webkit-transform: scale(1,1);-moz-transform: scale(1,1);-o-transform: scale(1,1);-ms-transform: scale(1,1);}
		100% {.mt(0px);transform: scale(1,1);-webkit-transform: scale(1,1);-moz-transform: scale(1,1);-o-transform: scale(1,1);-ms-transform: scale(1,1);}
	}
	@-moz-keyframes jump{
		0%{.mt(0px);transform: scale(1,1);-webkit-transform: scale(1,1);-moz-transform: scale(1,1);-o-transform: scale(1,1);-ms-transform: scale(1,1);}
		10% {.mt(0px);transform: scale(1,1);-webkit-transform: scale(1,1);-moz-transform: scale(1,1);-o-transform: scale(1,1);-ms-transform: scale(1,1);}
		50% {.mt(-20px);transform: scale(1,1);-webkit-transform: scale(1,1);-moz-transform: scale(1,1);-o-transform: scale(1,1);-ms-transform: scale(1,1);}
		100% {.mt(0px);transform: scale(1,1);-webkit-transform: scale(1,1);-moz-transform: scale(1,1);-o-transform: scale(1,1);-ms-transform: scale(1,1);}
	}
	@-o-keyframes jump{
		0%{.mt(0px);transform: scale(1,1);-webkit-transform: scale(1,1);-moz-transform: scale(1,1);-o-transform: scale(1,1);-ms-transform: scale(1,1);}
		10% {.mt(0px);transform: scale(1,1);-webkit-transform: scale(1,1);-moz-transform: scale(1,1);-o-transform: scale(1,1);-ms-transform: scale(1,1);}
		50% {.mt(-20px);transform: scale(1,1);-webkit-transform: scale(1,1);-moz-transform: scale(1,1);-o-transform: scale(1,1);-ms-transform: scale(1,1);}
		100% {.mt(0px);transform: scale(1,1);-webkit-transform: scale(1,1);-moz-transform: scale(1,1);-o-transform: scale(1,1);-ms-transform: scale(1,1);}
	}
