#com-topbar{
	background-color: #fff;
	border-bottom: 1px solid #eee;
	height: 30px;
	line-height: 30px;
}
#com-topbar .inner{
	width: 1200px;
	margin: 0 auto;
	text-align: right;
}
#com-topbar li{
	display: inline-block;
	*display: inline;
	*zoom:1;
	text-align: center;
}
#com-topbar a{
	border-right: 1px dotted #ccc;
	padding: 0 15px;
	display: inline-block;
	height: 20px;
	line-height: 20px;
}
#com-topbar .last{
	border-right: none;
}
#com-topbar a:hover{
	color: #f69;
}
#com-topbar .nick{
	padding-right: 0;
	border-right: none;
}
#com-topbar .member{
	padding-left: 0;
}
#com-topbar .face{
	vertical-align: middle;
	height: 19px;
	width: 19px;
	border-radius: 9px;
	padding-right: 2px;
}
#com-topbar .level{
	height: 20px;
    width: 60px;
    display: inline-block;
    margin-right: 2px;
    vertical-align: -5px;
}
#com-topbar .level-0{background:url("http://d01.res.meilishuo.net/pic/_o/26/77/5e9439fc636d94ae034c04017700_60_20.ch.png") no-repeat;}
#com-topbar .level-1{background:url("http://d01.res.meilishuo.net/pic/_o/22/70/50297dfb1cfe7d51a8b673d4a087_60_20.cf.png") no-repeat;}
#com-topbar .level-2{background:url("http://d03.res.meilishuo.net/pic/_o/27/ad/f05449caeb1caea3ce89b7fad0db_60_20.cf.png") no-repeat;}
#com-topbar .level-3{background:url("http://d04.res.meilishuo.net/pic/_o/c7/28/4487c05ec65ab856bd0c555cd510_60_20.ch.png") no-repeat;}
#com-topbar .level-4{background:url("http://d05.res.meilishuo.net/pic/_o/65/69/44e19a62beec452644edbeb96dd3_60_20.cg.png") no-repeat;}

#com-topbar a em{
	background: url("http://i.meilishuo.net/css/images/header/mls_global140607.png") no-repeat;
	padding: 1px 12px;
}
#com-topbar .weixin{
	background: url("http://i.meilishuo.net/css/images/header/i_wx_single.png") no-repeat;
	padding: 1px 11px;
}
#com-topbar .like{
	background: url("http://i.meilishuo.net/css/images/header/heart.png") no-repeat;
	padding: 1px 11px;
}
#com-topbar .qq{
	     background-position: -186px -21px;
}
#com-topbar .cart{
	    background-position: -1px -163px;
}
#com-topbar .cart-num,#com-topbar .msg-num{
	color: #fff;
	background-color: #ff6497;
	padding:0 6px;
}
#com-topbar .order{
	    background-position: -218px -45px;
}
#com-topbar .mob{
	    background-position: -164px -45px;
}
#com-topbar .arrow{
	display: inline-block;
	padding: 0;
	width: 10px;
	height: 6px;
	background-position: -20px -70px;
}
#com-topbar .drop{
	position: relative;
	z-index: 110;
}
#com-topbar .down{
	position: absolute;
	top: 30px;
	right: 0;
	z-index: 999;
	background-color: #fff;
	padding: 5px 10px;
	border: 1px solid #eee;
	line-height: 20px;
	display: none;
}
#com-topbar .down li{
	text-align: left;
}
#com-topbar .down a{
	border: none;
 	padding: 0 5px; 
}
#com-topbar .account{
	width: 70%;
}
#com-topbar .account li{
	width: 100%;
}
#com-topbar .account li:hover{
	background: #eee;
}
#com-topbar .msg{
	width: 120px;
}
#com-topbar .msg li{
	width: 100%;
}
#com-topbar .msg li:hover{
	background: #eee;
}
#com-topbar .app li{
	text-align: left;
	width: 240px;
	padding-top: 5px;
}
#com-topbar .app .client{
	font-size: 14px;
	color: #ff6699;
}
#com-topbar .app img{
	float: left;
	padding-right: 10px;
	width: 70px;
	height: 70px;
}
#com-topbar .notice i{
	font-style: normal;
	color: #f69;
	font-weight: bold;
}
#com-topbar .down li.know{
	text-align: right;
	border-bottom: 1px solid #e6e6e6;
	cursor: pointer;
	padding-right: 5px;
}
#com-topbar .down li.know:hover{
	background: none;
}
