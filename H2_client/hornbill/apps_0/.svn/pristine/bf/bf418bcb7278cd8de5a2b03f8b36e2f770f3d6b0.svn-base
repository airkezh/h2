@import "../block.less";

body{
	.bgc(#f8f8f8);.w(6.4rem);
}
img{
	.w(100%);
	.block;
} 
a{
	.block;
}

.seckillBox{
	.mt(.04rem);
	border-top: solid 1px #efefef;
	border-bottom: solid 1px #efefef;
	.bgc(#fff);
}
.seckillBox .lrboxs>div{
	.hidden;
	a{
		.pr;
		.mb(.14rem);
	}
	.infobox{
		.pa;
		.h(.42rem);
		.l(.42rem);
		.w(100%);
		bottom: 0;
		left: 0;
		.fc(#fff);
	}
	.price{
		.pa;
		top: 0;
		left:0;
		.w(.58rem);
		.h(.42rem);
		
		.bgc(rgba(255,95,95,.9));
		.f(.21rem);
		.c;
		overflow: hidden;
		big{
			.f(.3rem);
		}
	}
	.title{
		.pa;
		left: .58rem;
		top: 0;
		.w(2.34rem);
		.h(.42rem);
		.f(.22rem);
		.bgc(rgba(0,0,0,.6));;
		label{
			.mr(.12rem);
			.ml(.1rem);
		}
	}
	.cp{
		.c;
	}
}
.seckillBox .leftm{
	.left;
	.w(2.92rem);
	.h(3.54rem);
	border: solid 1px #e5e5e5;
	img{
		.h(3.54rem);
	}
}
.seckillBox .rightm{
	.right;
	.w(2.92rem);
	.h(3.56rem);
	.hidden;
	a{
		.h(1.69rem);
		border: solid 1px #e5e5e5;
		img{
			.h(1.69rem);
		}
	}
	.infobox .title{
		.w(2.33rem);
	}
	// .bolay{
	// 	.mt(.14rem);
	// }
}


.secregion{
	padding: .28rem .2rem 0 .2rem;
}

.lrboxs{
	.clearfix;
}

.headInfo{
	.pb(.1rem);
	.icon{
		.left;
		.w(.53rem);
		.h(.71rem);
		.ml(.16rem);
		background: url(http://d01.res.meilishuo.net/pic/_o/d2/a0/c4e44ff5094782d2edaa0df55059_53_70.cf.png) no-repeat;
		background-size: 100%;
	}
	.tm{
		background: url(http://d05.res.meilishuo.net/pic/_o/36/75/fe71bac12f2d1f929afb2c726216_53_70.cg.png) no-repeat;
		background-size: 100%;
	}
	.qing{
		background: url(http://d03.res.meilishuo.net/pic/_o/24/9b/f9d04d8e0be2e15f134002d40c51_53_71.cf.png) no-repeat;
		background-size: 100%;
	}
	.tip{
		.pr;
		.pt(.1rem);
		.ml(.81rem);

		.title{
			.fc(#adadad);
			.f(.18rem);
			strong{
				font-weight: normal;
				letter-spacing: -0.01rem;
				.f(.24rem);
				.fc(#000);
				.mr(.09rem);
			}
		}
		.timetip{
  			.f(.20rem);
  			.pa;
  			top: 31%;
  			right: .36rem;
  			.fc(#5f5f5f);
  			label{
  				.i_block;
  				.w(.32rem);
  				.h(.32rem);
  				.l(.32rem);
  				.bgc(#5f5f5f);
  				.c;
  				border-radius: .06rem;
  				.fc(#fff);
  				.ml(.05rem);
  				.mr(.05rem);
  			}
		}
		.content{
			.fc(#adadad);
			.f(.2rem);
			.mt(.06rem);
		}
	}
	.tip:after{
		content: '';
		.w(.16rem);
		.h(.24rem);
		.block;
		.pa;
		right: 0;
		top: 35%;
		background: url('http://d04.res.meilishuo.net/pic/_o/e2/1e/40e5ea8417eab6a00c2e129428b4_16_25.cg.png') no-repeat;
		background-size: 100% 100%;
	}
}
.desc{
	.mt(.18rem);
	.c;
	p{
		.mt(.09rem);
	}
	.feature{
		.h(.22rem);
		.l(.23rem);
		overflow: hidden;
		.fc(#666);
		.f(.22rem);
	}
	.feature:before{
		content: '';
		.i_block;
		.w(.09rem);
		.h(.17rem);
		.mr(.02rem);
		background: url(http://d01.res.meilishuo.net/pic/_o/5a/59/80c97dce55ea62147ba35ac9c63b_6_15.cf.png) no-repeat;
		background-size: 65%;
	}
	.feature:after{
		content: '';
		.i_block;
		.w(.09rem);
		.h(.17rem);
		.ml(.04rem);
		vertical-align: text-bottom;
		background: url(http://d05.res.meilishuo.net/pic/_o/b6/ae/126bb06a457735e0cb54dbbd2c11_6_15.cg.png) no-repeat;
		background-size: 65%;
	}
	.title{
		.h(.20rem);
		.l(.21rem);
		overflow: hidden;
		.fc(#b6b6b6);
		.f(.20rem);
	}
	.special{
		.fc(#666);
	}
	.price{
		.h(.32rem);
		.l(.32rem);
		.fc(#fe0000);
		.f(.18rem);
		
		.nowp{
			.f(.28rem);
		}
		del{
			.fc(#d1d1d1);
			.ml(.1rem);
		}
	}
	.discount{
		.ml(.15rem);
		.bgc(#ff5f5f);
		.fc(#fff);
		.f(.16rem);
		vertical-align: top;
		padding : 0.04rem 0.06rem;
	}
	.status{
		.w(2.4rem); .border-1(#f66); .h(.36rem); .auto; .border-r(3px); .f(.2rem); .l(.36rem); .mt(.09rem); .hidden;
		span.status_num, span.status_btn{
			.w(50%); .h(100%); .c; 
		}
		.status_num{ .left;
			i{ .fc(#f66); font-style: normal;}
		}
		.status_btn{ .right; .f(.22rem); .white; .bgc(#f66);}
	}
}
.seckillBox .goodsWall{
	.pl(.13rem);
	.pr(.13rem);
	.goodsInfo .link_box { .ml(.07rem); .mr(.07rem);}
}
.goodsWall{
	.pl(.1rem);
	.pr(.1rem);
	.pb(.26rem);
	.goodsInfo {
		.left;.w(50%);.mt(.14rem);
		.imgwrap{
			border: solid 1px #e5e5e5;
			.pr;
			.countdown{
				.pa;
				bottom: 0;
				left: 0;
				.w(100%);
				.h(.36rem);
				.l(.36rem);
				.f(.18rem);
				.c;
				.fc(#fff);
			}
			.content{.pr;}
			.progress{
				.pa;
				.h(.36rem);
			}
			.rpbgc{
				left: 0;
				.bgc(rgba(255,0,0,.6));
			}
			.gpbgc{
				right: 0;
				.bgc(rgba(0,0,0,.6));
			}
		}
		.link_box { 
			.ml(.1rem); .mr(.1rem);
		}
	}
	.clearfix;
}
.more{
	.block;
	.pt(.26rem);
	.pb(.32rem);
	.c;
	.f(.22rem);
	.fc(#9b9b9b);
	.ml(.1rem);
	.mr(.1rem);
	border-top: dashed 1px #e7e7e7;
}
.tuanBox,.qingcangBox{
	.mt(.3rem);
	.mb(.3rem);
	border-top: solid 1px #efefef;
	border-bottom: solid 1px #efefef;
	.bgc(#fff);
	.headInfo{
		padding: .2rem .2rem 0 .2rem;
		border-bottom: none;
		overflow: hidden;
	}
}
.tuanBox > .headIcon .icon{
	background: url(http://d05.res.meilishuo.net/pic/_o/9e/e4/60077e51eb0ae64dde42ec77d433_70_70.cf.png) no-repeat;
	background-size: 100%;
}

.soldout{
	.pr;
}
.rightm .soldout:after{
	width: 32%;
	height: 32%;
	top: 34%;
	left: 34%;
}
.soldout:after{
	content: '';
	display: block;
	background: url(http://d02.res.meilishuo.net/pic/_o/ab/f6/6851076401a266f70b5cefc9e2de_168_97.ch.png) no-repeat;
	background-size: 100% 100%;
	position: absolute;
	width: 58%;
	height: 26%;
	top: 37%;
	left: 21%;
}

/*秒杀商品是方图*/
.seckillBox .goodsWall span.bs_load_img {
	display: block;
	opacity: .75;
	width: 100%;
	padding-top: 100%;
	background-image: url("http://i.meilishuo.net/css/images/huodong/tuan/prebg.png");
	background-size: 100% 100%;
}
/*团购商品是长图*/
.tuanBox span.bs_load_img {
	display: block;
	opacity: .75;
	width: 100%;
	padding-top: 140%;
	background-image: url("http://i.meilishuo.net/css/images/huodong/tuan/pre1.png");
	background-size: 100% 100%;
}

.qingcangBox{
	.pl(.1rem);
	.pr(.1rem);
	.item{
		.mt(.18rem);
		border: solid 1px #e5e5e5;
	}
}
.gotop{
	.w(.82rem);
	.h(.82rem);
	.none;
	right: .1rem;
	position:fixed;
	z-index: 100;
	bottom: 12%;
}

