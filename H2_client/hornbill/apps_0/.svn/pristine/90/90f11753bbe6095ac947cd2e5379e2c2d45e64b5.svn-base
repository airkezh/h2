@import "../block.less";

html{
	.f(10*@f);
}

body{
	.bgc(#fff);
}

img{
	.block;
	.w(100%);
}

a{
	.block;
	outline:none;
}

ul{
	list-style:none;
}

i{
	font-style:normal;
}

@750w:1/750*100%;

/* 0.5的水平线 */
.line-h05(@lw){
	width:@lw;
	height:1px;
	-webkit-transform:scaleY(.5);
	background-color:rgb(213,213,213);
}

.lhp100{
	.line-h05(100%);
}

.beans_tips_wrap{
