.shadow{
	box-shadow:7px 2px 7px #e6e6e6;
	/*for ie6,7,8*/		
	filter:progid:DXImageTransform.Microsoft.Shadow(Strength=7, Direction=180, Color='#e6e6e6');
}

.scrollin{
	.pf;
	top:-1px;
	left:0;
	right:0;
	margin:0 auto;
	z-index:100;
}

.scrollout{
	.pr;
}

.banner_box{
	.hidden;
	margin:0 1rem;

	li{
		.left;
		.w(100%);
		.mt(1rem);
	}
}

.recommend_box{
	.hidden;
	margin:0 1rem 1rem;

	li{
		.left;
		.w(48.3%);
		.mt(1rem);
	}

	.odd{
		li:nth-child(odd){.mr(3.4%);}
	}
}

.container{
	.hidden;
	min-height:700px;
	margin:0 30*@750w 30*@750w;

	.even{
		li:nth-child(odd){.mr(4%);}
	}

	.odd{
		li:nth-child(odd){.mr(4%);}
	}
}

.goods_wall{
	.clearfix;

	li{
		.left;
		.mr(0);
		.w(48%);

		.link_box{
			.block;
		}

		.hjfpos{
			.pr;

			.ownh{
				height:61px;
			}

			.adenter{
				.pa;
				top:0;
				left:0;
				.h(100%);
			}
		}

		figure{
			.pr;

			.soldout{
				.pa;
				top:50%;
				left:50%;
				z-index:10;
				.w(298*@750w);
				.pt(298*@750w);
				margin:-149*@750w 0 0 -149*@750w;
				background:url("http://d04.res.meilishuo.net/pic/_o/1a/d7/70bb562514b6d8f86d9dcf7699d9_149_149.ch.png") no-repeat center;
				background-size:100%;
			}

			span.bs_load_img{
				.block;
				.w(100%);
				.pt(140%);
				opacity:.75;
				background-image:url("http://d01.res.meilishuo.net/pic/_o/b9/0a/fd965c6f94c2fd9df704e481098f_304_425.cg.png");
				/*background-image:url("http://d05.res.meilishuo.net/pic/_o/33/cf/a618bae2cdf43bc03289e82cb53f_290_290.cg.png");*/
				background-size:cover;
			}

			span.df_show_img{
				.block;
				.w(100%);
				.pt(140%);
				opacity:.75;
				background-image:url("http://d01.res.meilishuo.net/pic/_o/b9/0a/fd965c6f94c2fd9df704e481098f_304_425.cg.png");
				/*background-image:url("http://d05.res.meilishuo.net/pic/_o/33/cf/a618bae2cdf43bc03289e82cb53f_290_290.cg.png");*/
				background-size:cover;
			}

			div.new_png{
				.pa;
				.w(100%);

				div{
					.left;
					.ml(6/290*100%);

					img{
						.border-r(0 0 0 0);
					}
				}
			}

			.lave{
				.pa;
				.c;
			    .w(100%);
			    .fc(#fff);
			    .f(1rem);
			    bottom:0;
			    z-index:90;
			    padding:4px 0;
		    }

		    .progress{
		    	.pa;
				.block;
				.f(10px);
				.w(100%);
				.h(12px);
				bottom:0;
				opacity:.6;
				padding:4px 0;
		    }

		    .rpbgc{
		    	.bgc(#dd8200);
		    }

		    .gpbgc{
		    	.bgc(#80737b);
		    }

		    .ypbgc{
		    	.bgc(#ffb414);
		    }
		}

		figcaption{
			.c;
			.bgwhite;
			padding:8px 0 16px;

			.pro{
				&Title{
					.hidden;
					.l(1.4);
					.f(1.2rem);
					.fc(#666);
					text-overflow:ellipsis;
					display:-webkit-box;
					-webkit-line-clamp:1;
					-webkit-box-orient:vertical;
					clear:both;
				}

				&Price{
					.l(1.8);
					.i_block;
					.f(1.3rem);
					.fc(#ff4b42);

					i{
						font-style:normal;
					}

					.origPrice{
						.fc(#999);
						.f(1.2rem);
					}

					.word{ 
						.fc(#999);
						.f(1.2rem);
					}
				}

				&Discount{
					.l(2);
					.w(80%);
					.f(1.2rem);
					.fc(#ff9f17);
					margin:0 auto;
					.border-r(12px);
					.bgc(rgba(255,159,23,0.1));

					.bean{
						.pl(30*@px);
						background:url("http://d01.res.meilishuo.net/pic/_o/9d/a0/76a466d841d5156aa297b243d50f_22_22.cf.png") no-repeat;
						background-size:12px auto;
					}
				}
			}
		}
	}
}

.more_bar{
	.c;
	.none;
	.w(90%);
	.l(1.4);
	.f(1.3rem);
	.fc(#595056);
	.border-r(4px);
	padding:8px 0;
	margin:10px auto;
	border:solid 1px #595056;
}

.pullUp{
	.pullUpDown > .common;.pullUpDown > .loading;
	&:before{
		.nowrap;
		.ml(-10px);
	}
	&[status="tap"]{
		background:0 none;	
	}
	&[status="stop"]{
		background:0 none;	
	}
}

.gotop_wrap{
	.pf;
