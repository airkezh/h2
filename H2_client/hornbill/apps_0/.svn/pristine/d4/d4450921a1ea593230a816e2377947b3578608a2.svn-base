/**
 * @fileoverview 首页－会员晋级礼包
 * @requires
 * @author kehuang@meilishuo.com
 * @date 2015-11-02
 */

@import "../block.less";

html{
	.f(10*@f);
}

body{
	.bgc(#fff);
}

img{
	.block;
	.w(100%);
}

a{
	.block;
	outline:none;
}

ul{
	list-style:none;
}

i{
	font-style:normal;
}

.top_marqueen_wrap{
	.pr;
	.hidden;
	.l(2rem);
	.h(2rem);
	.f(1.1rem);
	.fc(#fff);
	.bgc(rgba(0,0,0,.5));

	.top_marqueen_box{
		.w(560*@w);
		margin:0 40*@w;
		
		p{
			.tl;
			.w(800px);
			white-space:nowrap;
		}
	}

	.close_marqueen_btn{
		.pa;
		top:0;
		right:0;
		.w(10px);
		.h(10px);
		margin:6px;
		background:url("http://d02.res.meilishuo.net/pic/_o/c5/31/931b04a51b5505b957b2231eadf9_15_15.cg.png") no-repeat;
		background-size:100%;
	}
}

.no_gift_list{
	.c;
	.w(50%);
	.l(1.4);
	.f(1.2rem);
	.fc(#666);
	margin:20% auto;
}

.gift_list_wrap{
	.hidden;
  	clear:both;
  	.bgc(#f5f5f5);

  	.gift_list{
  		.auto;
  		.mb(20*@w);
  		.bgc(#fff);
  		padding:30*@w;

    	.gift_title{
    		.pr;
    		.w(100%);
    		.pt(55*@w);
    		.f(1.6rem);
    		.fc(#fff);
    		background:url("http://d01.res.meilishuo.net/pic/_o/9a/07/934865f4cf0ac5ce4135d994803f_580_55.cf.png") no-repeat;
    		background-size:100%;

    		p{
    			.c;
    			.pa;
    			top:0;
    			left:10%;
    			.l(28px);
    			.w(80%);
    			.h(28px);
    		}

    		i{
    			.fc(#fee503);
    		}
    	}

    	.item_title{
    		.c;
    		.f(1.3rem);
    		.fc(#333);
    		margin:30*@w 0;
    	}

    	.no_gift_bg{
    		.pt(210px);
    		background:url("http://d05.res.meilishuo.net/pic/_o/ca/e4/8638dbfbf7c363dfaff8ad9d763e_580_420.cf.png") no-repeat;
    		background-size:100%;
    	}

    	.no_gift_text{
    		.c;
    		.l(1.8);
    		.f(1.4rem);
    		.fc(#333);

    		i{
    			.fc(#fe6054);
    		}
    	}

    	.receive_gift_bg{
    		.pt(170px);
    		background:url("http://d01.res.meilishuo.net/pic/_o/7d/ca/e2a01d8a8572dfb760bab41dd227_580_340.cf.png") no-repeat;
    		background-size:100%;
    	}

    	.receive_gift_btn{
    		.c;
    		.w(32%);
    		.l(30px);
    		.h(30px);
    		.f(1.5rem);
    		.fc(#fff);
    		.bgc(#fe6054);
    		margin:30*@w auto;
    	}
  	}

  	li.gift_list:last-child{
    	.mb(0);
  	}
}

.shop_coupon_wrap{
  	.hidden;
  	clear:both;

  	li{
  		.mb(20*@w);
    	padding:20*@w;
    	border:1px solid rgba(3,0,0,.1);
  	}

  	li:last-child{
    	.mb(0);
  	}

  	.coupon{
    	.pr;
    	.pt(140/600*100%);
  	}

  	.shop_wrap{
    	.hidden;
    	.mb(20*@w);

    	.img_box,.shop_box{
      		.left;
    	}

    	.img_box{
      		.w(23%);

      		img{
        		.w(59px);
        		.h(59px);
      		}
    	}

    	.shop_box{
      		.w(77%);
      		margin:12px 0 13px;
      		background:url("http://d04.res.meilishuo.net/pic/_o/03/dc/0555cd3f35af8a8e211a36cc52cd_22_68.cg.png") no-repeat;
      		background-size:11px auto;
      		background-position:right center;

      		p{
        		.hidden;
        		.w(88%);
        		.ml(20*@w);
        		white-space:nowrap;
        		text-overflow:ellipsis;
      		}

      		.title{
        		.l(1.5);
        		.f(1.3rem);
        		.fc(#333);
      		}

      		.desc{
        		.l(1.3);
        		.f(1rem);
        		.fc(#999);
      		}
    	}
  	}
}

.interests_wrap{
	.pr;
	.hidden;
	.w(100%);

	.interests_iscroll{
		.left;
		.hidden;
		.w(100%);
		.bgc(#fff);

		li{
			.c;
			.pr;
			.left;
			.block;
			.w(80px);
			.f(.9rem);
			.fc(#ccc);

			.text_title{
				.mt(10px);
			}

			.member_interests{
				.pt(50px);
				background:url("http://d03.res.meilishuo.net/pic/_o/64/46/1b3dcdf5f969bd18420e5b11bd74_100_100.cg.png") no-repeat;
				background-size:50px auto;
				background-position:center;
			}
		}

		li.active{
			.fc(#666);
		}
	}
}

.layer_shade{
	.pf;
	top:0;
	left:0;
	.w(100%);
	.h(100%);
	z-index:999;
	background:rgba(0,0,0,.85);
}

.layer_main{
	.pf;
	top:0;
	left:0;
	.w(100%);
	.h(auto);
	z-index:999;
}

.gift_main{
	top:5%;

	.layer_bg{
		.c;
		.pr;
		.auto;
		.w(596*@w);
		.pt(721*@w);
		background:url("http://d05.res.meilishuo.net/pic/_o/88/13/9ee099a15985637a6ece88193186_596_721.cg.png") no-repeat;
		background-size:100%;
	}

	p{
		.c;
		.pa;
		.w(88%);
		left:6%;
		.fc(#ff3826);
	}

	.title{
		top:21%;
		.f(1.6rem);
		.fc(#fff);

		i{
			.f(3rem);
			.fc(#fee503);
			font-weight:900;
			-webkit-text-fill-color:#fee503; /*文字的填充色*/
            -webkit-text-stroke:2px #ff3826; /*描边的像素，也就是粗细，这里指定是2像素的黑色边框*/
		}
	}

	.gift_img{
		.w(24%);
		left:38%;
		top:40%;
	}

	.gift_name{
		top:53%;
		.f(1.4rem);

		i{
			.f(1.7rem);
		}
	}

	.gift_title{
		top:34%;
	}

	.interests_title{
		top:60%;
	}

	.layer_receive_btn{
		.c;
		.pa;
		bottom:0;
		.f(1.5rem);
		.fc(#fe6054);
		padding:4% 0;
	}
}

.layer_interests_wrap{
	.pa;
	.w(88%);
	top:66%;
	left:6%;
	.hidden;

	.layer_interests_iscroll{
		.left;
		.hidden;
		.w(100%);

		li{
			.c;
			.left;
			.block;
			.w(80px);
			.f(.9rem);
			.fc(#fe6054);

			.text_title{
				.mt(10px);
			}

			.member_interests{
				.pt(50px);
				background:url("http://d03.res.meilishuo.net/pic/_o/64/46/1b3dcdf5f969bd18420e5b11bd74_100_100.cg.png") no-repeat;
				background-size:50px auto;
				background-position:center;
			}
		}
	}
}

.windowDrive{
	.dialog{
		.border-r(6px);
	}

	.dialog-con{
		.l(1.8);
		.f(1.4rem);
		.fc(#666);
	}

	.dialog-btn{
	    border-top:1px solid #e5e5e5;

	    span{
	      	.f(1.5rem);
	      	.fc(#f66);
	    }
	}
}

.pullUp{
	.pullUpDown > .common;.pullUpDown > .loading;
	&:before{
		.nowrap;
		.ml(-10px);
	}
	&[status="tap"]{
		background:0 none;	
	}
	&[status="stop"]{
		background:0 none;	
	}
}

.gotop_wrap{
	.pf;
	.none;
	.w(82*@w);
	.pt(82*@w);
	z-index:10;
	right:10px;
	bottom:40px;

	.gotop{
		.block;
		.w(100%);
	}

	img{
		.w(100%);
	}
}

@media screen and (min-width:360px){
	.gift_main .title{
		top:22%;
	}
}

@media screen and (min-width:400px){
	.gift_main .title{
		top:23%;
	}
}