@import "bs_base.less";

body{.bgc(#efeff4);}
.act_box{.pt(20*@w);}
.act_img{
	.block;.hidden;
	&:hover {.fc(#666);}
	img{border-top: 1px solid #dedede;min-height: 100px;.block;}
}
.time_wrap{
	.pt(20*@w);.pb(20*@w);.pl(40*@w);.bgc(#fff);border-bottom: 1px solid #dedede;
	&:before{
		content: "";
		.w(20px);.h(15px);.i_block;.mb(-3px);
		background: url("http://d06.res.meilishuo.net/img/_o/ff/4a/81f5d5a9d42073b5886d0c6a168a_30_30.c6.png") no-repeat; 
		background-size: 15px;
	}
}
.act_end:before{background: url("@{pic_url}/images/wap/biz/all/bs_list_time.png") no-repeat;background-size: 15px;}

