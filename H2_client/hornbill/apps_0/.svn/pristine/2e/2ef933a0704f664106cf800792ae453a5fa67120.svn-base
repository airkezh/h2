.shadow{
	box-shadow:7px 2px 7px #e6e6e6;		
	filter: progid:DXImageTransform.Microsoft.Shadow(Strength=7, Direction=180, Color='#e6e6e6'); 
}
.scrollout{
	.pr;
}
.scrollin{
	.pf;top:-1px;left: 0; right: 0; margin: 0 auto; z-index:100;
}
.bs_load_img{.block;opacity: .75;.w(100%);.pt(140%);background-image: url("@{pic_url}/images/huodong/tuan/pre1.png");background-size: cover;}
.gotop{.w(12%); .pf; right:15px; bottom:30px;}
.wrapper { .mt(3%); .bgwhite; .pt(5%);}
