@import "../../block.less";
html,body { .bgc(#efefef);}

img { .w(100%); .block;}
.main{
	.w(100%);
	margin:0 auto;
}
.nav_wap{
	.w(100%);
	.nav{
		.bgwhite;
		.hidden;
		.clearfix;
		.pt(4%);
		.pb(4%);
		
		li{
			.left;
			.w(20%);
			.c;
			img{
				.w(100%); .i_block;
			}
		}
	}
}
.goods_banner{
	.c; .pb(20*@px);
}
.new_goods{
	.w(50%); .pb(40*@px);
	.left;
	a{
		.block;
		margin:5px 3px;
	}
}
.more{
	.w(98%);
	.auto;
	.pb(4%);
