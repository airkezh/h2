.mask-bottom{
	display:none;
	.w(100%);
	.h(100%);
	background-color:rgba(0, 0, 0, 0.4);
	position:absolute;
	top:0;
	z-index:102;
}

.mask {
	display: none;
	position: absolute;
	top: 0;
	.w(100%);
	.h(100%);
