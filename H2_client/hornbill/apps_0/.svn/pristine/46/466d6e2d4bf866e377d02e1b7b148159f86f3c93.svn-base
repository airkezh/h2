}

.mypopbox{
	display: none;
	position: fixed;
	z-index: 999;
	top: 0px;
	left: 0px;
	width: 100%;
	height: 160%;
	overflow-x: hidden;

	.dialog {
		display: none;
		position: absolute;
		z-index: 100;
		top: 100px;
		background: #FBFBFB;
		width: 270px;
		left: 50%;
		margin-left: -135px;
		.close{
			position: absolute;
			top: 18px;
			right:18px ;
			width: 18px;
			height: 18px;
			background: url('http://d02.res.meilishuo.net/pic/_o/4c/57/e3f8cb2e0784eb61bdf8243a8993_32_32.ch.jpg');
			background-size: 100% 100%;
		}
		.con{
			padding:35px 35px 25px 35px;
			.expression{
				display: block;
				margin: 0 auto 13px;
				width:110px;
			}
			p{
				font-size: 13px;
				line-height:20px;
				text-align: center;
				color: #444;
			}
			.fz16{
				font-size: 16px;
			}
			.winBut,.againBut{
				display: block;
				margin: 10px auto;
				width: 200px;
				height: 34px;
				line-height: 34px;
				border-radius:17px;
				background-color:#F9D407;
				color: #fff;
				font-size: 15px;
				text-align: center;
			}
			.winBut:hover{
				color: #fff;
			}
		}
	}

	.overlay{
		display: none;
		position: absolute;
		z-index: 90;
		top: 0px;
		left: 0px;
		bottom: 0px;
		right: 0px;
		background-color: rgba(0, 0, 0, 0.5);
	}

}

.mypopbox .dialog_input{
	position: relative;
	top: 60px;
	display: none;
	background-color:  transparent;
	z-index: 100;
}
.dialog_input img {
  width: 100%;
}
.dialog_input  .mask-msg {
  position: absolute;
  top: 125px;
  width: 80%;
  left: 10%;
}
.dialog_input .mask-msg-inner {
  margin-bottom: 20px;
  overflow: hidden;
}
.dialog_input .mask-msg label {
  width: 40%;
  text-align: center;
  float: left;
  height: 25px;
  line-height: 25px;
}

.dialog_input .mask-msg input {
  width: 50%;
  height: 25px;
  float: left;
}
.dialog_input .mask-sub {
  position: absolute;
  bottom: 7.8125%;
}

.dialog_input .mask-sub .mask-btn-one {
  margin-left: 15%;
  float: left;
  width: 70%;
}


.shareTo{

	width: 100%;
	text-align: center;
	overflow: hidden;
	margin: 14px 0;
	a{
		display: inline-block;
		width: 45px;
		height: 45px;
		margin: 0 18px;
		background-size: 100% 100%;
	}
	.shareTo_pyq{
		background-image: url('http://d03.res.meilishuo.net/pic/_o/65/23/9e4e776ef1f8bcdbe52e91b0947b_90_90.cf.jpg');
	}
	.shareTo_wx{
		background-image: url('http://d03.res.meilishuo.net/pic/_o/ac/f6/f1f308b6cc055c6ec4c96037e290_90_90.cf.png');
	}
}



.shake-act {
    -webkit-animation-name: move;
    -webkit-animation-iteration-count: 4;
    -webkit-animation-timing-function: ease;
    -webkit-animation-duration: .25s;
    -webkit-animation-direction: normal;
    -webkit-animation-fill-mode: both;
}

@-webkit-keyframes move {
    0% {
        -webkit-transform: rotate(0deg)
    }

    25% {
        -webkit-transform:  rotate(-10deg)
    }

    75% {
        -webkit-transform: rotate(10deg)
    }

    100% {
        -webkit-transform: rotate(0deg)
    }
}






@media screen and (min-width:320px) and (max-width:360px){html{font-size: 100px;}}
@media screen and (min-width:360px) and (max-width:375px){html{font-size: 112.5px;}}
@media screen and (min-width:375px) and (max-width:414px){html{font-size: 117.18px;}}
@media screen and (min-width:414px) and (max-width:480px){html{font-size: 129.37px;}}
@media screen and (min-width:480px) and (max-width:640px){html{font-size: 150px;}}
@media screen and (min-width:640px){
	html{font-size: 200px;}
	.main {
  		width: 640px;
  		max-width: 640px;
  		margin: 0 auto;
	}
}



