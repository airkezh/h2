@import "block.less";
@import "window.less";
@import "block/common_sale.less";

@media screen and (max-width:1220px){
	body{.w(1220px);}
}

body { background: #da011e url("@{pic_url}/images/welcome11/wel_bg.gif") repeat; }

/* ===========原有模块需要的样式============ */

.top_bnr { .bgc(#f7f7f7);  .h(420px); .hidden; .w(960);
	margin-left:240px;
	.banner { .h(419px); .w(100%); z-index:0;
		li { .left; .w(100%);
			.pic { .block; .h(419px); .w(100%); }
		}
	}
}

.top_wrap { .auto; .w(1200px);
	.top_box { .pr; .w(100%); }
	.bnr_btn_wrap {
		.pr; top: -232px; z-index: 15;
		.bnr_btn { .w(100px); .h(44px); .block; .pa; .cursor; .none;}
		.bnr_btn_left{ left: 239px; background: url("@{pic_url}/images/welcome/bnr_btn_left1.png") no-repeat;}
		.bnr_btn_right { right: 0; background: url("@{pic_url}/images/welcome/bnr_btn_right1.png") no-repeat center right;}
	}
	//原二级导航
	.nav_attr { .bgwhite; .border-1(#e6e6e6); .left; .pl(19px); .pr(19px); .h(572px); .w(200px); .pa;left:0;top: -420px;
		li { .border-bc2; .pt(17px); .pb(10px); 
			.nav_tle { .h(18px); .f(16px);
				a { .red; }
			}
			p { .h(44px); .l(22px); .nowrap; .hidden; .pt(5px); .w(100%);
				a { .left; .mr(14px); }
			}
		}
		.last { .border-n; }
	}
	.attr_box { .w(240px);  z-index: 10; .h(574px); .pa; left:0; top: -420px; 
		.sec_attr { .bgwhite; .border-1(#e6e6e6); .left; .h(572px); .w(238px); 
			.list { .pr;  .border-bc2; .ml(20px); .mr(20px);  
				.nav_tle { .h(18px); .f(14px);
					.nav_ico { .left; .h(18px); .w(23px); }
					.logo1212 { .left; .w(43px); .h(18px); background: url("@{pic_url}/images/meilishuo/logo12.png") no-repeat left top; background-size: auto 100%;  }
					a { .red; .left; .mr(10px); }
				}
				p.listP {.h(72px); .l(24px); .nowrap; .hidden; .w(190px); .pt(5px);
					a { .left; .mr(14px); }
				}
				.corner { .f(14px); .gray; .pa; right: -5px; top: 48px; }
			}
			.list_cont { .pr; z-index: 8; padding: 12px 0 6px;}
			.active { .bgwhite; border-top: 1px solid #e6e6e6; border-bottom: 1px solid #e6e6e6; margin: 0; .pl(20px); .mt(-1px);
				.corner { .red; right: 15px; }
				.list_cont {.mr(-1px);  border-right: 1px solid #fff;}
			}
			.last { border-bottom: none;}
			.list_cont_last { .pb(9px);}
		}
	}
	.nav_list { .none; .border-1(#e6e6e6); .bgwhite; .pl(20px); .w(330px); z-index: 4; .pb(10px);
		.pa; top: -1px; right: -352px;
		h4 { .f(14px); .h(20px); .l(20px); .pt(12px); }
		p { .l(24px); .nowrap; .hidden; .w(100%);
			a { 
				.left; .mr(14px); .fc(#666); 
				&:hover { .red;}
			}
		}
	}
	.round { .w(440px); .pa; right: 225px; top: -35px; 
		.adType{ text-align:center; 
			a { .bgwhite; .border-r(10px); .cursor; .i_block; .mr(22px); .h(10px); .w(10px); }
			a.current { .bgnone; border: 3px solid #fff; .mb(-4px); .h(12px); .w(12px); }
		}
	}
	.bnr_box { .h(145px); .w(960px); .pa; right:0; top: 0; z-index: 1; .bgwhite; .pt(9px);
		.pic_attr li, .ads_one {  .left; .hidden; .h(145px); }
		.pic_attr { .left; .hidden; .h(145px); 
			li { .ml(13px); .hidden; .inline; .w(227px); .w(182px); .ml(10px);}
			img{ .h(145px); }
		}
		.ads_one { .w(198px); }
	}
}
.common_title;
.show { display: block; }
.hide { display: none; }
.ads_banner { margin:0; }
.footer .foot_con { .w(1200px); .auto; }
.groupBox { .left; .mr(15px); .w(228px); 
	.groupCon { .group > .9GG; .hidden; }
}
.groupBox.last { .mr(0); }
.words { .border-rc(dotted); .p0_13; }

.auto_wrap { 
	.hidden; .auto; .w(1200px);
}
/* =================END================== */

/*双11大促首页 start*/
.bnr_11 { .h(150px); .bgwhite; margin: 154px auto 0; .w(1200px); .pt(10px); }

.ico_11 { background: url("@{pic_url}/images/staticbed/ico_11.png") no-repeat;
	.h(82px); .w(83px); .pa; left: -5px; top: -5px;
}
.sales_one,.sales_two,.sales_three,.sales_four,.sales_five,.sales_six,.sales_sev{ .pb(30px); margin: 30px auto 0; .w(1200px);.box-shadow-r(2px,50px,50px,rgba(00,00,00,0.2)); }
/*hover 效果*/
.hoverBox li { .pr; .left; .mt(18px); .ml(12px);
	a{ .left; .block; } 
	.hoverpic { background:rgba(255,255,255,0.1); .pa; left: 0; top: 0; z-index: 1; }
}
/*模块*/
.sales_one { background: url("@{pic_url}/images/welcome11/nszc.png") no-repeat; .pt(402px);
	.one_top{ 
		a { .h(298px); .w(384px); }
	}	
	.one_bot {
		a { .h(258px); .w(285px); }	
	}
}
.sales_two { background: url("@{pic_url}/images/welcome11/tsfashion.png") no-repeat; .pt(396px); 
	.fashion { 
		li { .h(250px); .w(582px); .left; .pr; .ml(12px); .mt(12px); 
			.shark_ico { .f(36px); .white; .pa; left: 191px; top: 103px; z-index: 2; }
			.f_tle { .w(204px); .hidden; .left; }
			.f_pic { .w(378px); .h(250px); .hidden; .left; 
				img { transition: all 1000ms ease 0s;-webkit-transform: all 1000ms ease 0s; }
				&:hover img { transform: scale(1.1); -webkit-transform: scale(1.1);}
			}
		}
	}
}
.sales_three { background: url("@{pic_url}/images/welcome11/styles.png") no-repeat; .pt(414px);
	.hot_style li { .mt(12px);
		a { .h(268px); .w(186px); }
	}
}
.sales_four { background: url("@{pic_url}/images/welcome11/caizhi.png") no-repeat; .pt(402px);
	.caizhi {
		a{ .h(354px); .w(285px); }
	}
}
.sales_five { background: url("@{pic_url}/images/welcome11/yuansu.png") no-repeat; .pt(402px);
	.yuansu {
		a{ .h(354px); .w(285px); }
	}
}
.sales_six { background: url("@{pic_url}/images/welcome11/jujia.png") no-repeat; .pt(410px);
	.big_pic { .left; .w(594px);
		li { .mt(12px); 
			a{ .h(432px); .w(582px); }
		}
	}
	.small_pic {.left; .w(600px);
		li { .mt(12px);
			a{ .h(210px); .w(285px); }
		}
	}
}
.sales_sev { background: url("@{pic_url}/images/welcome11/chaozhi.png") no-repeat; .pt(400px); .mb(40px);
	.chaozhi_t { .w(1200px);
		a{ .h(354px); .w(285px); }
	}
	.chaozhi_b { .w(1200px);
		li{ .mt(12px);
			a{ .h(200px); .w(582px); }
		}
	}
}
/* 底通 */
.bottom_bnr { .w(1200px);margin: 0 auto;.mt(-20px);.mb(40px); }
/*END*/
