@import "../block.less";

.sq{
	margin :1px;
	width:19px;
	height:19px;
	background:#0ff;
	position:absolute;
}
.con{
	top:0px;
	left:0px;
	width:100%;
	background:#555;
	position:absolute;
}
 .d{
	background:gray;
	margin :1px;
	width:19px;
	height:19px;
	position:absolute;
}
#side{
	position: relative;
}
#top{
	position: absolute;
	top:180px;
	left: 250px;
}
#right{
	position: absolute;
	top:230px;
	left: 300px;
}
#left{
	position: absolute;
	top:230px;
	left: 210px;
}
#bottom{
	position: absolute;
	top:300px;
	left: 250px;
}

#start{ 
	position: absolute;
	top:120px;
	left: 250px;
	cursor:pointer;

}
