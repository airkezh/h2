/* 弹出框 */
.mypopbox{
	display:none;
	position: fixed;
	z-index: 999;
	top: 0px;
	left: 0px;
	width: 100%;
	height: 160%;
	overflow-x: hidden;

	.dialog {
		display:none;
		position: absolute;
		z-index: 100;
		top: 1.65rem;
		width: 5.7rem;
		height: 7.65rem;
		left: 50%;
		transform: translateX(-50%);
		-webkit-transform: translateX(-50%);
	}

	.dialog_appshare{
		width: 4.5rem;
		height: 2.91rem;
		top:3rem;
		background: #e2738d;
		border-radius: .2rem;
		text-align: center;
		color: #fff;
		.dialog_appshare_tit{
			height: 1rem;
			line-height: 1rem;
			text-align: center;
			font-size: .33rem;
		}
		.dialog_appshare_box{
			float: left;
			width: 50%;
			overflow: hidden;
			p{
				line-height: .6rem;
				font-size: .21rem;
			}
		}
		.shareBut{
			display: inline-block;
			margin: 0   .6rem ;
			width: .95rem;
			height: .95rem;
		}
		.shareBut_wx{
			background:url('http://d04.res.meilishuo.net/pic/_o/85/7a/aab5e12571cce8c326038468beb5_95_95.cg.png');
			background-size:100% 100%;
		}
		.shareBut_pyq{
			background:url('http://d03.res.meilishuo.net/pic/_o/ed/1e/c4b27c9150dfd8cf374366ffd883_95_95.ch.png');
			background-size:100% 100%;
		}

		.shareBut_close{
			position: absolute;
