.place-hold{
	.h(28*@px);
	.w(100%);
	.bgc(#f0f0f0);
}
.goods-li{
	a:hover,a:visited{
		.fc(#666);
	}
	.pt(22*@px);
	.bgc(#fff);
	.left{
		.left;
	}
	.li-head{
		.w(600*@w);
		.auto;
		.mb(20*@px);
		.hidden;
		.avatar{
			.w(60*@px);
			.pr;
			img{
				.w(60*@px);
				.h(60*@px);
				.border-r(60*@px);
			}
			span{
				.pa;
				right: 0px;
				bottom: 0px;
				.w(25*@px);
				.h(25*@px);
				background-image: url('http://d03.res.meilishuo.net/pic/_o/a7/7c/38f006213ead2165e17f42fb1774_26_26.ch.png');
				background-size: cover;
			}
		}
		.name{
			.f(30*@px);
			.l(60*@px);
			.h(60*@px);
			.ml(5px);
			.w(320*@w);
		}
		.follow-btn{
			.c;
			.w(100*@px);
			.h(60*@px);
			.f(26*@px);
			.l(60*@px);
			.border-1(#ff7a9a);
			.bgc(#ff7a9a);
			.fc(#fff);
			.border-r(8*@px);
		}
		.follow-btn-ok{
			.c;
			.w(100*@px);
			.h(60*@px);
			.f(26*@px);
			.l(60*@px);
			.border-1(#ff7a9a);
			.bgc(#fff);
			.fc(#ff7a9a);
			.border-r(8*@px);
		}
	}
	.li-info{
		.w(600*@w);
		.auto;
		.hidden;
		a{
			.w(290/6%);
			.block;
			.pr;
			span{
				.pa;
				bottom: 5px;
				right: 5px;
				.fc(#fff);
				.f(32*@px);
			}
		}
		a:nth-child(2){
			.ml(10/3%);
		}
	}
	.li-tag{
		.hidden;
		.w(600*@w);
		.auto;
		.mt(28*@px);

		a{
			.mr(3px);
			.h(29px);
			.block;
			.left;
			img{
				.w(26*@px);
				.inline;
				.mr(6px);
				.left;
			}
			span{
				.fc(#ffaabe);
				.f(26*@px);
				.l(28*@px);
			}
			.mr(5px);
		}
	}
}
.banner-li{
	.w(100%);
	.block;
}