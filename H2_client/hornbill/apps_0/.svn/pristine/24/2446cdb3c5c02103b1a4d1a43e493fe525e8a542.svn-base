<div class="groupBox">
	<a class="groupCon" href="/guang/attr/<%= this.attr_id%>">
		<h4><%= this.name%></h4>
		<div class="pic_box">
			<%if(this.mix_pic){%>
			<p class="maxpic">
				<img src="<%= this.mix_pic%>">
			</p>
			<%}else{%>
			<p class="minpic">
				<% this.pics && this.pics.map(function(pic , index){%>
				<img src="<%= pic%>">
				<%});%>
			</p>
			<%};%>
		</div>
	</a>
</div>
