.content { .auto; .mt(14px); .w(948px); 
	.twitter_l { .left; .w(708px); }
	.twitter_r { .right; .w(228px); }
	.twitter_box { .common_shadowBox; .left; .hidden; .w(606px); .pl(50px); .pr(50px); 
		.goods_info { .left; .pr(15px); .mt(25px); .w(305px);
			.tui_tle { .f(14px); .h(36px); .l(18px); .hidden; }
			.tui_sour { .gray; .break-word; .mt(8px); }
		}
		.show_tle { .h(62px); .l(62px); .f(16px); .left; .hidden; .mt(25px); .w(420px);
			a { .red; }
		}	
		.btn_box { .right; .mt(25px); .w(285px); 
			.tui_heart { background: url("@{pic_url}/images/share/@{tui}") no-repeat 0 -67px; .c; .left; .fb; .f(14px); .h(50px); .l(25px); .pt(6px); .pb(6px); .w(175px); .pr;
				.hackie { .h(50px); .hidden; }
				span, b { .block; }
				.t_collect, .t_like { .red; .cursor; .left; .h(50px); }
				.t_collect { .w(95px); }
				.t_like { .w(80px); }
				.lm_love2, .lm_love, .lm_income { background: url("@{pic_url}/images/share/@{tui}") no-repeat; }
				.lm_love2, .lm_love { .bp(-136px,-141px); padding: 0 6px 0 7px; }
				.lm_income { .bp(-162px,-142px); padding: 0 5px; }
				.love_pro { background: url("@{pic_url}/images/goods/love_pro.png") no-repeat; .red; .left; .c; .f(12px); .h(30px); .l(25px); .w(130px); .fn; 
					.pa; left: 0; top: -27px;	
				}
			}
			.price_go{ background: url("@{pic_url}/images/share/@{tui}") no-repeat 0 -133px; .right; .c; .h(46px); .l(22px); .f(14px); .fb; .white; .mt(1px); 
				.w(98px); padding: 7px 5px 7px 0;
				span { .block; }
			}
		}
		.codepic_box { .hidden; .w(606px); }
		.code_pic { .c; margin: 20px auto 0; max-width: 450px; .pr;
			.off_shelf, .show, .limit { .h(72px); .w(72px); .left; .pa; left: -20px; top: -20px; }
			.off_shelf { background: url("@{pic_url}/images/share/shelf.png") no-repeat; _filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='@{pic_url}/images/share/shelf.png', sizingMethod='scale'); _background: none; }
			.show { background: url("@{pic_url}/images/share/xiu.png") no-repeat; _filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='@{pic_url}/images/share/xiu.png', sizingMethod='scale'); _background: none; }
			.limit { background: url("@{pic_url}/images/share/limit2.png") no-repeat; _filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='@{pic_url}/images/share/limit2.png', sizingMethod='scale'); _background: none; }
			.view_pic { .cursor; .h(28px); .pa; right: 5px; top: 5px; filter:alpha(opacity=90); opacity:0.9; .w(93px); }

		}
		.detail { .l(18px); .pt(20px);
			.dtl_l { .left; .break-word; .w(456px); }
			.dtl_r { .right; .tr; .w(150px);
				a{ .pl(5px); .pr(5px); }
				.click_pl { .red; .cursor; }
			}
		}
		.comm_line { border-bottom: 1px solid #ddd; .h(8px); .mt(5px); .pr; 
			.corner { background: url("@{pic_url}/images/share/@{tui}") no-repeat -185px -143px; .block; .hidden; .h(8px); .w(15px); .pa; right: 25px; bottom: -1px; }
		}
		.comments {
			.list { 
				li { border-bottom: 1px dotted #ddd; .l(18px); .hidden; .pt(6px); .pb(6px); width: 100%;
					p { .ml(40px); 
						.date { color: #ccc; .right; }
						.reply { .red; .cursor; .right; .ml(10px); }
						.del { .red; .cursor; .right; }
						.facetableSetxy { .h(24px); .w(24px); }
					}
				}
				li.last { .border-n; }
			}		
		}
		.text_tui { .mt(20px);  
			.twitter_context { .border-1; .f(14px); .l(18px); .p10; background: #f1efef; }	
		}
		.tags { .border-tc; .h(46px); .l(46px); .mt(10px); color: #ccc; 
			a { .red; }
		}
	}
	.bar { .common_shadowBox; .hidden; .w(200px); .pr; padding: 0 13px 20px;  
		.user_box { .mt(18px); 
			.avatar48_f img { .border-r(5px); }
			h4, p { .ml(56px); }
			h4 { .h(24px); .l(24px); .hidden; .mb(3px);
				a { .red; .f(14px); }
			}
			.btn,.followed { .i_block; .h(20px); .l(20px); .w(64px); }
		}
		.mls_edit { .gray; .mt(20px); }
		.g_tle { .border-tc; .h(18px); .mt(18px); .pt(20px); 
			span { .gray; .f(16px); .left; }
			a{ .f(14px); .red; .left; .ellipsis; .w(112px); }
		}
		.imgBox{
			.mt(15px); .block; .w(200px); .auto; .h(204px); background: url("@{pic_url}/images/person/pg_bg1.png") no-repeat bottom;
			.minpic,span{.avatar64;.mr(4px);.mt(4px);}
			.maxpic{ .mt(4px); .h(200px); .w(200px); }
		}
		.red_follow, .pink_follow { background: url("@{pic_url}/images/share/@{tui}") no-repeat 0 0; .c; .red; .cursor; .i_block; .f(16px); .fb; .h(32px); .l(32px); .w(200px); }
		.red_follow:hover { .bp(0,-33px); .white; }
		.rec_ico { .common_shadowBox; .pf; right: -40px; top: 0; }
	}
	.shop_info { .shopinfo(); }
}
