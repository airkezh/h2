.element { .auto; .common_shadowBox; .mt(14px); .w(846px); .pl(50px); .pr(50px); 
	h2 { .yahei; .f(18px); .mt(20px); }
	.ent_list{  
		li { .border-bc; .h(210px); .pt(20px); .pb(20px); .hidden; 
			.pic { .left; .h(210px); .w(300px);
				img { .h(210px); .w(300px); }
			}
			.ent_info { .left; .pl(20px); .w(520px); 
				.tle { .yahei; .f(24px); .h(26px); .l(26px); .ellipsis; .mb(25px); }
				.txt { .f(14px); .h(36px); .l(18px); .hidden; .pl(30px); .pr(25px); .mt(22px); .mb(22px); .pr;
					span { color: #cbcbcb; .f(70px); .pa; }
					.mark_l { left: 0; top: 18px; }
					.mark_r { right: 0; top: 36px; }
				}
				.sm_pic { .h(64px); .hidden; .pr;
					a { .block; .h(64px); }
					img { .avatar64; .mr(4px); }
					span { background: @lightGrayBd; .c; .cursor; .f(14px); .fb; .h(40px); .l(20px); .pa; .w(30px); padding: 12px 5px; right: 0; top: 0; }
				}
			}
		}
		li.last { .border-n; }
	}
}

.lite { 
	&_info{.auto; .common_shadowBox; .mt(14px);.w(948px); .pt(14px);.pb(14px);}
	&_title{.fn;.f(30px);.l(32px);.pl(20px);.left;
		a{.red;}
	}
	&_prefix{.f(18px);.l(32px);.pl(20px);.left;
		a{color:#000;}
	}
	&_content{.pl(22px);.mt(8px);.l(18px);.w(340px);.left;}
	.sm_intro{.left;}
	.sm_pic { .h(64px); .hidden; .pr;.ml(380px);.pl(20px);
		border-left:1px solid #ccc;
		a { .i_block; .h(64px); }
		img { .avatar64; .mr(4px); }
		span { background: @lightGrayBd; .c; .cursor; .f(14px); .fb; .h(40px); .l(20px); .w(30px); padding: 12px 5px; .left;}
	}
}
