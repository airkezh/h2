<%#head.html%>

<div class="efficacy_wrap">
	<% this.efficacy.forEach(function(efficacy){ %>
		<a href="<%= efficacy.url %>">
			<div class="img_wrap" style="background-image: url(<%= efficacy.pic %>);"><div class="img_mask"></div></div>
			<p><%= efficacy.word_name %></p>
		</a>
	<% }); %>
</div>

<%#foot_simple.html%>
