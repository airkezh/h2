		<img src="<%= this.PICTURE_URL%>images/wap/weixin/anguide1.png" />
	<% } else {%>
		<img src="<%= this.PICTURE_URL%>images/wap/weixin/iosguide1.jpg" />
	<% } %>
<%}else{%>
