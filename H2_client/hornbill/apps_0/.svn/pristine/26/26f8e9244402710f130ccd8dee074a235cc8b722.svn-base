@import "../block";


html,body{
	.w(100%);
	.h(100%);
	.bgc(#fff);
	border-top:6px solid #f8f8f8;
	font-family: "Helvetica Neue";
}
.main{
	.h(100%);
}
.service{
	.h(100%);
	.w(90%);
	.auto;
	h3{
		.pb(6%);
		.pt(6%);
		border-bottom:1px solid #e6e5e6;
		color:#666666;
		font-weight:normal;
	}
	p{
		overflow:hidden;
		.l(22px);
	}
	img{
		.w(98%);
		.ml(1%);
		.mt(3%);
		.mb(2%);
	}
	.box{
		color:#93898f;
		.mt(3%);
	}

}



.content{
	.search{
		.mt(3%);		
		form{
			.pr;
			.Input{
				.auto;
				.w(80%);
				.h(30px);
				.l(27px);
				border:1px solid #f8f8f8;
				border-radius:24px;
				background-color:#f8f8f8;
				color:#666;
			}
		}
		input[type="search"]{
			.w(95%);
			border:0;
			outline:0;
			margin:0;
			padding:0;
			-webkit-appearance:none;
			background:0 none;
			.f(14px);
			.pl(30px);

		}
		.search-input{
			background:url('http://i.meilishuo.net/css/images/wap/web/icon3.png') no-repeat;
			background-size:120px 130px;
			background-position:-12px -51px;	
			.h(17px);
			.w(17px);
			.pa;
			top:7px;
			left:42px;
		}
	}	
}

