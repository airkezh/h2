@import "../block.less";
img{.w(100%);}
body{
	background-image: url("http://i.meilishuo.net/css/images/staticbed/bg2.png");
	background-repeat: no-repeat;
	background-size: 100%;
}
.result_wrap{
	.f(0.26rem);
	.c;
	.pics{
		.pr;
		.mt(5.32rem);
		bottom: -0.20rem;
		.peo-pic{
			.pa;
			.w(50%);
			bottom: 0rem;
			left:0;
			.user_head{
				.w(1.36rem);
				.h(1.36rem);
				.pr;
				z-index: 10;
				left: 1.17rem;
				top:-0.6rem;
				border-radius: 50% 50%;
				-webkit-animation:shake infinite linear .5s alternate;
				-webkit-transform-style:preserve-3d;
				-webkit-transform-origin:50% 80%;
			}
			.user_head_n{
				.w(1.14rem);
				.h(1.42rem);
				z-index: 10;
				.pr;
				left: 1.17rem;
				top:-0.6rem;
				-webkit-animation:shake infinite linear .5s alternate;
				-webkit-transform-style:preserve-3d;
				-webkit-transform-origin:50% 80%;

			}
			.user_body{
				.w(1rem);
				.h(.85rem);
			}
		}
		.run-pic{
			.pa;
			.w(50%);
			right:0rem;
			bottom: 0rem;
			.run_head{
				.animation(shake .5s linear 0s infinite alternate);
				-webkit-transform-style:preserve-3d;
				-webkit-transform-origin:50% 80%;
				.pr;

			}
			.head0{
				.w(2rem);
				.ml(-1.2rem);
