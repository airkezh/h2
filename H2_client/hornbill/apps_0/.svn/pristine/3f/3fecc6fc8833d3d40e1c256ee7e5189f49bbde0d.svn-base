@import "../../block.less";

body{
	.bgc(#fef7ed);
}

.start{
	.w(100%);
	img{
		.block();
		.w(100%);
	}
	.content{
		background: url('http://d05.res.meilishuo.net/pic/_o/6f/e0/d04b31a7486de6f4b90b386096a3_640_230.ch.jpg');
		background-size: 100%;
		.pt(6px);
		.pb(6px);
		.distance{
			line-height: 18px;
			text-align: center;
			.num{
				display: inline-block;
				.f(16px);
				.fc(#4a0b07);
			}
			.jindu{
				display: inline-block;
				.w(48%);
				.h(14px);
				.bgc(#ffdae3);
				border-radius: 10px;
				.ml(4px);
				.mr(4px);
				.hidden();
				.have-jindu{
					/*.w(30%);*/
					.h(100%);
					.bgc(#ff1616);
				}
			}
		}
		p{
			.f(16px);
			.fc(#4a0b07);
			text-align: center;
			.mt(2px);
			.num2{
				.fc(#da0303);
			}
		}
		.start-btn{
			.w(40%);
			.mt(6px);
			.ml(30%);
		}
		.zhu-btn{
			display: inline-block;
			.w(40%);
			.mt(6px);
		}
		.play-btn{
			display: inline-block;
			.w(40%);
			.mt(6px);
			.ml(10px);
		}
		.search-btn{
			display: inline-block;
			.w(40%);
			.mt(6px);
			.ml(10px);
		}
	}
	.friends-list{
		.w(100%);
		text-align: center;
		.friends-msg{
			display: inline-block;
			.w(83%);
			border-bottom: #fd8aa4 dashed 1px;
			.pt(10px);
			.pb(10px);
			.pl(6px);
			.pr(6px);
			.friend-img{
				.left();
				.w(16%);
				border-radius: 50%;
				.hidden();
			}
			span{
				.f(14px);
				.fc(#fc5283);
				.mt(14px);
				.ml(6px);
				white-space: nowrap;
			}
			.friend-name{
				float: left;
				.w(45px);
				text-overflow: ellipsis;
				white-space:nowrap;
				overflow: hidden;
			}
			.friend-desc{
				.left();
			}
			.friend-distance{
				.right();
			}
		}
	}
	.first-rules{
		.pa();
		top: 0;
		right: 0;
		.w(18%);
		.h(59px);
	}
	.first-popwin{
		.none();
		.pa();
		top: 0;
		left: 0;
		bottom: 0;
		right: 0;
		background-color: rgba(0,0,0,.6);
		.first-popwin-rules{
			.w(86%);
			.pa();
			top: 4%;
			left: 50%;
			transform:translateX(-50%);
			-ms-transform:translateX(-50%); 	/* IE 9 */
			-moz-transform:translateX(-50%); 	/* Firefox */
			-webkit-transform:translateX(-50%); /* Safari 和 Chrome */
			-o-transform:translateX(-50%); 	/* Opera */
		}
		.rules-content{
			.w(74%);
			.pa();
			top: 4%;
			left: 50%;
			transform:translateX(-50%);
			-ms-transform:translateX(-50%); 	/* IE 9 */
			-moz-transform:translateX(-50%); 	/* Firefox */
			-webkit-transform:translateX(-50%); /* Safari 和 Chrome */
			-o-transform:translateX(-50%); 	/* Opera */
			.f(15px);
			.fc(#503403);
			overflow: auto;
			.l(21px);
		}
		.first-popwin-know{
			.w(33%);
			.pa();
			top: 71%;
			left: 50%;
			transform:translateX(-50%);
			-ms-transform:translateX(-50%); 	/* IE 9 */
			-moz-transform:translateX(-50%); 	/* Firefox */
			-webkit-transform:translateX(-50%); /* Safari 和 Chrome */
			-o-transform:translateX(-50%); 	/* Opera */
		}
	}
	.share-popwin{
		.none();
		.pa();
		top: 0;
		left: 0;
		bottom: 0;
		right: 0;
		.pt(60px);
		background-color: rgba(0,0,0,.6);
		img{
			.w(63%);
			.pa();
			top: 60px;
			right: 10%;
		}
	}

	.flower{
		.w(14px);
		.h(14px);
		.pa();
		.none();
	}
	.bird{
		.pa();
		.none();
	}
	.bird1{
		.pa();
		.none();
	}
	.hudie{
		.pa();
		.none();
	}
}



/* 弹出框 */
.mypopbox{
	display:none;
	position: fixed;
	z-index: 999;
	top: 0px;
	left: 0px;
	width: 100%;
	height: 160%;
	overflow-x: hidden;

	.dialog {
		display:none;
		position: absolute;
		z-index: 100;
		top: 1.65rem;
		width: 5.7rem;
		height: 7.65rem;
		left: 50%;
		transform: translateX(-50%);
		-webkit-transform: translateX(-50%);

		.dialog_but{
			margin: 0 auto;
			display: block;
			width: 2.1rem;
			height: .74rem;
		}
		
	}
	.dialog_applogin{
		width: 4rem;
		height: 2.5rem;
		left: 50%;
		background: #fff;
		border-radius: 7px;
		
		.loginBut{
			display: block;
			margin:1rem auto 0 auto;
			width: 1.4rem;
			height: .5rem;
			line-height: .5rem;
			color: #fff;
			font-size: .3rem;
			text-align: center;
			background:#FF5079;
		}

	}

	.dialog_appshare{
		width: 4.5rem;
		height: 2.91rem;
		top:3rem;
		background: #e2738d;
		border-radius: .2rem;
		text-align: center;
		color: #fff;
		.dialog_appshare_tit{
			height: 1rem;
			line-height: 1rem;
			text-align: center;
			font-size: .33rem;
		}
		.dialog_appshare_box{
			float: left;
			width: 50%;
			overflow: hidden;
			p{
				line-height: .6rem;
				font-size: .21rem;
			}
		}
		.shareBut{
			display: inline-block;
			margin: 0   .6rem ;
			width: .95rem;
			height: .95rem;
		}
		.shareBut_wx{
			background:url('http://d04.res.meilishuo.net/pic/_o/85/7a/aab5e12571cce8c326038468beb5_95_95.cg.png');
			background-size:100% 100%;
		}
		.shareBut_pyq{
			background:url('http://d03.res.meilishuo.net/pic/_o/ed/1e/c4b27c9150dfd8cf374366ffd883_95_95.ch.png');
			background-size:100% 100%;
		}

		.shareBut_close{
			position: absolute;
			top: -0.18rem;
			right: -0.22rem;
			width: .62rem;
			height: .62rem;
			background:url('http://d02.res.meilishuo.net/pic/_o/91/e6/78f00dfccc1646ad3bf0067bb997_62_62.cg.png');
			background-size:100% 100%;
		}
	}
	
	.dialog_wxshareguide{
		width: 100%;
		top: 0;
		img{
			width: 100%;
		}
	}
	
	.overlay{
		display:;
		position: absolute;
		z-index: 90;
		top: 0px;
		left: 0px;
		bottom: 0px;
		right: 0px;
		background-color: rgba(0, 0, 0, 0.6);
	}

}