/*h5播放器*/
.music{ .w(.68rem); .h(.68rem); .pa; right: .25rem; top: .2rem; z-index:100; -webkit-animation:2s infinite linear; }
.musicon{ background:url("http://i.meilishuo.net/css/images/cooper/music/musicon.png") no-repeat;
		-webkit-animation-name:pulse;
		animation-name:pulse
}
.musicoff{ background:url("http://i.meilishuo.net/css/images/cooper/music/musicoff.png") no-repeat; -webkit-transform:scale3d(.8, .8, .8);transform:scale3d(.8, .8, .8);  }
.musicon,.musicoff{ background-size:contain; }
@-webkit-keyframes pulse{
	0%{-webkit-transform:scale3d(.8, .8, .8);transform:scale3d(.8, .8, .8);} 
	50%{-webkit-transform:scale3d(.9, .9, .9);transform:scale3d(.9, .9, .9);}
	100%{-webkit-transform:scale3d(.8, .8, .8);transform:scale3d(.8, .8, .8);}
}

