@import "../block.less";
body{
	.bgc(#FFFFFF);
}
body::-webkit-scrollbar{
        display:none;
    }
img{
	.block;
}
//轮播样式
.sliderBanner;
.banner-slider-point {
	font-size: 0.24rem;
	.pa; .w(100%); left:0; .mb(0.2rem);
	li { float: none; .i_block; .bgwhite; .vm; .w(5px); .h(5px); .mr(0.05rem);}
	.curr { background-color:transparent; border:1px solid #fff;}
}
.banner-slider { .pr; .pb(8px);}

//ppt轮播样式
.swipe {
	overflow: hidden;
	visibility: hidden;
	position: relative;
}
.swipe-wrap {
	overflow: hidden;
	position: relative;
}
.swipe-wrap > div {
	float:left;
	width:100%;
	position: relative;
}
.wrap{
	.bannerwrap{
		.banner{
			.w(100%);
		}
	}
	.qualityWrap{
		overflow: hidden;
		.pt(0.2rem);
		.w(5.65rem);
		margin: 0 auto;
		.saler{
			.left;
			.f(0.26rem);
			.fc(#333333);
			.l(0.3rem);
			.icon{
				.block;
				.left;
				.mr(0.1rem);
				.w(0.3rem);
				.h(0.3rem);
				background-repeat: no-repeat;
				background-size: 100%;
			}
		}
		.saler:nth-child(n+2){
			.pl(0.64rem);
		}
	}
	.contentWrap{
		.segmentation{
			clear: both;
			.mt(0.25rem);
			.pt(0.2rem);
			.bgc(#EFEFEF);
		}
		.preferablyText{
			.mt(0.25rem);
			.mb(0.05rem);
			.c;
			font-weight: bold;
			.f(0.3rem);
			.fc(#333333);
		}
		.segLine{
			.w(100%);
		}
		.newInfo{
			.mt(0.05rem);
			.f(0.24rem);
			.fc(#666666);
			.c;
		}
		.swipe{
			clear: both;
			.mt(0.25rem);
			.pr(0.2rem);
			.pl(0.2rem);
			.swipe-wrap{
				.preferablyInfoWrap{
					overflow: hidden;
					.goodsImgWrap{
						img{
							.w(6rem);
							.h(5rem);
						}
					}
					.topGoodWrap{
						overflow: hidden;
						.mt(0.2rem);
						.topGood{
							.left;
							.mt(0.08rem);
							// .pt(0.5rem);
							// background : url('http://d01.res.meilishuo.net/pic/_o/3e/dd/f399e9cc91d3e17e9c141df500dc_51_61.ch.png');
							// background-repeat: no-repeat;
							// background-size: 100%;
							.noImage{
								.w(0.5rem);
								.h(0.6rem);
								background : url('http://d02.res.meilishuo.net/pic/_o/41/be/ef052c054eb83ca963bcafdf434d_60_70.cf.png');
								background-repeat: no-repeat;
								background-size: 100%;
							}
							.num{
								.mt(0.15rem);
								text-align: justify;
								.f(0.24rem);
								.fc(#D52B22);
							}
							.numLine{
								.mt(0.15rem);
								.w(0.5rem);
								box-sizing: border-box;
								border-bottom: 3px solid #D52B22;
							}
						}
						.preferablyInfo{
							.left;
							.ml(0.3rem);
							.w(5.1rem);
							.designerDesc{
								.f(0.26rem);
								.l(0.4rem);
								.salerRead{
									font-weight: bold;
									.fc(#333333);
								}
								.salerInfo{
									.fc(#333333);
								}
							}
							.shopInfo{
								.mt(0.32rem);
								.f(0.24rem);
								.shopFrom{
									.fc(#999999);
								}
								.shopName{
									.fc(#EDC400);
								}
							}
						}
					}
				}
				.preferablyInfoWrap:nth-child(2) {
					.topGood{
						.noImage{
							background : url('http://d05.res.meilishuo.net/pic/_o/2e/8a/bf73a2af1ebed7fcee22507b4afb_60_70.cf.png');
							background-repeat: no-repeat;
							background-size: 100%;
						}
						.num{
							.fc(#FFC100);
						}
						.numLine{
							border-bottom: 3px solid #FFC100;
						}
					}
				}
				.preferablyInfoWrap:nth-child(3) {
					.topGood{
						.noImage{
							background : url('http://d04.res.meilishuo.net/pic/_o/f2/24/24cdecb65afde74530b337476803_60_70.cg.png');
							background-repeat: no-repeat;
							background-size: 100%;
						}
						.num{
							.fc(#16C9CA);
						}
						.numLine{
							border-bottom: 3px solid #16C9CA;
						}
					}
				}
				.preferablyInfoWrap:nth-child(n+4) {
					.topGood{
						.noImage{
							background : url('http://d02.res.meilishuo.net/pic/_o/de/a2/6ed24fff93de12a590c2d3136c12_60_70.cg.png');
							background-repeat: no-repeat;
							background-size: 100%;
						}
						.num{
							.fc(#000000);
						}
						.numLine{
							border-bottom: 3px solid #000000;
						}
					}
				}
			}
				.spotWrap{
					overflow: hidden;
					.spot{
						.left;
						.ml(10px);
						.mr(10px);
						.mt(0.3rem);
						.w(8px);
						.h(8px);
						.bgc(#D8D8D8);
						-webkit-border-radius : 50%;
						border-radius: 50%;
					}
					.on{
						.bgc(#FBCD00);
					}
				}
		}
		.designerShop{
			.w(100%);
			.classification{
				padding: 0.3rem 0rem 0.15rem;
				position: -webkit-sticky;
				position: -moz-sticky;
				position: -o-sticky;
				position: -ms-sticky;
				position: sticky;
				top:0;
				left:0;
				z-index: 10;

				.bgc(#FFFFFF);
				.w(100%);
				border-bottom: 1px solid #F1F1F1;
				.selectWrap{
					overflow: hidden;
					.w(5.6rem);
					margin: 0 auto;
					span{
						.left;
						padding:0rem 0 0.14rem 0rem;
						.f(0.28rem);
						.fc(#666666);
					}
					span:nth-child(n+2){
						.ml(0.69rem);
					}
					.active1{
						.fc(#000000);
						border-bottom: 2px solid #FBCD00;
					}
				}
			}
			.designerGoods{
				.w(100%);
				min-height: 500px;
			}
			.shopWrap{
				.pt(0.3rem);
				.shopInfo{
					.desigerWrap{
						.designPortrait{
							margin: 0 auto;
							.w(1.2rem);
							.h(1.2rem);
							border-radius: 0.6rem;
						}
						.designerInfo{
							// .ml(0.2rem);
							.h(0.75rem);
							.c;
							.designerNameWrap{
								.mt(0.27rem);
								.f(0.24rem);
								.fc(#333333);
								.designerName{
									.fc(#FBCD00);
								}
							}
							.designerDesc{
								padding: 0.15rem 0rem 0;
								.f(0.24rem);
								.fc(#333333);
								.l(0.3rem);
							}
						}
					}
					.line{
						.w(100%);
						margin: 0.2rem 0;
					}
					.goodsWrap{
						overflow: hidden;
						.w(6rem);
						margin: 0 auto;
						.pt(0.15rem);
						.goodsLink{
							.block;
							.left;
							.pr;
							overflow: hidden;
							.goods{
								.w(2.97rem);
								.h(3.9rem);
							}
							.goodsInfo{
								.pa;
								bottom: 0;
								padding: 0.15rem 0;
								.w(100%);
								.f(0.24rem);
								.fc(#666666);
								background-color: RGBA(255,255,255,0.88);
								.goodsName{
									.left;
									padding-left: 0.1rem;
								}
								.goodsPrice{
									.right;
									padding-right: 0.1rem;
									.fc(#333333);
								}
							}
						}
						.goodsLink:nth-child(2n){
							.ml(0.05rem);
						}
						.goodsLink:nth-child(n+3){
							.mt(0.05rem);
						}
					}
						.visitShop{
							overflow: hidden;
							margin: 0.25rem auto 0;
							padding: 0 0.25rem;
							.w(1.32rem);
							.h(0.6rem);
							.l(0.6rem);
							.c;
							.bgc(#FBCD00);
							span{
								.block;
								.left;
								.f(0.28rem);
								.fc(#FFFFFF);
							}
							.visitBig{
								.left;
								.w(0.12rem);
								margin: 0.18rem 0;
							}
						}
				}
			}
		}
	}
}


/*回顶部*/
.gotop{ .pf; right:20px; .w(12.5%); .none;  }
.img100 { .w(100%); .block; }