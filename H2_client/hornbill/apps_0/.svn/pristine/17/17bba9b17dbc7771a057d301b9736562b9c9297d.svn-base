@import "../block.less";
@import "../doota/shareact_bs_goods.less";

.main{.bgc(#fff);.mb(-14px);}

.head_bg{background-position: center;background-repeat: no-repeat;}
.top_bannar, .seckill{.w(946px);.auto;}

.bs_tab_wrap{.w(960px);.hidden;}
.bs_tab{
	.pt(10px);
	a,
	div{
		.mr(34px);.w(286px);.left;
		.c;.fc(#fff);.f(14px);.l(42px);
		.bgc(#feba95);.cursor;-webkit-transition: background-color .3s ease-out;
		&:hover{
			.bgc(#ee678c);
		}
	}
	.tab_active{.bgc(#ee678c);}
}

.seckill{
	.w(926px);.h(335px);.hidden;.pr;
	.tab_prev{.pa;top: 50%;left: 0;.mt(-22px);z-index: 10;.w(34px);.h(44px);background: url("@{pic_url}/images/welcome/bnr_btn_left1.png") no-repeat left center;}
	.tab_next{.pa;top: 50%;right: 0;.mt(-22px);z-index: 10;.w(34px);.h(44px);background: url("@{pic_url}/images/welcome/bnr_btn_right1.png") no-repeat left center;}
	.seckill_inner{.h(335px);.w(2878px);-webkit-transition: margin-left .6s ease-in-out;-moz-transition: margin-left .6s ease-in-out;transition: margin-left .6s ease-in-out;}
	.timeStamp_wrap{.pa;bottom: 60px;left: 550px;.f(16px);.fb;.red;padding: 0;}
	.seckill_goods{.w(926px);.hidden;.left;.pr;}
	.soldout{
		.w(100%);.h(100%);background:url("http://d06.res.meilishuo.net/img/_o/00/b2/6ca14135dacbd6b8cba4e380ba62_205_205.cc.png") no-repeat center;
		_background:none;
		_filter:~"progid:DXImageTransform.Microsoft.AlphaImageLoader(src='http://d06.res.meilishuo.net/img/_o/00/b2/6ca14135dacbd6b8cba4e380ba62_205_205.cc.png',sizingMethod='scale')";
		.pa;top: 0;left: 0;
	}
	.gtitle{.f(12px);.pb(10px);.pt(10px);}
	p{padding: 0 14px;.fc(#666);}
	i{font-style: normal;}
	.now_price{.f(14px);.fb;.mr(14px);}
	.num{.f(14px);.right;}
}

.poster{
	.hidden;.w(970px);.auto;.pb(20px);
	.bs_tab{.ml(22px);.w(960px);}
	.fx_hidden{.none;.h(52px);.w(100%);}
	.fx_tab{
		.pf;top: 30px;left: 0;.bgc(rgba(0,0,0,.2));z-index: 100;.w(100%);
		.bs_tab{.pr;left: 50%;.ml(-463px);}
	}
}
.goods_unit();
