<%#head_simple.html%>
</head>
<body>
	<div class="w_bg">
		<div class="w_title">
			<img src="<%= this.PICTURE_URL%>images/wap/activity/act91/w_title.png" />
		</div>
		<div class="w_1">
			<img src="<%= this.PICTURE_URL%>images/wap/activity/act91/w_t_1.png" />
		</div>
		<ul class="w_list">
			<% this.winners['一等奖'].map(function(v, k){ %>
				<li><span><%= v['姓名'] %></span><%= v['电话'] %></li>
			<%});%>
			<div class="clear"></div>
		</ul>
		<div class="w_2">
			<img src="<%= this.PICTURE_URL%>images/wap/activity/act91/w_t_2.png" />
		</div>
		<ul class="w_list">
			<% this.winners['二等奖'].map(function(v, k){ %>
				<li><span><%= v['姓名'] %></span><%= v['电话'] %></li>
			<%});%>
			<div class="clear"></div>
		</ul>
		<div class="w_3">
			<img src="<%= this.PICTURE_URL%>images/wap/activity/act91/w_t_3.png" />
		</div>
		<ul class="w_list">
			<% this.winners['三等奖'].map(function(v, k){ %>
				<li><span><%= v['姓名'] %></span><%= v['电话'] %></li>
			<%});%>
			<div class="clear"></div>
		</ul>
	</div>
	<img src="<%= this.PICTURE_URL%>images/wap/activity/act91/bg_btm.jpg" />
</body>
<%#foot_simple.html%>
