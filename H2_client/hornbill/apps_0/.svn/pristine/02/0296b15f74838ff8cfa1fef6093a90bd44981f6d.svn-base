@import '../../block.less';

body {
	background-image:-webkit-linear-gradient(-45deg,#97c2be 25%,#b2ddd8 25%,#b2ddd8 50%,#97c2be 50%,#97c2be 75%,#b2ddd8 75%);
    background-image:linear-gradient(-45deg,#97c2be 25%,#b2ddd8 25%,#b2ddd8 50%,#97c2be 50%,#97c2be 75%,#b2ddd8 75%);
    background-size:6px 6px;
    background-color: #97c2be;
}

* {
	padding: 0;
	margin: 0;
}

.resutl_wrap {
 	position: absolute;
	width: 560/640*100%;
   	margin: .54rem .42rem;
	background: #fff;
	border-radius: .1rem;
	text-align: center;
}

.login_btn, .start_btn {
	width: 320*@w;
	height: .64rem;
	margin: .24rem auto 0;
	color: #fff;
	font-size: .28rem;
	text-align: center;
	line-height: .64rem;
	background: #ff6666;
	border-radius: .3rem;
	box-shadow:  0rem .08rem .02rem #cc3535;
	-webkit-box-shadow:  0rem .08rem .02rem #cc3535;
}

.logo {
	width: 1.30rem;
	height: .44rem;
	margin: .3rem auto 0;
	display: block;
}
.login_wrap{ 
	padding: .4rem 0 .48rem 0;
	.style_title {
		font-size: .4rem;
		color: #000;
		font-weight: bolder;
	}
	.title {
		margin-top: .3rem;
		color : #000;
		font-size: .3rem;
	}
	.content {
		margin-top: .3rem;
		color: #666;
		font-size: .22rem;
		text-align: justify;
		padding: 0 .5rem;
		line-height: 1.3; 
	}
	.fit,.dress {
		padding: 0 .5rem;
		font-size: .22rem;
		color: #000;
		text-align: left;
		line-height: 1.4;
	}
	.fit {
		margin-top: .1rem;
	}
}

.style_img {
	position: relative;
	width: 2.8rem;
	height: 2.8rem;
	margin: .36rem auto 0;
	background-size: 100% 100%;
	img {
		position: absolute;
		bottom: -0.42rem;
		right: -0.6rem;
		width: 1.3rem;
		height: 1.3rem;
	}
}

.unlogin_wrap {
	color: #000;
	padding: .48rem 0 .48rem 0;
	.dear {
		font-size: .52rem;
	}
	.note {
		font-size: .4rem;
		margin-top: .2rem;
	}
	.note2 {
		font-size: .3rem;
		margin-top: .46rem;
		line-height: 1.2;
	}
	.rec {
		font-size: .24rem;
		color: #666;
		margin-top: .9rem;
	}
}


.share_title {
	position: relative;
    z-index: 0;
    margin-top: .2rem;
    .line {
    	height: .01rem;
	    position: absolute;
	    top: 50%;
	    left: 0;
	    width: 450/540*100%;
	    margin-top: -0.01rem;
	    margin-left: .46rem;
	    z-index: -1;
	    background: rgba(0,0,0,.2);
    }
    .show_up {
    	background: #fff;
    	width: 26%;
	    height: .62rem;
	    z-index: 10;
	    font-size: .26rem;
	    color: #000;
	    line-height: .62rem;
	    text-align: center;
	    margin: auto;
	    z-index: 0;
	    font-weight: bolder;
    }
}


.login_wrap .share_box {
  text-align : center;
  font-size  : 0;
  a {
    display : inline-block;
    width   : 200/540*100%;
    height  : .68rem;
    background      : #ff6666;
    border-radius: .34rem;
    color: #fff;
    font-size: .28rem;
    line-height: .68rem;
     box-shadow:  0rem .08rem .02rem #cc3535;
     -webkit-box-shadow:  0rem .08rem .02rem #cc3535;
    span {
    	vertical-align: middle;
    }
    .icon {
    	display: inline-block;
    	width: .4rem;
    	height: .4rem;
    	margin-left: .12rem;
    }
  }
  .share_pengyou {
  	margin-right: .5rem;
    .pengyou {
    	background: url('http://d03.res.meilishuo.net/pic/_o/90/81/d27f71177a4b59e109845dc59e5b_46_40.ch.png') no-repeat center;
    	background-size: 100%;
    }
  }
  .share_quan {
   	 .quan {
   	 	background: url('http://d04.res.meilishuo.net/pic/_o/29/2a/e7b3a7c9dc3041fe22d051dd5f20_40_40.ch.png') no-repeat center;
   	 	background-size: 100%;
   	 }
  }
}


.share_mask {
  position   : fixed;
  top        : 0;
  left       : 0;
  width      : 100%;
  height     : 100%;
  background : rgba(0, 0, 0, .8);
  .arrow {
    width    : .88rem;
    position : absolute;
    right    : .26rem;
  }
  .desc {
    padding-top : 2.7rem;
    text-align  : center;
    font-size   : .32rem;
    color       : #fff;
    line-height : .4rem;
  }
  .icons {
    margin-top : .2rem;
    font-size  : 0;
    text-align : center;
    .pengyou {
      width        : .89rem;
      display      : inline-block;
      margin-right : 1.82rem;
    }
    .quan {
      width   : .7rem;
      display : inline-block;
    }
  }
}


.wx_share,.wx_download {
	width: 444/540*100%;
	height: .64rem;
	margin: .24rem auto 0;
	color: #fff;
	font-size: .28rem;
	text-align: center;
	line-height: .64rem;
	background: #ff6666;
	border-radius: .3rem;
	box-shadow:  0rem .08rem .02rem #cc3535;
	-webkit-box-shadow:  0rem .08rem .02rem #cc3535;
}

