<script type="text/html" id="bs_circle">
	<? this.bs_circle.forEach(function(circle, index){ ?>
		<a class="circle_item <?= index%2 ? 'right' : 'left' ?>" href="<?= circle.link ?>" data-id="<?= circle.id ?>">
			<div class="circle_icon" style="background-image: url(<?= circle.img ?>);">
				<div class="circle_mask"></div>
				<img class="avatar" src="<?= circle.admin.avatar ?>">
				</div>
			<div class="circle_name"><?= circle.title ?></div>
			<div class="circle_addr"><span></span><?= circle.address ?></div>
			<div class="circle_desc"><?= circle.description ?></div>
		</a>
	<? }); ?>
</script>