@import "../../block.less";
@import "../tuanbase.less";
@import "../imgSlide.import.less";

html,body{
	.bgc(#fff);
	.w(100%);
	.f(10px);
}
.main{
	.pr;
	.hidden;
	min-width: 320px;
	margin: 0 auto;
	max-width: 640px;
}
.slideWraper{
	.pr;
}
.slide;
img{
	.w(100%);
	.block;
}
ul{list-style:none;}
i{font-style:normal;}
.line{
	.pa; 
	top: 30px;
}
.f_14{.f(1.4rem);}
.f_15{.f(1.5rem);}
.f_16{.f(1.6rem);}
.f_20{ .f(2rem);}
.f_30{.f(3rem);}
.f_23{.f(2.3rem);}
.f_25{.f(2.5rem);}
.red_f1{.fc(#f66);}
.bgray_f{ .bgray;}
.top-bnr{.pr;.hidden;}
