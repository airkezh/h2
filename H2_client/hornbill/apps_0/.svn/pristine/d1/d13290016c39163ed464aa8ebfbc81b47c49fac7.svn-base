.label-arrow-top {
  &::before {
    position            : absolute;
    top                 : -14px;
    left                : 43%;
    content             : " ";
    border              : 7px solid transparent;
    border-bottom-color : #e6e6e6;
  }

  &::after {
    position            : absolute;
    left                : 43%;
    top                 : -13px;
    content             : " ";
    border              : 7px solid transparent;
    border-bottom-color : #fff;
  }
}

