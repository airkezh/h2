<%# head.html %>
<div class="before" >
	<div class="bag">
		<img class="moneybag" src="http://d05.res.meilishuo.net/pic/_o/98/6d/ec6dd0b61ecfeae69a5a30c4df2b_640_928.ch.jpg" alt="">
		<div class="msg">
			<input class="phonenum require" placeholder="请输入你的手机号" maxlength="11">
			<div class="cell">
				<input class="code require" placeholder="请输入短信验证码">
				<div class="send_btn normal" data-clicked=false>发送验证</div>
			</div>
		</div>
	</div>
	<div class="apply_btn btn">
		立即领取
	</div>
	<img class="advertising" src="http://d02.res.meilishuo.net/pic/_o/36/cd/a7f840a1a1720ddc5baa36cf3a7a_600_200.ch.jpg" alt="99元变女神广告">
</div>

<div class="after" style="display:none">
	<div class="result_note">
		<img class="success rpic" src="http://d02.res.meilishuo.net/pic/_o/43/dc/d1e027e0792bef991bbacf29bbeb_409_55.cf.png" alt="领取成功" style="display:none">
		<img class="applied rpic" src="http://d02.res.meilishuo.net/pic/_o/25/91/43c54c86aa406b7ef63b790ca313_458_55.cf.png" alt="已经领取过" style="display:none">
		<img class="empty rpic" src="http://d01.res.meilishuo.net/pic/_o/64/5b/b5f2a097cf5b84c0fdc9beb2a351_565_49.cg.png" alt="今日领完" style="display:none">
		<img class="end rpic" src="http://d01.res.meilishuo.net/pic/_o/34/04/1d3d6c4056cd90faf0110a1a62bb_414_55.ch.png" alt="" style="display:none">
	</div>
	<div class="result_pic">
		<img class="richbag" src="http://d02.res.meilishuo.net/pic/_o/1a/61/9ec5910adfb3f023305b0ee51ecd_640_673.ch.jpg" alt="领取成功图" style="display:none">
		<img class="nonebag" src="http://d03.res.meilishuo.net/pic/_o/6e/1c/c0b9e3f300e69daafc8a07414bb6_640_673.cg.jpg" alt="领取失败图" style="display:none">
		<img class="download_btn download" src="http://d05.res.meilishuo.net/pic/_o/8a/3d/8dc90212a2d61f45453e77a44e20_460_88.cg.png" alt="下载按钮">
	</div>
	<img class="paper" src="http://d05.res.meilishuo.net/pic/_o/1d/1d/5c185b8cec5f9ab3557aeeae3b7e_512_426.ch.png" alt="优惠券种类">
	<img class="note" src="http://d04.res.meilishuo.net/pic/_o/82/eb/2e8e330d4e39d701a17d7dd8226d_512_635.ch.jpg" alt="公告">
</div>

<div class="footer">
	<img class="download_link download" src="http://d01.res.meilishuo.net/pic/_o/c6/2b/8b2df3269ebfe565b3640819e59f_288_24.ch.png" alt="下载美丽说客户端">
	<img class="logo" src="http://d03.res.meilishuo.net/pic/_o/2f/42/8008a6f0968aa2cd91552d849cba_324_54.ch.png" alt="">
</div>

<script>
	fml.vars.cid = '<%= this.cid %>';
</script>
