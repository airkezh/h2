@import "../../block.less";
@import "../imgSlide.import.less";
html{
	.f(62.5%);
}
body{
	.bgc(#fff);
	max-width: 640px;
	margin: 0 auto;
}
img{
	.w(100%);
	.block;
}
.sliderBanner;
.nav_wap{.w(100%); min-height: 3.7rem; .fc(#fff);}
.nav{
	.w(100%);max-width:640px;
	.nav_tabs {
		.border-2t(#e5e5e5);
		.bgc(#f5f5f5);
		.f(1.2rem);
		.hidden;
		li{
			a{
				.fc(#929292);
			}
			span{
				padding: .2rem .5rem;
			}
			.mt(1.3rem);
			.mb(1.3rem);
			.left;
			.c
		}
		.curr{
			span{
				.fc(#fff);
				.bgc(#666);
				border-radius: .3rem;
			}
		}
	}
	.lisix li{
		.w(16.6%);
	}
	.lifive li{
		.w(20%);
	}
	.lifour li{
		.w(25%);
	}	
}
.shadow{
	box-shadow:7px 2px 7px #e6e6e6;
	/*兼容IE6、 7、8*/		
	filter: progid:DXImageTransform.Microsoft.Shadow(Strength=7, Direction=180, Color='#e6e6e6'); 
	/*for ie6,7,8*/
}
.scrollin{
	.pf;top:-1px;left: 0; right:0; margin: 0 auto;.w(100%);z-index:100;
}
.scrollout{
	.pr;
}
.item{
	h1,.moduleimg{
		.mt(1rem);
		.mb(1rem);
		.f(1.2rem);
		font-weight: normal;
		span{
			border-left: solid 3px red;
			.pl(.5rem);
			.ml(.5rem);
		}
	}
}
.pullUp{
	.pullUpDown > .common;.pullUpDown > .loading;
	&:before{
		.nowrap;
		.ml(-10px);
	}
	&[status="tap"]{
		background:0 none;	
	}
	&[status="stop"]{
		background:0 none;	
	}
}

@import "../tuan_tmp.import.less";