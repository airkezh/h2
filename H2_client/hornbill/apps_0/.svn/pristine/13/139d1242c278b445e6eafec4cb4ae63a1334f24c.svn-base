/**
* 引入atom
**/
@import "../block.less";
/**
* css reset
**/
html, body, div, h1, h2, h3, h4, h5, h6, ul, ol, dl, li, dt, dd, p, blockquote, pre, form, fieldset, table, th, td, map{margin: 0; padding: 0;}
* html,* html body{background-image: url(about:blank);background-attachment: fixed;}
ul, ol, dl, li{ list-style: none;}
h3{_zoom: 1;}
textarea{resize: none;}
fieldset, img{border: 0;}
legend { .none; }
a{color: #666; .tnone;
	&:hover { .red; }
}
a:focus{outline: none;}
em{font-style: normal;}
html{_overflow-x: hidden;}
body{background: #fff; color: #666; .f(12px); font-family: Arial;}
.none_f{.none;}
.left_f{.left;}
.red_f{	.red;
	a{ .red; .tnone;
		&:hover {.red; text-decoration: underline; }
	}
}
.gray_f{ .gray;	.tnone;
	a{ .gray; .tnone;
		&:hover {.red;}
	}	
}
.ml10_f{.ml(10px);}

.logo_new{.logo;.logo>.simple;.auto;}

.small_qq, .small_sina {
	background: url("@{pic_url}/images/window/login_icon2.png") no-repeat; .cursor; .i_block; 
	a { .block; .h(50px); }	
}
.small_qq, .small_sina { .h(41px); .w(173px); }
.small_qq { background-position: 0 -111px; }
.small_sina { background-position: 0 -156px; }

.closeBtn{.right;.red;.cursor;background: url("@{pic_url}/images/home/like_merge1.png") repeat-x 0px -5px;border:#FCBCD2 solid 1px;.l(18px);.c;.w(65px);.mt(24px);}

.l_ipt {.border-1;.left;.gray;.h(20px);.l(20px);.p05;.w(170px);}
.l_ico { .c; .pt(14px);.border-tc;.mt(10px);}
.win_login{
	margin:10px;
	.l_ico { .c; }
	fieldset { 
		.mt(14px);
		p{.hidden;.w(254px);.auto;.mt(14px);.h(22px);.l(22px);}
		div{ .w(254px); .auto; .mt(10px); .l(28px);
			.button{ .common_button; .common_button > .pinkBtn; .border-n; .f(14px); .fb; .h(28px); .mr(10px); .p010; .ml(0px); }
		}
	}		
}
.win_success, .win_boyfail{
	margin:20px 10px 10px 10px;
	h4{.auto;.w(198px);.red;.f(16px);.l(64px);.h(65px);.pl(74px);}
	p,ul{.auto;.w(290px);.f(14px);}
	p{.mt(6px);}
}
.win_success{
	h4 {background: url("@{pic_url}/images/window/leben.gif") no-repeat;}
	p{.l(26px);}
	ul{.l(22px);}
}
.win_boyfail{
	h4 {background: url("@{pic_url}/images/window/yihan1.png") no-repeat 0px 4px;}
	p{.l(22px);}
}
