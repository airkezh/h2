@import "../block.less";
@import "item_foot.less";
@icon: 'http://d01.res.meilishuo.net/pic/_o/a9/4c/b58067cfb7e81101f1665e85d946_1280_1200.cg.png';
.ellipsis-row(@var){ .hidden;text-overflow:ellipsis;display:-webkit-box;-webkit-line-clamp:@var;-webkit-box-orient:vertical; }

.icon(@x:0, @y:0){
	background: url(@icon) no-repeat;background-size: 6.4rem;background-position: @x @y;
}

body{ .w(6.4rem);.f(.24rem);.fc(#999);.bgc(#f4f4f4); }

.page_head{
	.h(.88rem);.w(100%);.hidden;.c;border-bottom: .05rem solid #F69;.icon(0, -4%);.bgc(#fff);

	.back_icon{ .pr;z-index: 5;.left;.w(2em);padding: 0 .3rem;.h(.88rem); }
	.right_btn{ .pr;z-index: 5;.right;padding: 0 .3rem;.h(.88rem);.indent; }
	.title{ .w(6.4rem);.f(.32rem);.h(.88rem);.pa;left: 0;z-index: 1;.indent; }
	.cart_nums{ .pa;.w(.34rem);.h(.32rem);.l(.32rem);.c;top: .06rem;right: .1rem;text-indent: 0;.f(.16rem);.fc(@red);.icon(43%, 56.7%);.none; }
}

.seo_wrods{
	.h(0);.l(.32rem);.f(.22rem);.fc(#FF7A9A);margin: .14rem 0;.hidden;.none;

	a{ .fc(#FF7A9A);padding: .02rem .14rem;border: 1px solid #FF7A9A;.border-r(.9rem);.mr(.1rem); }
	a:last-child{ .mr(0); }
	.title{ .icon(0, 41.4%);.pl(.36rem);.ml(.2rem);.left; }
	.words_wrap{ .w(4.34rem);.h(.32rem);.hidden;overflow-x: auto;white-space: nowrap; }
}
.seo_wrods_bottom{
	margin: .3rem 0;.h(.32rem);.block;

	.words_wrap{ .w(5.36rem); }
}

.picture_wrap{
	.hidden;.pr;.bgc(#fff);.h(6.1rem);.w(100%);.mt(.1rem);

	.slider_wrap{ .h(6.1rem);.w(4.9rem);.auto; }
	.image_item{ .left;.pr;.w(4.9rem);.h(6.1rem);.mr(.3rem);background: no-repeat center;background-size: cover;opacity: .5; }
	.image_item:first-child{ opacity: 1; }
	.swipe{ .pr; }
	.swipe_wrap{ .pr;-webkit-backface-visibility:hidden;backface-visibility:hidden;z-index:1; }

	.left_btn_wrap,
	.right_btn_wrap{ .pa;top: 0;left: 0;.w(.75rem);.h(100%);.l(6.1rem);.c; }
	.left_btn,
	.right_btn{ .i_block;.w(.42rem);.h(.74rem);.icon(0%, 19%);opacity: .3; }
	.right_btn_wrap{
		left: auto;right: 0;

		.right_btn{ background-position: right 19%; }
	}

	.goods_desc_wrap{ .pa;bottom: 0;left: 0;.w(6.2rem);padding: .1rem;.bgc(rgba(0,0,0,.34)); }
	.goods_desc{ .ellipsis-row(2);.fc(#fff);.l(.28rem);font-weight: normal; }
}

.sku_meta{
	.bgc(#fff);padding: .2rem .3rem;.pr;

	.price{ .f(.38rem);.fc(#FF7A9A);.l(.42rem); }
	.original_price{ margin: 0 .1rem;.f(.22rem);.fc(#999);text-decoration: line-through; }
	.discount_price{ .pr;top: -.2rem;padding: .01rem .1rem;.bgc(#FF7A9A);.f(.16rem);.fc(#FFF);.l(.16rem); }
	.goods_cmt{
		.f(.22rem);.fc(#666);.right;.mt(.18rem);

		span{ .fc(#FF7A9A); }
		.space{ .pl(.15rem); }
	}
	.title_wrap{ .hidden; }
	.title{ .left;.w(4.4rem);.mt(.1rem);.f(.28rem);.l(.3rem);.fc(#666);font-weight: normal;.ellipsis; }
	.label{ .right;.f(.22rem);.fc(#666);.l(.24rem);.mt(.15rem); }
}

.welfare{
	.hidden;.bgc(#FFF);border-top: 1px dashed rgba(147,147,147,0.39);

	.item{ padding: .2rem 0;.w(5.8rem);.auto;.f(.24rem);.fc(#666);.l(.4rem);.h(.4rem);.hidden; }
	.activity{
		.block;border-top: 1px solid #E5E5E5;.pr;.icon(0, 59.5%);

		&:first-child{ border-top:  none; }
		span{ .ellipsis;.w(5.6rem);.left;.pl(.24rem); }
	}
	.coupon_bannar{
		.hidden;

		p{ .left; }
		span{ .left;.pr;padding: 0 .1rem;.ml(.25rem);.f(.2rem);.fc(#fff);.l(.4rem); }
		span:nth-child(2){ .bgc(#FF7A9A); }
		span:nth-child(3){ .bgc(#FFB259); }
		span:nth-child(4){ .bgc(#67CDFF); }
	}
	.promote_activity{ .block;.fc(#E96268);.c;.bgc(#FEE9E9);padding: .2rem .3rem; }
}

.sku_main{
	.mt(.2rem);.bgc(#fff);.hidden;

	.title,
	.no_title{
		.pr;padding: 0 .3rem;.f(.26rem);.fc(#666);.l(.7rem);

		span{ .right;.mr(.3rem);.f(.22rem);.fc(#999); }
	}
	.title:after{ content: '';.pa;top: 0;right: .3rem;.h(100%);.w(.15rem);.icon(right, 49.9%); }

	.sku_detail,
	.sku_size{
		border-top: 1px solid #E5E5E5;.fc(#FF7A9A);.l(.7rem);.c;.left;.w(50%);box-sizing: border-box;
	}
	.sku_detail{ border-right: 1px solid #E5E5E5;.w(3.19rem); }

	.sku_comment .title{ .pb(0);.mb(-.22rem); }
	.item{ padding: .3rem;.pl(0);.ml(.3rem);.w(5.8rem); }
	.item:first-child{ border-bottom: 1px solid #E5E5E5; }
	.img_wrap{
		.left;.w(.6rem);.h(.6rem);.border-r(50%);.hidden;

		img{ .w(100%); }
	}
	.info,
	.content,
	.sku{
		.ml(.8rem);.f(.22rem);
	}
	.info{
		.fc(#666);

		.nickname{ .i_block;.w(2.8rem);.ellipsis; }
		.words{ .fc(#999);.pl(.1rem); }
		.time{ .right;.f(.2rem);.fc(#ccc);.l(.22rem); }
	}
	.content{ .mt(.17rem);.w(5rem);.fc(#666);text-align: justify; }
	.sku{ .fc(#999);.mt(.1rem); }
}

.service{
	.block;.h(.22rem);padding: .26rem 0;.bgc(#fff);.c;

	.promise{
		.hidden;.f(.22rem);.fc(#999);.i_block;

		img{ .w(.22rem);.h(.22rem);.left; }
		li{ .left;.nowrap;.l(.22rem);.c;.mr(.4rem); }
		li:last-child{ .mr(0); }
		.words{ .pl(.05rem); }
	}
}
.sku_shop{
	.block;.mt(.2rem);.bgc(#fff);padding: .3rem;.pb(0);.h(.6rem);.hidden;

	.shop_info{ .hidden; }
	.guang{
		.right;.h(.52rem);padding: 0 .16rem;.border-r(.04rem);.bgc(#FBA947);.white;.c;.l(.52rem);
	}
	dt{
		.left;.mr(.18rem);

		img{ .w(.6rem);.h(.6rem);.border-r(50%); }
	}
	dd{
		.left;

		.title { .fc(#666);.pb(.14rem); }
		.info {
			.f(.22rem);.fc(#999);

			span{ .pl(.15rem); }
		}
	}

	.shop_eva {
		.f(.2rem);.fc(#666);.pt(.2rem);.hidden;.c;

		li{
			.i_block;.mr(.2rem);.l(.24rem);

			&:last-child{ .mr(0); }
			span { .i_block;.pl(2px);.pr(.3rem); }
			.up{ .icon(-12%, 52%);.fc(#FB3F3F); }
			.down{ .icon(95%, 52%);.fc(#ADBD31); }
			.equal{ .icon(41%, 52%);.fc(#ED7D2B); }
		}
	}
}

.shop_wrap{
	padding: 0;.bgc(#fff);border-top: 1px solid #E5E5E5;.w(6.4rem);

	h3 { .f(.28rem);.fc(#666);padding: .3rem;font-weight: normal; }
}

.shop_wrap{
	#goods_wall { .h(3.19rem);.pr(.2rem);.hidden;overflow-x: scroll;white-space: nowrap;.pl(.3rem);.c;.f(0); }
	.poster-link {
		.pr;
		.block;
		background: #fff url("@{pic_url}/images/huodong/tuan/prebg.png") center center no-repeat;background-size: cover;
		background-size: 100% auto;
		img { .w(100%);.h(100%);.pa; top: 0;left: 0;}
		.none { .none; }
	}
	.poster {
		.i_block;.pb(.25rem);.w(1.6rem);.mr(.1rem);

		.bs_load_img{ .block;.w(100%);.h(2.26rem);background: no-repeat center;background-size: cover;opacity: 0; }
		.poster-info-title{ .mt(.1rem);.mb(.1rem);.f(.20rem);.l(.24rem);.fc(#666);.ellipsis; }
		.poster-info-price { .f(.2rem);.fc(#FF7A9A); }
	}
	[data-col="0"] { figure { .pl(.6/100*1rem);.pr(.3/100*1rem); } }
	[data-col="1"] { figure { .pl(.3/100*1rem);.pr(.6/100*1rem); } }

	/*加载提示*/
	.status_tips {
		.c;.l(4/100*1rem);.f(1.4/100*1rem); color:#888;
		&[data-status="loading"] {
			&:before {
				content: '加载中 ...'
			}
		}
		&[data-status="stop"] {
			&:before {
				content: '木有了'
			}
		}
	}
}

.hot_goods_wrap{
	.hidden;.bgc(#FFF);.mt(.2rem);

	h3{ .f(.28rem);.fc(#666);padding: .3rem;font-weight: normal; }
	.goods_wrap{ .pl(.3rem); }
	.poster{
		.left;.w(1.86rem);.mr(.12rem);.mb(.26rem);

		figcaption div{ .h(.22rem);.c; }
		.img_frame{ .block;background: #fff url("@{pic_url}/images/huodong/tuan/prebg.png") center center no-repeat;background-size: cover; }
		.bs_load_img{ .block;.w(1.86rem);.h(2.6rem);background: no-repeat center;background-size: cover;opacity: 0; }
		.title{ .mt(.14rem);.mb(.06rem);.f(.2rem);.l(.2rem);.fc(#666);.ellipsis; }
		.price{ .f(.2rem);.fc(#FF7A9A); }
		.like{ .f(.2rem);.fc(#999);.ml(.18rem);.pl(.2rem);.icon(0, 77.2%);background-size: 5rem; }
	}
	.download_btn{
		.left;.w(4.1rem);.h(.6rem);.ml(1.15rem);.mt(.3rem);.mb(.3rem);.bgc(#F66);.border-r(.35rem);.f(.24rem);.fc(#FFF);.l(.6rem);.c;

		span{ .auto;.pl(.32rem);.h(.3rem);.icon(right, 58%);.mr(.1rem); }
	}
}

.bottom_search_wrap{
	.bgc(#fff);.border-r(.4rem);.w(5.8rem);.h(.6rem);.auto;.mb(.36rem);.f(.22rem);outline: none;

	input{ .left;.w(4.3rem);.f(.22rem);.h(.6rem);.l(.24rem);padding: .18rem 0 .18rem .08rem;-webkit-appearance: none;appearance: none;border: none;outline: none;box-sizing: border-box; }
	label{ .fc(#FF7A9A);.right;.l(.3rem);.mt(.15rem);border-left: 1px solid #FF7A9A;.pl(.2rem);.mr(.3rem); }
	.search_icon{ .left;.w(.21rem);.h(.6rem);.icon(right, 79%);.ml(.3rem); }
}


/*
	模拟客户端单品页优惠券弹窗
*/
.wap_fix_coupon(){
	.fix_coupon{ .w(100%);.h(100%);.pf;top: 0;left: 0;z-index: 500;visibility: hidden; }
	.black_mask{ .bgc(rgba(0,0,0,.58));.w(100%);.h(100%);transition: opacity .3s ease; }
	.fix_coupon_wrap{
		.pa;bottom: 0;left: 0;transition: -webkit-transform .5s ease;-webkit-transform:translateY(100%);transform:translateY(100%);.w(100%);

		.title{ .bgc(#fff);.pr;.f(.28rem);.c;.l(.8rem);.fc(#666);border-bottom: 1px solid #E5E5E5; }
		.close{ .pa;right: 0;.w(72*@w);.h(100%);background: url('http://d04.res.meilishuo.net/img/_o/14/82/4104a16dfd61fd48bbfec8d3bb37_64_64.cg.png') no-repeat center;background-size: .32rem; }
		.coupons{ .pl(20*@w);.bgc(rgba(255,255,255,.95));.fc(#80737b);max-height: 3.63rem;overflow: auto; }
		.coupon{
			.hidden;border-bottom: 1px solid #E5E5E5;padding: .2rem 0;

			.desc{ .i_block;.w(480/620*100%);.ellipsis;.l(.46rem); }
			.get_btn{ .w(.84rem);.c;.fc(@red);.right;.mr(.2rem);.border-1(#fd92b6);padding: .1rem 0;.border-r(.04rem); }
			.has_get,
			.has_over{ .fc(#ccc2c9);border: none; }
		}
		.ps{
			padding: .24rem 0 .22rem;

			span{ .bgc(#f0edef);.i_block;padding: .1rem;.border-r(.04rem); }
		}
		.get_all{
			padding: .2rem 0;.bgc(#fff);border-top: 1px solid #E5E5E5;

			.get_all_btn{ .w(2.1rem);.h(.6rem);.auto;.border-r(.06rem);.bgc(#FF7B9A);.fc(#fff);.c;.l(.6rem); }
		}
	}
	//进场和退出动画
	.slide_bottom_in{
		.black_mask{opacity: 1;}
		.fix_coupon_wrap{
			-webkit-transform: translateY(0);
			transform: translateY(0);
		}
	}
	.slide_bottom_out{
		.black_mask{opacity: 0;}
		.fix_coupon_wrap{
			-webkit-transform: translateY(100%);
			transform: translateY(100%);
		}
	}
}
.wap_fix_coupon;
.ui_notice_rem;
