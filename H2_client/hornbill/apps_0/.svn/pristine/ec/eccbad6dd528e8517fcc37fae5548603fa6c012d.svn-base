.windowDrive .dialog{ .w(4rem); }
.windowDrive .dialog-con{
	.fc(#666);padding: .2rem;

	.title{ .pb(.15rem); }
}
.windowDrive #alert .dialog-btn span{ .l(.6rem);.f(.26rem); }


