@import "block.less";
body{
	background-color: #FFFFFF;
}
//搜索框相关
.searchPlaceholder{
	// .h(0.8rem);
	position: -webkit-sticky;
	position: -moz-sticky;
	position: -o-sticky;
	position: -ms-sticky;
	position: sticky;
	top:0;
	left:0;
	z-index: 12;
}
.searchWrap{
	z-index: 12;
	padding: 0.14rem 0;
	.w(100%);
	.bgc(#F7F7F5);
	.search{
		padding:0.14rem 0.6rem;
		margin: 0 auto;
		.w(5rem);
		background: url('http://d04.res.meilishuo.net/pic/_o/92/83/926107162b78ab2bfb7f8e2fc27b_32_33.cj.png') no-repeat;
		background-size: 0.3rem;
		border-radius:30px;
		background-position: 0.2rem center;
		.bgc(#fff);
		.f(0.24rem);
	}
}
// 轮播banner
.sliderBanner;
.banner-slider-point{.pa;.w(100%);left:0;.mb(-10px);.mt(0);
	li{float: none;.i_block;background-color:#fff;.vm;.w(5px);.h(5px);.mr(2px);}
	.curr{background-color:#FFFFFF;border:1px solid #fff}
}
.banner-slider{.pr;
}
//ppt轮播样式
.swipe {
	overflow: hidden;
	visibility: hidden;
	position: relative;
}
.swipe-wrap {
	overflow: hidden;
	position: relative;
}
.swipe-wrap > div {
	float:left;
	width:100%;
	position: relative;
}

.title{
	padding: 0.3rem;
	.f(0.32rem);
	.fc(#333333);
	em{
		.fc(#FACD00);
		padding: 0 5px;
	}
	img{
		.mr(0.1rem);
		.right;
		.w(2.45rem);
		.h(0.3rem);
	}
}
.wrap{
	.swipe{
		padding: 0 0.3rem 0.2rem 0.3rem ;

	}
	.imageSlide{
		overflow: hidden;
		.w(5.8rem);
		.mb(0.2rem);
		.goodsOverage{
			.pr;
			.w(2.9rem);
			.goods{
				.left;
				img{
					.w(2.9rem);
					.h(3.6rem);
					.block;
				}
			}
			.overage{
				.pa;
				top:3.1rem;
				clear: both;
				.w(100%);
				.h(0.5rem);
				.l(0.5rem);
				background-color: RGBA(255,255,255,0.9);
				.c;
				.f(0.24rem);
				.fc(#333333);
			}
		}
		.dec{
			.left;
			padding: 0.2rem 0.2rem 0;
			.w(2.5rem);
			.h(3.4rem);
			.f(0.26rem);
			.fc(#333333);
			.c;
			.bgc(#F5F5F5);
			.readding{
				.pb(0.15rem);
				.f(0.26rem);
				.fc(#333333);
				.l(0.36rem);
				text-align: left;
			}
			.time{
				overflow: hidden;
				.pt(0.1rem);
				.pl(0.18rem);
				background: url('http://d03.res.meilishuo.net/pic/_o/88/d1/4885259a2c715502529e8b3fed69_24_26.cj.png') no-repeat;
				background-position: 1% 0;
				clear: both;
				.f(0.3rem);
				background-size: 0.12rem 0.13rem;
				img{
					.w(0.07rem);
					.h(0.48rem);
					.left;
					margin: 0 0.06rem;
				}
				label{
	  				.left;
	  				.w(.46rem);
	  				.h(.46rem);
	  				.l(.48rem);
	  				.c;
	  				border:1px solid #DCDCDC;
	  				border-radius: .06rem;
	  				font-weight: 600;
	  				.fc(#333333);
	  				.f(0.3rem);
	  				.ml(.05rem);
	  				.mr(.05rem);
	  			}
			}
			.price{
				.mt(0.2rem);
				background: url('http://d02.res.meilishuo.net/pic/_o/dc/2a/79b05b434b990ebbf4c059456c8b_24_26.cj.png') no-repeat;
				background-size: 0.12rem 0.13rem;
				background-position: 98% bottom;
				font-weight: 600;
				.f(0.4rem);
				.fc(#333333);
				span{
					.f(0.24rem);
				}
			}
			.orginalPrice{
				.mt(0.15rem);
				.c;
				.fc(#999999);
				.f(0.24rem);
				text-decoration: line-through;
			}
			.buy{
				margin:0.2rem auto 0;
				.w(1.68rem);
				border-radius: 5px;
				img{
					.w(1.68rem);
					.h(0.6rem);
				}
			}
		}
	}
	.spotWrap{
		.l(10px);
		margin: 0 auto;
		vertical-align: middle;
		overflow: hidden;
		.c;
		li{
			float: none;
			.i_block;
			.vm;
			.w(27px);.h(10px);.mr(2px);
		}
		.spot{
			.left;
			.ml(10px);
			.mr(10px);
			.mt(1px);
			.w(5px);
			.h(5px);
			.bgc(#D8D8D8);
			-webkit-border-radius : 50%;
			border-radius: 50%;
		}
		.on{
			.bgc(#FBCD00);
			.w(7px);
			.h(7px);
			.mt(0px);
		}
	}
}
.segment{
	.w(100%);
	.h(0.2rem);
	.bgc(#F0F0F0);
}
//hi好货
.higood{
