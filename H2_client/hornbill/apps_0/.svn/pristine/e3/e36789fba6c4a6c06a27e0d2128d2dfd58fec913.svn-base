		padding:.1rem 0 .1rem .08rem;.hidden;
	}
	ul{
		.hidden;
	}
	li{
		.left;
		a{
			.block;
			img{
				.block;
				.w(2.4rem);
				.h(2rem);
			}
		}
	}
}
.goodsWrap{
	.w(100%);
	-webkit-overflow-scrolling: touch;
	-moz-overflow-scrolling: touch;
	-o-overflow-scrolling: touch;
	-ms-overflow-scrolling: touch;
	overflow-scrolling: touch;
	overflow: scroll;
	.pb(1rem);
	ul{.w(5000px);overflow:visible;}
	li{.w(2.63rem);}
}
// hi品
.pinWrap{
	overflow: hidden;
	padding: 0 0.3rem;
	.pin{
		.left;
		.pb(0.35rem);
		.w(1.8rem);
		.f(0.24rem);
		.c;
		.pinImg{
			.block;
			overflow: hidden;
			.w(1.8rem);
			border-radius:0.9rem;
			img{
				.w(100%);
			}
		}
		.pinName{
			padding: 0.1rem 0 0.2rem;
			.f(0.28rem);
		}
		.borderDiv{
			margin: 0 auto;
			.w(0.4rem);
			border-top: 1px solid #666666;
		}
		.desc{
			overflow: hidden;
			padding: 0.1rem 0 0.2rem;
		}
	}
	.pin:nth-child(-n+2){
		.pr(0.2rem);
	}
	.pin:nth-child(n+5){
		.pl(0.19rem);
	}
}
//hi店
.hiShop{
	.classWrap{
		.w(100%);
		.h(0.92rem);
		position: -webkit-sticky;
		position: -moz-sticky;
		position: -o-sticky;
		position: -ms-sticky;
		position: sticky;
		top:0.8rem;
		left:0;
		z-index: 10;
		.classification{
			display                 : -webkit-box;
			display                 : -moz-box;
			display                 : -ms-flexbox;
			display                 : -webkit-flex;
			display: flex;
			display: -webkiit-flex;
			-webkit-box-pack        : center;
	 		-moz-box-pack           : center;
			-ms-flex-pack           : center;
			-webkit-justify-content : center;
			justify-content         : center;
			padding: 0.3rem 0rem 0.15rem;
			z-index: 10;
			.bgc(#FFFFFF);
			.w(100%);
			border-top: 1px solid #F1F1F1;
			.selectWrap{
				.c;
				span{
					.left;
					.block;
					padding:0 0.05rem 0.14rem 0.05rem;
					.pb(0.14rem);
					.f(0.28rem);
					.fc(#666666);

				}
				span:nth-child(n+2){
					.ml(0.5rem);
				}
				.active1{
					.fc(#333333);
					border-bottom: 2px solid #FBCD00;
				}
			}
		}
	}
	.waterFall{
		.goodsList{
			.f(0.26rem);
			.shop{
				.pr;
				background: url('http://d05.res.meilishuo.net/pic/_o/84/5f/0aea1d897e91347a10e9c5e43d53_180_180.cj.png') no-repeat;
				background-size: 1.8rem;
				background-position: center;
				.bgc(#F8F8F8);
				img{
					.block;
					.w(100%);
					.h(3rem);
					opacity: 0;
				}
				.goShop{
					.pa;
					left: 38.2%;
					bottom:0.35rem;
					.w(1.54rem);
					.h(0.56rem);
					.l(0.56rem);
					border-radius: 3px;
					.c;
					.f(0.24rem);
					.fc(#FFFFFF);
					.bgc(#FBCD00);
				}
			}
			.waterGoods{
				overflow: hidden;
				padding: 0.2rem 0.3rem 0;
				.goodsInfo{
					.left;
					.c;
					background: url('http://d05.res.meilishuo.net/pic/_o/84/5f/0aea1d897e91347a10e9c5e43d53_180_180.cj.png') no-repeat;
					background-size: 100%;
					img{
						.block;
						.w(1.8rem);
						.h(1.8rem);
						opacity: 0;
					}
					.goodsName{
						padding: 0.15rem 0 0.1rem;
						.fc(#535353);
					}
					.goodsPrice{
						.f(0.24rem);
						.fc(#535353);
					}
				}
				.goodsInfo:nth-child(n+2){
					.ml(0.2rem);
				}
			}
			.segWrap{
				padding: 0.5rem 0 0;
				.h(0.6rem);
			}
			.seg{
				.block;
				margin: 0 auto;
				.w(2.8rem);
				.h(0.1rem);
			}
		}
	}
}
.loading{
	.pullUpDown > .common;.pullUpDown > .loading;
	.w(.4rem);
	.h(.4rem);
	background-size: 100%;
	&:before{
		.nowrap;
		.ml(-.1rem);
	}
	&[status="tap"]{
		background:0 none;	
	}
	&[status="stop"]{
		background:0 none;	
	}
}
.more{
	.pullUp > .pullUp;
	.l(20px);
	.w(2rem);
	.h(0.4rem);
	margin: 0 auto;
	background-size: 100%;
	&[status="tap"]{
		background:0 none;	
		&:before{
			content:"上拉看更多...";	
		}
	}
	&[status="stop"]{
		background:url('http://d04.res.meilishuo.net/pic/_o/86/4f/4ac3de09751d53b935b1b047ef17_350_236.cj.png') no-repeat;
		background-size: 100%;
		padding: 0.7rem 0;
		&:before{
			content:"";	
		}
	}
}
