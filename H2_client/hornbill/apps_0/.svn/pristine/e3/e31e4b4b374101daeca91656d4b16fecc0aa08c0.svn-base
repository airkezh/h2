#com-nav{
	position: relative;
	z-index: 400;
	height: 50px;
	line-height: 47px;
	border-bottom: 2px solid #f69;
	background-color: #fff;
}
#com-nav.fixed{
	position: fixed;
	width: 100%;
	top: 0;
}
#com-nav .inner{
	width: 1200px;
	margin: 0 auto;
}
#com-nav li{
	float: left;
}
#com-nav a{
	float: left;
    font-size: 14px;
    font-weight: bolder;
    color: #333;
    padding: 0 22px;
}
#com-nav a:hover,#com-nav a.active{
	border-color: #ff6699;
	color: #ff6699;
}