		opacity: .6;
		small{
			.f(.16rem);
			vertical-align: baseline;
		}
	}
	img{.h(100%);}
	.left-b{
		.left;
		.w(3.8rem);
		.h(5.44rem);
	}
	.right-b{
		.right;
		.ml(.02rem);
		.mt(-.02rem);
		.w(1.8rem);
		.h(5.44rem);
		a{
			.h(1.8rem);
			.mt(.02rem);
		}
	}
