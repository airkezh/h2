// 轮播
//.imageSlide_wrap{ .w(5.6rem);.hidden; }
.imageSlide{
	border: 0px solid black;
	font-size: 0;
	.w(5.62rem); .h(6.2rem);
	.hidden;
	ul{
		.pr;
		li{
			.w(5.62rem);
			.hidden();
			float: left;
		}
		.clearfix;
	}

}
.pullUp{
	.pullUpDown > .common;.pullUpDown > .loading;
	.w(.4rem);
	.h(.4rem);
	background-size: 100%;
	&:before{
		.nowrap;
		.ml(-.1rem);
	}
	&[status="tap"]{
		background:0 none;	
	}
	&[status="stop"]{
		background:0 none;	
	}
}
