@import "sq.less";

html, body {
	.bgc(#efefef);
}

li{
	list-style: none;  
}

.section_wrap {
	.mb(1.18rem);
}

.doota;
.doota > .list;
.doota > .numBox;

.numBox{
	.mt(.10rem);
	.w(1.60rem);
	.h(.44rem);
	.minus, .plus{
		.w(.44rem);
		.h(.44rem);
		.f(.26rem);
		.l(.44rem);
	}
	.nums{
		.f(.26rem);
		.w(.64rem);
		.h(.44rem);
		.pt(0);
		.l(.44rem);
		//.l(.44rem);
		// background: gray;
		//.pt(.08rem);

	}
}
.singleThing-grey{
	.nums{
		//border-color: #f7f7f7;
	}
}

#empty{
	.none;
	.auto();
	.c();
	.shoppingCart{
		.w(1.00rem);
		.auto();
		.mt(2.44rem);
	}
	.content{
		.f(.36rem);
		.mt(.40rem);
	}
	.goMallButton{
		.auto();
		.w(3.60rem);
		.h(.78rem);
		.border-r();
		.bgc(#ff6699);
		.mt(1.04rem);
		.buttonContent{
			.f(.30rem);
			.l(.78rem);
			.fc(#ffffff);
		}
	}
}

.alert{
	.f(.20rem);
	.w(20%);
	.hidden();
	.l(.20rem);
	.h(.20rem);
	.block();
	.ml(.65rem);
	.fc(#b71414);
	.c();
	.pr();
	top:-.10rem;
}

.sec1-b{
	.header{
		.h(.82rem); 
		.delButton{
			.f(.32rem);
			.fc(#2d91e8);
			.right();.l(.80rem);
			.mr(.40rem);.ml(.30rem);
		}
		.b_red{.right(); .pr(.30rem);.fc(#ee4f4f); .l(.40rem); .h(.40rem);.mt(.20rem); .border-rc(solid);}
	}
	.shopThings{
		.hidden;
		.bgc(#ffffff);
		.mb(.20rem);
		.shop-info{
			.pb(.2rem);
			.ml(.3rem);
			.pr;
			.shop-info-name{
				.fc(#666);
				.pl(.64rem);
				.pt(.42rem);
				.f(.28rem);
				a {
					.fc(@red);
				}
			}
			.shop-info-coupon {
				.pr;
				.pl(1.07rem);
				.pr(.3rem);
				.pt(.1rem);
				.pb(.1rem);
				.mt(.1rem);
				.f(.24rem);
				.fc(#999);
				background: url(http://d04.res.meilishuo.net/pic/_o/ba/cc/99ffbb6f4985b3cca5550072e1b8_26_26.ch.png) .66rem center no-repeat;
				background-size: .28rem .28rem;
				&::after {
					content: '';
					float: right;
					.w(.14rem);
					.h(.14rem);
					//.mt(.3rem);
					border-top: 1px solid #f69;
					border-right: 1px solid #f69;
					-webkit-transform: rotate(45deg) translateY(.02rem);
					transform: rotate(45deg) translateY(.02rem);
				}
			}
			.select-all{
				.pa();
				left: 0;
				top: .36rem;
				.w(.44rem);
				.h(.44rem);
			}
			.null{
				background: url("@{pic_url}/images/wap/web/sq/cart/null.png") no-repeat;
				background-size: .44rem;
			}
			.right{
				background: url("@{pic_url}/images/wap/web/sq/cart/right.png") no-repeat;
				background-size: .44rem;
			}
		}
		.singleThing-grey{
			.bgc(#f7f7f7);
		}
		.singleThing{
			.pr();
			.h(1.80rem);
			.pl(.3rem);
			//.bgc(#efefef);
			.select{
				.pa();
				left: .20rem;
				top: .70rem;
				padding: .1rem;
				.w(.44rem);
				.h(.44rem);
			}
			.null{
				background: url("@{pic_url}/images/wap/web/sq/cart/null.png") no-repeat;
				background-size: .44rem;
				background-origin: content-box;
			}
			.right{
				background: url("@{pic_url}/images/wap/web/sq/cart/right.png") no-repeat;
				background-size: .44rem;
				background-origin: content-box;
			}
			a.single-img{
				.pr;
				.block;
				.mask{
					.w(1.20rem);
					.pa;
					.h(.40rem);
					left: .70rem;
					top: 1.10rem;
					.bgc(#000);
					opacity: 0.3;
					z-index: 1;
				}
				.mask-con{
					.w(1.20rem);
					.c;
					.pa;
					.h(.40rem);
					left: .70rem;
					top: 1.10rem;
					.fc(#fff);
					z-index: 2;
					.l(.40rem);
					.f(.24rem);
				}
			}
			img{
				.left();
				.w(1.20rem);
				// .h(1.20rem);
				.ml(.64rem);
				.mt(.30rem);
			}
			.thingInfo{
				.w(3.96rem);	
				.left();
				.pt(.30rem);
				.pl(.20rem);
				.f(.20rem);
				.thingName{
					.fc(#666);
					.l(.28rem);
					.f(.26rem);
					.hidden();
					.mb(.10rem);
					span{
						.fc(#666);
					}
				}
				.secondLine{
					.h(.48rem);
					.thingOther{
						.h(.44rem);
						.l(.44rem);
					}
					//.mt(.10rem);
					.del{
						float: right;
						padding: .20rem;
						.w(.34rem);
						.h(.32rem);
						background: url('http://d03.res.meilishuo.net/pic/_o/8b/69/1f5454cff154a4aac727c6dc8908_34_31.cf.png') center center no-repeat;
						background-size: cover;
						background-origin: content-box;
					}
				}
				.thingOther{
					.fc(#999);
					.vm();
					.f(.22rem);
					.i_block();
					.break-all();
				}
				.otherOne{
					.hidden();
				}
				.otherThree{
					.w(50%);
					.h(.44rem);
					.l(.44rem);
					.hidden();
				}
				.otherTwo{
					.mr(.2rem);
					float: right;
					.hidden();
					.f(.20rem);
					.vm;
				}
				.otherFive{
					.pl(.20rem);
				}
			}
		}
		.lastThing{
			.border-n();
		}
		.shopBt {
			background: #f0edee url("@{pic_url}/images/wap/web/wx/huabian.png") repeat-x;
			.h(.10rem);
		}
		.line{
			 opacity: 0.5;
			 .w(100%);
  			.h(1px);
  			.bgc(#eaeaea);
		}
		.bt-main{
			.w(100%);
			.pr;
			.hidden;
			.h(.90rem);
			.shop-num{
				.l(.90rem);
				.pl(.30rem);
				.fc(#999);
				.f(.24rem);
			}
			.shop-total{
				.pr(.1rem);
				.mt(.4rem);
				.fc(#333);
				span{
					.fc(#ff7a9a);
				}
				.f(.28rem);
			}
		}
		.del-all{
			.w(2.05rem);
			.f(.26rem);
			.h(.60rem);
			.auto;
			.mt(.25rem);
			.mb(.25rem);
			.border-r(.10rem);
			.l(.60rem);
			.c;
			.border-1(#999);
			.fc(#a49fa2);
		}
	}
	.shopThings-del{
		.singleThing{
			img, .thingName, .thingOther{
				opacity: 0.5;
			}
			.thingInfo{
				.pr;
				.secondLine{
					.pa;
					right: 0;
					top: .60rem;
				}
			}
		}
	}
	.floatRight{
			.mr(.2rem);
			.right;
		}
	// .gap{
	// 	.h(1.18rem);
	// }
	.float_ctrl{
		.pf; bottom: 0;
		.h(.98rem);
		.w(100%);
		.f(.28rem);
		background:rgba(255,255,255,1);
		//border-top: 1px solid #e1e1e1;
		box-shadow: 0 -2px 7px #e1e1e1;
		z-index: 10;
		.selectAll{
			background: url("@{pic_url}/images/wap/web/sq/cart/null.png") no-repeat left;
			background-size: .44rem;
		}
		.cancleAll{
			background: url("@{pic_url}/images/wap/web/sq/cart/right.png") no-repeat left;
			background-size: .44rem;
		}
		li{
			.left;
			&:nth-child(1){
				.w(.7rem);
				.l(.98rem);
				.ml(.30rem);
				.pl(.64rem);
			}
			&:nth-child(2){
				.w(2.5rem);
				.tr;
				p{
					&:nth-child(1){
					.fc(@red);
					.l(.60rem);
					}
					&:nth-child(2){
						.f(.20rem);.fc(#b3b3b3);
					}
				}
			}
			&:nth-child(3){
				.right;
				.w(2rem);
				.l(.98rem);
				.white;
				.c;
				.bgc(#FF6666);
				.go_buy_none{
					.bgc(#d1d1d1);
				}
				a{
					.block;
					.w(100%);
					.h(100%);
				}
			}
		}
	}
}

.sec2 {
	.none;
	.pb(.1rem);
	.bgwhite;
	h2 {
		.pt(.38rem);
		.pb(.38rem);
		.ml(.3rem);
		.f(.3rem);
		.fc(#333);
		.fn;
	}
	.sliders-section-g {
		.w(1.7rem);
		margin: 0 .06rem;
		a {
			.h(2.33rem);
		}
		.sliders-section-g-title {
			.h(.48rem);
			.l(.25rem);
			.hidden;
		}
	}
	li:first-child {
		.ml(.26rem);
	}
	li:last-child {
		.mr(.26rem);
	}
}

.sec3 {
	@sec3Red: #FE92B5;

	.none;
	.pf; left: 0; bottom: 0;
	.w(100%);
	.pt(9.99rem);
	z-index: 70;
	.bgc(rgba(0,0,0,.6));
	.sec3-t {
		.pr;
		.l(.76rem);
		.fc(#666);
		.f(.3rem);
		.fn;
		.c;
		.bgwhite;
		span {
			.pa; top: 0; right: .04rem;
			.w(.4rem);
			.h(.4rem);
			padding: .16rem;
			//.f(0);
			background: url(http://d01.res.meilishuo.net/pic/_o/68/38/7ca5c38700c2580c239eddc962fc_28_29.ch.png) center center no-repeat;
			background-size: contain;
			background-origin: content-box;
		}
	}
	.sec3-m {
		border-top: 1px solid #efefef;
		border-bottom: 1px solid #efefef;
		.coupon {
			.h(.76rem);
			.pl(.26rem);
			.bgwhite;
			.f(0);
			//.hidden;
			//.clearfix;
		}
		.coupon-l,
		.coupon-r {
			.i_block;
			.h(100%);
			.vm;
		}
		.coupon-l {
			.w(4.74rem);
			border-bottom: 1px solid #efefef;
		}
		.coupon-l-t {
			.mt(.16rem);
			.f(.24rem);
		}
		.coupon-l-b {
			.mt(.06rem);
			.f(.17rem);
			.fc(@sec3Red);
		}
		.coupon-r {
			.w(1.4rem);
			.f(.24rem);
			.c;
			border-bottom: 1px solid #efefef;
			.apply {
				.i_block;
				.h(.34rem);
				.l(.36rem);
				.mt(.17rem);
			}
			.get {
				.w(.68rem);
				.fc(@sec3Red);
				border: 1px solid @sec3Red;
				.border-r(.06rem);
				&::before {
					content: '领取';
				}
			}
			.got {
				.fc(#999);
				&::before {
					content: '可使用';
				}
			}
		}
		.prompt {
			.l(.68rem);
			.pl(.26rem);
			.f(.2rem);
			.bgwhite;
			i {
				.l(.34rem);
				padding: .07rem .1rem .05rem;
				.pl(.09rem);
				.pr(.15rem);
				font-style: normal;
				.border-r(.06rem);
				.bgc(#e8e8e8);
			}
		}
	}
	.sec3-b {
		.l(1rem);
		.f(.26rem);
		.c;
		.bgwhite;
		span {
			.i_block;
			.w(2.74rem);
			.l(.64rem);
			.white;
			.border-r(.06rem);
			.bgc(@sec3Red);
		}
	}
}
