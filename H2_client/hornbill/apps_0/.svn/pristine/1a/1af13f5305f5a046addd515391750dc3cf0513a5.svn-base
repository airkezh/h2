	}
	.norms_type { 
		p { /*.pt(.20rem);*/ .l(.36rem); .fc(#666); .f(.24rem);}
		.last { .border-n;}
		span { .w(260/560*100%); .l(.4rem); .i_block; }
		b { font-weight: normal;}
	}
	.norm_tab {
		.w(100%);
		td{
			.l(.60rem); .border-bd9;
		}
	}
	.img_wrap { /*.border-bd9; */ }
