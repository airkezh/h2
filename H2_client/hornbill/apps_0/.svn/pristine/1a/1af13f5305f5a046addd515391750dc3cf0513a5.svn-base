	.pro_txt { .pb(.2rem); .l(.48rem); }

	.size {
	    padding: 0 .3rem;
	    .bgwhite;
	    h2, .table_gap { .block; .f(.28rem); .l(.76rem); .h(.76rem); .fn; .fc(#333); .tl; .border-bd9; }
	    table { 
	    	.mt(.04rem);
			.w(100%); 
			.f(.24rem);
			tr { .l(300%); .border-bd9; }
			.tr1 { .border-td9; }
			th, 
			td { .f(.24rem); }
			th { .tl; .fc(#999); .w(20%); }
			td { .c; }
			.size-content { .red; }
  	    }
		.td-style2 { td { .w(80%); } }
		.td-style3 { td { .w(40%); } }
		.td-style4 { td { .w(26.66%); } }
		.td-style5 { td { .w(20%); } }
  	    img { .w(100%); }
	    .no-size-info {
		    .c;
		    .f(.28rem);
		    .fn;
		    .fc(#999);
