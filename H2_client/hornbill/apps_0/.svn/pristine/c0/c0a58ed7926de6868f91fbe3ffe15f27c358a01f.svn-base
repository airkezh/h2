	body:nth-of-type(1){
		/*form 表单 checkbox*/
		.mls-input-label {
			-webkit-touch-callout: none;
			-webkit-user-select: none;
			-khtml-user-select: none;
			-moz-user-select: none;
			-ms-user-select: none;
			user-select: none;
		}

		input[type=checkbox].mls-input-checkbox {
			position: absolute; 
		    overflow: hidden;
		    visibility: hidden;
		    height:1px; 
		    width:1px; 
		    margin:-1px; 
		    padding:0;
		    border:0;
		}

		input[type=checkbox].mls-input-checkbox + label.mls-input-checkbox-label {
			padding-left:20px;
			height:15px; 
			display:inline-block;
			line-height:15px;
			background-repeat:no-repeat;
			background-position: 0 0;
			vertical-align:middle;
			cursor:pointer;
		}

		input[type=checkbox].mls-input-checkbox:checked + label.mls-input-checkbox-label {
			background-position: 0 -15px;
		}

		.mls-input-checkbox-label{
			background-image:url("@{pic_url}/images/wilee/order/checkbox_16.png?2014");
		}

		/*form 表单 radio*/
		input[type=radio].mls-input-radio {
								display:none;
							}

		input[type=radio].mls-input-radio + label.mls-input-radio-label {
			padding-left:24px;
			height:24px; 
			display:inline-block;
			line-height:24px;
			background-repeat:no-repeat;
			background-position: 0 0;
			vertical-align:middle;
			cursor:pointer;

		}

		input[type=radio].mls-input-radio:checked + label.mls-input-radio-label {
			background-position: 0 -24px;
		}

		label.mls-input-radio-label {
			background-image:url("@{pic_url}/images/wilee/order/radio_16.png?as23asfas2322");
			-webkit-touch-callout: none;
			-webkit-user-select: none;
			-khtml-user-select: none;
			-moz-user-select: none;
			-ms-user-select: none;
			user-select: none;
		}

		/*form 表单 select*/
		@media screen and (-webkit-min-device-pixel-ratio:0) {
			select.mls-select{
				color:#666666;
				background: url("@{pic_url}/images/wilee/order/select_16.png") no-repeat right center transparent;
				-webkit-appearance: none;
				outline: none;
				margin: 0;
				padding: 4px 30px 4px 5px;
				border: 1px solid #d2d2d2;
				font-size: 12px;
				border-radius: 0px;
			}
			/*select.mls-select:focus{
				background: url("@{pic_url}/images/wilee/order/select_top_16.png") no-repeat right center transparent;
			}*/
		}
	}
