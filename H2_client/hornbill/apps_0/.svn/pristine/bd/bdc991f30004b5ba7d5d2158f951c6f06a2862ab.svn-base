/* 组图 */
.swipe{
	.pr;
	.hidden;
	.w(600*@w);
	margin:20*@w auto;
}

.swipe_wrap{
	.pr;
	.hidden;
	-webkit-backface-visibility:hidden;
	-moz-backface-visibility:hidden;
	-ms-backface-visibility:hidden;
	-o-backface-visibility:hidden;
	backface-visibility:hidden;
	/* Fix of Webkit flickering */
	z-index:2;
}

.swipe_wrap > div{
	.pr;
	.left;
	.w(100%);
}

.swipe_pic{
	.pr;
	.hidden;
	.w(100%);
	.bgc(#fff);
}

.swipe_pic img{
	.w(100%);
	min-height:120px;
}

.list_pics{
	clear:both;
	overflow:hidden;
	*zoom:1;
}

.wrapper{
	.pr;
	.hidden;
	.w(600*@w);
	margin:20*@w auto;
}

.list_pics li{
	.tl;
	.left;
	.inline;
	.w(31.2%);
	.mr(3.2%);
}

.list_pics li img{
	.w(100%);
}

.list_pics li:nth-child(3n){
	.mr(0);
}

.wrapperIndexActivity{
	.pr;
	.block;
	.hidden;
	.w(620*@w);
	.h(100%);
	margin:20*@w 0 20*@w 20*@w;
	z-index:1;

	.scrollerActivity{
		.left;
		.hidden;
		.h(100%);
		padding:0;

		ul li{
			.left;
			.block;
			.w(90px);
			.mr(10px);
			box-sizing:border-box;
			-webkit-box-sizing:border-box;
			&:last-child{
				.mr(0);
			}
		}  
	} 
}

@media screen and (min-width:321px){
	.wrapperIndexActivity .scrollerActivity ul li{
		.w(100px);
	}
}
