@import "../../block.less";

html{
	.f(10*@f);
}

body{
	.bgc(#fff);
}

img{
	.block;
	.w(100%);
}

a{
	.block;
	outline:none;
}

.banner_wrap{
	.pr;
	.w(100%);
	.pt(160*@w);
	background:url("@{pic_url}/images/wap/activity/may_sale/sign_mall_banner2.jpg") no-repeat;
	background-size:100%;

	.sign_bonus{
		.pa;
		.c;
		.w(20%);
		top:56%;
		left:10%;
		.f(2.5rem);
		.fc(#ff5f53);
	}

	.sign_txt{
		.pa;
		top:14%;
		left:35%;

		p{
			.l(1.6);
	    	.f(1.2rem);
			.fc(#fff);

			a{
				.fc(#fff);
			}

			em{
				.fc(#ffe814);
				font-style:normal;
			}
		}

		p.tit{
			.f(1.6rem);
		}
	}

	.icon_date{
		.pa;
		top:20%;
		right:1rem;
		.w(76*@w);
		.pt(93*@w);
		background:url("@{pic_url}/images/wap/activity/sign_mall/icon_date.png") no-repeat;
		background-size:100%;
	}

	.icon_animate{
		-webkit-animation:shake 1s .2s ease both;
		-moz-animation:shake 1s .2s ease both;
		@-webkit-keyframes shake{
			0%,100%{-webkit-transform:translateX(0);}
			10%,30%,50%,70%,90%{-webkit-transform:translateX(-10px);}
			20%,40%,60%,80%{-webkit-transform:translateX(10px);}
		}
	}
}

.coupon_wrap{
	.l(1.5);
	.f(1.2rem);
	.hidden;
	clear:both;
	padding:.6rem 1rem;
	border-bottom:1px solid #dcdcdc;

	.title{
		.left;
		.fc(#333);
		.i_block;
		.pl(32px);
		background:url("@{pic_url}/images/wap/activity/sign_mall/icon_coupon.png") no-repeat;
		background-size:auto 17px;
	}

	.text{
		.right;
		.fc(#ff5f53);
	}

	.text_box{
		.right;
		.fc(#b6b6b6);

		span{
			.i_block;
			.ml(.4rem);
			.bgc(#fff);
			.fc(#ff5f53);
			.border-r(5px);
			padding:0 1rem;
			border:1px solid #ff5f53;
			
		}
	}
}

.head_wrap{
	.c;
	margin:1.3rem auto;

	.head{
		.vm;
		.w(10%);
		.h(10%);
		.i_block;
		.mr(1rem);
		.border-r(50%);
		overflow:hidden;

		img{
			border:0;
			.border-r(50%);
		}
	}

	.award{
		.vm;
		.f(1.5rem);
		.fc(#ff5f53);
	}
}

.caption_title{
	.pr;
	.pt(10*@w);
	.w(600*@w);
	margin:0 auto;
	border-bottom:1px solid #e5e5e5;

	p{
		.pa;
		.c;
		.w(10rem);
		left:50%;
		.ml(-5rem);
		.bgc(#fff);
		bottom:-.7rem;
		.fc(#595056);
		.f(1.3rem);
		.l(1.3rem)
	}
}

.caption_pic{
	margin:1.5rem 0 1.2rem;
}

.dotted_line{
	.pr;
	.w(100%);
	.pt(11*@w);
	margin:3rem 0 2.4rem;
	background-image:url("@{pic_url}/images/wap/activity/sign_mall/dotted_line.png");
	background-size:100%;

	p{
		.pa;
		.c;
		left:50%;
		.w(12rem);
		.ml(-6rem);
		.fc(#333);
		.f(1.5rem);
		.l(1.5rem);
		.bgc(#fff);
		bottom:-1.4rem;

		em{
			.fc(#ff525f);
			font-style:normal;
		}

		.caption_btn{
			.f(1.2rem);
			.fc(#999);
			text-decoration:underline;
		}
	}
}

.banner_box{
	.hidden;
	margin:0 1rem;

	li{
		.left;
		.w(100%);
		.mt(1rem);
	}
}

.recommend_box{
	.hidden;
	margin:0 1rem 1rem;

	li{
		.left;
		.w(48.3%);
		.mt(1rem);
	}

	.odd{
		li:nth-child(odd){.mr(3.4%);}
	}
}

.new_box{
	.hidden;
	.bgc(#fff);
	margin:0 1rem;

	div.lazy_pic{
		opacity:.75;
		.w(100%);
		.pt(100%);
		background-image:url("@{pic_url}/images/wap/activity/sign_mall/default_pic.png");
		background-size:100%;
	}

	li{
		.left;
		.mr(0);
		.w(48.5%);
		.mb(2rem);

		p{
			.pl(2*@w);
			.pr(2*@w);
			.mt(.8rem);
		}

		.price{
			.f(1.2rem);
			.l(1.8rem);
			.fc(#ff6699);

			del,span{
				.ml(.4rem);
			}

			del{
				.fc(#c7c7c7);
			}

			span{
				.f(1.6rem);
			}
		}

		.coupon{
			.pr;
			.w(100%);
			.pt(46%);
			.mt(.2rem);
			background-image:url("@{pic_url}/images/wap/activity/sign_mall/coupon_bg_yes.png");
			background-size:100%;

			div {
				.c;
				.pa;
				.w(100%);
			}

			.tit1{
				top:10%;
				.f(1.8rem);
				.fc(#fff);
			}

			.tit2{
				top:41%;
				.f(1rem);
				.fc(#fffa75);
			}

			.tit2.robbed{
				.fc(#cec5c6);
			}
		}

		.coupon.robbed{
			background-image:url("@{pic_url}/images/wap/activity/sign_mall/coupon_bg_no.png");
		}

		.goods_title{
			.hidden;
			.f(1.4rem);
			.l(1.8rem);
			.fc(#666);
			text-overflow:ellipsis;
			display:-webkit-box;
			-webkit-line-clamp:1;
			-webkit-box-orient:vertical;
			clear:both;
		}
	}

	.even{
		li:nth-child(odd){.mr(3%);}
	}

	.odd{
		li:nth-child(odd){.mr(3%);}
	}
}

.ios_simple;
.pullUp{
	.pullUpDown > .common;
	.pullUpDown > .loading;
	&:before{.nowrap;.ml(-10px);}
	&[status="tap"]{background:0 none;}
	&[status="stop"]{
		background:0 none;.h(2px);
		&:before{
			content:"木有了...";.pb(2rem);
		}
	}
}

.gotop_wrap{
	.pf;
	.none;
	.w(82*@w);
	.pt(82*@w);
	z-index:10;
	right:10px;
	bottom:20px;

	.gotop{
		.block;
		.w(100%);
	}

	img{
		.w(100%);
	}
}

.error_msg{
	.none;
	.c;
	.f(1.2rem);
	.pt(20*@w);

	.error_reload{
		.c;
		.mt(1rem);
		.pb(20*@w);

		span{
			.i_block;
			.border-r(6px);
			border:solid 1px #595056;
			padding:.5rem 1.2rem;
		}
	}
}

