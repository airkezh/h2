}

.logistic_info {
	.padding;
	.pt(25*@px);
	.mt(0);
	.bgwhite;
	.logistic_li { 
		.pr; 
		border-left: 1px solid #d9d9d9; 
		.pb(20*@w); 
		.pl(30*@px);
		.time { .fc(#999999); border-bottom: 1px solid #d9d9d9; .pb(20*@w);}
