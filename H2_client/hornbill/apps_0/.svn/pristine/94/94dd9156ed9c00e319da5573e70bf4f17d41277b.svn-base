@import "block.less";
html{.f(10*@f); }
body{.bgc(@white);}
img{ width: 100%; .block; }
.navBan;
.header{.mb(42*@px)}
.down_{padding: 30*@px; .f(36*@px);
	textarea{.w(100%); .f(24*@px); padding: 6px; .border-1(#ccc);.border-r(4*@px);.bs; .h(148*@px);.mb(20*@px);}
	input{.common_input; .f(24*@px); /*.mb(40*@px);.mt(80*@px);*/}
	.reqErrorMessage{.fc(@red);.mt(20*@px);.c;.f(1.6rem);}
	.button{.common_button; .common_button > .redBtn; .f(36*@px); .mt(20*@px); .c; .border-n;.i_block; .w(100%);.pt(20*@px);.pb(20*@px);}
}
.br_(){border-right: 1px solid #ddd;}
.bb_(){border-bottom: 1px solid #ddd;}
.bt_(){border-top: 1px solid #ddd;}
.ins_main{.hidden;
	.ins_box{ .bb_; .i_block; .c; .w(49.8%);.h(200*@w);.left;.bs;
		&:nth-child(odd){.br_;}
		&:nth-child(1){.bt_;}
		&:nth-child(2){.bt_;}
		.bg_img{.i_block;.w(132*@w); .pt(132*@w);  background: url("@{pic_url}/images/wap/uninstall/uninstal1_b.jpg") no-repeat; background-size: 200%;.mt(18*@px); .mb(16*@px);}
		.bg_1{.bp(0%,-1%);}
		.bg_2{.bp(100%,0%);}
		.bg_3{.bp(0%,49%);}
		.bg_4{.bp(100%,48%);}
		.bg_5{.bp(0%,98%);}
		.bg_6{.bp(100%,97%);}
		p{.f(28*@px);}
		i{font-style: normal; .fc(@gray);.block;.mt(8*@px);.mb(16*@px); }
	}
}

