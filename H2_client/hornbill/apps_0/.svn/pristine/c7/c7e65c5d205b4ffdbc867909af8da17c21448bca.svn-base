					display:-webkit-box;
					-webkit-line-clamp:1;
					-webkit-box-orient:vertical;
					clear:both;
				}

				&Price{
					.l(1.8);
					.i_block;
					.f(1.3rem);
					.fc(#ff4b42);

					i{
						font-style:normal;
					}

					.origPrice{
						.fc(#999);
						.f(1.2rem);
					}

					.word{ 
						.fc(#999);
						.f(1.2rem);
					}
				}

				&Discount{
					.l(2);
					.w(80%);
					.f(1.2rem);
					.fc(#ff9f17);
					margin:0 auto;
					.border-r(12px);
					.bgc(rgba(255,159,23,0.1));

					.bean{
						.pl(30*@px);
						background:url("http://d01.res.meilishuo.net/pic/_o/9d/a0/76a466d841d5156aa297b243d50f_22_22.cf.png") no-repeat;
						background-size:12px auto;
					}
				}
			}
		}
	}
