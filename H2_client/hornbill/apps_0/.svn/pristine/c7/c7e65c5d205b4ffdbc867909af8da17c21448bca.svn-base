/* 组图 */
.swipe{
	.c;
	.pr;
	.hidden;
	/*visibility:hidden;*/
	min-height:180*@px;
}

.swipe_wrap{
	.pr;
	.hidden;
	-webkit-backface-visibility:hidden;
	-moz-backface-visibility:hidden;
	-ms-backface-visibility:hidden;
	-o-backface-visibility:hidden;
	backface-visibility:hidden;
	/* Fix of Webkit flickering */
	z-index:1;
}

.swipe_wrap > div{
	.pr;
	.left;
	.w(100%);
}

.swipe_pic{
	.pr;
	.hidden;
	.w(100%);
	.bgc(#fff);
	min-height:180*@px;
}

.swipe_pic img{
	.w(100%);
	min-height:180*@px;
}

.swipe_num{
	.pa;
	left:0;
	.w(100%);
	bottom:3px;
	z-index:9;

	li{
		.vm;
		.w(4px);
		.h(4px);
		.mr(2px);
		.i_block;
		float:none;
		.bgc(#e2e2e2);
		.border-r(50%);
	}

	li.active{
		.bgc(#f66);
	}
}

.mall_wrap{
	.bgc(#f5f5f5);
}

.gallery_box{
	.hidden;
	.clearfix;
    .bgc(#fff);
    .mb(20*@w);
}

.gallery_3_row{

	li{
		.left;
		.w(33.3334%);

		a{
			border-right:1px solid #e6e6e6;
		}
	}

	li:nth-child(3){
		.w(33.3332%);

		a{
			border:none;
		}
	}
}

.gallery_4_row{

	li{
		.left;
		.mr(0);
		.w(50%);
	}

	li:nth-child(1),li:nth-child(2){
		border-bottom:1px solid #e6e6e6;
	}

	li:nth-child(odd){
		a{
			border-right:1px solid #e6e6e6;
		}
	}
}

