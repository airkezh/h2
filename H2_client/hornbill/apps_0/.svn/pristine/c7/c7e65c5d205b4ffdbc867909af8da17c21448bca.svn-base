				background-image:url("http://d01.res.meilishuo.net/pic/_o/b9/0a/fd965c6f94c2fd9df704e481098f_304_425.cg.png");
				/*background-image:url("http://d05.res.meilishuo.net/pic/_o/33/cf/a618bae2cdf43bc03289e82cb53f_290_290.cg.png");*/
				background-size:cover;
			}

			span.df_show_img{
				.block;
				.w(100%);
				.pt(140%);
				opacity:.75;
				background-image:url("http://d01.res.meilishuo.net/pic/_o/b9/0a/fd965c6f94c2fd9df704e481098f_304_425.cg.png");
				/*background-image:url("http://d05.res.meilishuo.net/pic/_o/33/cf/a618bae2cdf43bc03289e82cb53f_290_290.cg.png");*/
				background-size:cover;
			}

			div.new_png{
				.pa;
				.w(100%);

				div{
					.left;
					.ml(6/290*100%);

					img{
						.border-r(0 0 0 0);
					}
				}
			}

			.lave{
				.pa;
				.c;
			    .w(100%);
			    .fc(#fff);
			    .f(1rem);
			    bottom:0;
			    z-index:90;
			    padding:4px 0;
		    }

		    .progress{
		    	.pa;
				.block;
				.f(10px);
				.w(100%);
				.h(12px);
				bottom:0;
				opacity:.6;
				padding:4px 0;
		    }

		    .rpbgc{
		    	.bgc(#dd8200);
		    }

		    .gpbgc{
		    	.bgc(#80737b);
		    }

		    .ypbgc{
		    	.bgc(#ffb414);
		    }
		}

		figcaption{
			.c;
			.bgwhite;
			padding:8px 0 16px;

			.pro{
				&Title{
					.hidden;
					.l(1.4);
					.f(1.2rem);
					.fc(#666);
