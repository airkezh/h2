@import "../block.less";
@import "../atom.less";
@import "../page_guang.less";
.banner{ color:white; .h(155px);
	div{ .w(1200px); .auto; overflow:hidden;
		h1{ float:left; .pl(130px); font-size:48px; .pt(30px); }
		p{ float:right; .i_block; .pt(50px); font-size:24px; .pr(20px); }
	}
	h1{}
}
.t_main{.bgwhite; .w(1186px);border:1px solid #e6e6e6;.auto;.pr;
	#left,#right{
		.i_block;
		.h(300px);
		.w(11px);
		background:url(http://d06.res.meilishuo.net/img/_o/8f/a1/8dea7d5a84fff3347a85311212c3_11_21.c8.png) center center no-repeat;
		.cursor;
		.pa;
		top:50px;
		left:15px;
	}
	#right{
		left:auto;
		right:15px;
		background:url(http://d06.res.meilishuo.net/img/_o/75/d1/dbfc0dd26a196c3e211bc31fa8aa_11_21.c8.png) center center no-repeat;
	}
	.title{ font-size:18px; font-weight:bolder; border-top:2px solid @red; .w(81px); .l(36px); .ml(7px); }
	.t_center{.w(1114px);.pr;overflow:hidden;.auto;.h(445px);.pt(10px);.hidden;.bgc(#fff);
		ul{
			.pa;
		}
		ul li{.pr;.w(204px);.h(420px);.ml(9px);.mr(9px);.bgc(#fff);.left;
			.t_price{.h(48px);.l(48px);.border-bd2(dashed);.w(190px);.hidden;
				span{.fc(#ff6b9b);.f(36px);}
				i{font-style: normal;.fc(#ff6b9b);.f(24px);}
				em{text-decoration: line-through;.ml(6px);}
			}
			.t_num{.h(38px);.l(38px);.f(18px);
				em{.f(24px);}
			}
			.t_btn{.i_block;.w(204px);.h(42px);.l(42px);.fc(#fff);.c;.f(18px)}
			.orange{.bgc(#ff9a00)}
			.pink{.bgc(#ff679b)}
			.lightpink{.bgc(#ffc1d6)}
			.mask{.w(204px);.h(288px);.pa;left: 0;top: 0;
				.bgc,.bg_img{.block;.w(100%);.h(100%);.pa;left: 0;top: 0;}
				.bgc{.bgc(#000);.opacity(60);}
				.bg_img{background:url("@{pic_url}/images/biz/phonepriv/st_sellover.png") no-repeat center center;}
				
			}
			.none{.none;}
		}
	}
}
