<%# head_simple.html%>
</head>
<body>
<div class="banner" id="mm">
	<a class="winners" href="http://m.meilishuo.com/activity/four_years/prepare/?cid=2085"></a>
</div>
<div class="shortPoster">
	<div class="t1">今天的门票还剩<span class="leftAccount">-</span>张</div> <div class="t2">机会还在等着你</div>
</div>
<div class="posterWall">
	<div class="poster">
		<div class="p" id="LUHAN" style="background: url('http://i.meilishuo.net/css/images/wap/activity/exo_ticket/luhan.jpg') no-repeat; background-size: 100%">
			<div>LUHAN</div>
		</div>
		<div class="p" id="XIUMIN" style="background: url('http://i.meilishuo.net/css/images/wap/activity/exo_ticket/xiumin.jpg') no-repeat; background-size: 100%">
			<div>XIUMIN</div>
		</div>
		<div class="p" id="LAY" style="background: url('http://i.meilishuo.net/css/images/wap/activity/exo_ticket/lay.jpg') no-repeat; background-size: 100%">
			<div>LAY</div>
		</div>
		<div class="p" id="CHEN" style="background: url('http://i.meilishuo.net/css/images/wap/activity/exo_ticket/chen.jpg') no-repeat; background-size: 100%">
			<div>CHEN</div>
		</div>
		<div class="p" id="TAO" style="background: url('http://i.meilishuo.net/css/images/wap/activity/exo_ticket/tao.jpg') no-repeat; background-size: 100%">
			<div>TAO</div>
		</div>
		<div class="p" id="EXO-M" style="background: url('http://i.meilishuo.net/css/images/wap/activity/exo_ticket/exom.jpg') no-repeat; background-size: 100%">
			<div>EXO-M</div>
		</div>
	</div>
</div>

<div class="findPoster">
	<div class="showIntro">
	</div>
</div>
<div class="share_con">
	<% if(this.share){ %>
	<%= requireFn('wapShare.html')({
	'share' : this.share
	, 'shareTitle' : '选择分享到'
	, '_JSstack' : this._JSstack
	, '_JSmods' : this._JSmods
	}); %>
	<%}%>
</div>
<div class="findPoster_b" style="width: 100%; padding-top: 14%; background: #ff6699"></div>
<script type="text/html" id="guide">
	<div class="shade"></div>
	<div class="guide">
		<div class="top">

			<div class="close"></div>
		</div>
		<div class="guideBtn"></div>
	</div>
</script>

<script type="text/html" id="repeat">
	<div class="shade"></div>
	<div class="repeat">
		<div class="r_top"></div>
		<div class="r_poster">
			<img src="http://i.meilishuo.net/css/images/wap/activity/exo_ticket/test.png" alt=""/>
		</div>
		<div class="r_btn"></div>
	</div>

</script>

<script type="text/html" id="new">
	<div class="shade"></div>
	<div class="new">
		<div class="n_top"></div>
		<div class="n_poster">
			<img src="http://i.meilishuo.net/css/images/wap/activity/exo_ticket/test.png" alt=""/>
		</div>
		<div class="n_btn"></div>
	</div>
</script>

<script type="text/html" id="good">
	<div class="shade"></div>
	<div class="good">
		<div class="g_info">

		</div>
		<div class="g_btn"></div>
	</div>
</script>

<script type="text/html" id="nullity">
	<div class="shade"></div>
	<div class="nullity">
		<div class="nu_info">
			这张海报已经被别人抢走了
		</div>
		<div class="nu_btn"></div>
	</div>
</script>

<script>
	fml.vars.isLogin = <%=this.isLogin%>;
	var posters = JSON.parse('<%=JSON.stringify(this.posters.data)%>');
	var leftAmount = <%=this.posters.left_amount%>;
	//var a = {"EXO-M": "http://d04.res.meilishuo.net/img/_o/41/78/32ef6ca184920a116bc6fd3bdae7_600_771.c8.jpg"};
	//posters.push(a);
</script>
