	.vote(){
		.pr;
		&:before{
			content:"";
			.pa;left:-30*@px;top:-3px;.h(24*@px);.w(24*@px);
			background:url("@{pic_url}/images/wap/activity/sale_vote/zan.png") no-repeat;
