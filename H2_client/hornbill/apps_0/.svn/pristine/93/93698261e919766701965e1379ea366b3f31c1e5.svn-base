		.mt(.06rem);
		.l(.56rem);
		.white;
		.border-r(.06rem);
		.bgred;
	}
	.shop-performance {
		.f(.24rem);
		.clearfix;
		li {
			.left;
			.mr(.18rem);
		}
	}
}
