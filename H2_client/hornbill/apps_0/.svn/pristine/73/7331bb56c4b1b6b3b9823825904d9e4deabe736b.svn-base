/*撕名牌样式*/
.cover,.cover_left{
    transition:all 0.09s ease 0s;
    -webkit-transition:all 0.09s ease 0s;
}
.brand_wrap{
       background: url("@{pic_url}/images/wap/activity/crackbrand/index_luh_brand.png");
       background-size:100%;
       box-shadow: 0 0 11px rgba(0,0,0,0.5);   
    .cover{
       position:absolute;
       right:0px;
       height: 100%;
        .cover_left{
                position:absolute;
                left:0px;
                height: 100%;
                width:50%;
                background: white;
                opacity:0.95;
                -webkit-box-shadow: 2px 2px 10px #909090;
                z-index:100;
                -webkit-transform-origin: top right;
            .cover_color{
                position: absolute;
                left:0px;
                width: 100%;
                height:100%;
                opacity:0.66;
                overflow: hidden;
            }
            .cover_img{
                width:4rem;
                position: absolute;
                background: url("@{pic_url}/images/wap/activity/crackbrand/index_luh_brand_fan.png");
                background-size:100%;
                height: 100%;
            }
            .cover_black{
                position: absolute;
                right:0px;
                width: 30%;
                height:100%;
                background: -webkit-gradient(linear, right top, left top, from(rgba(0, 0, 0, 0.80)), to(rgba(0,0,0,0)));
            }
        }  
        .cover_right{
            position:absolute;
            right: 0px;
            height: 100%;
            width:50%;
            background: #fff;
            .cover_bgc{
                position:absolute;
                right: 0px;
                height: 100%;
                width:100%;
                background: -webkit-gradient(linear, right top, left top, from(rgba(0, 0, 0, 0)), to(rgba(0,0,0,0.18)));
            }
        }
    }
}
/*查看规则*/
.first_rules{
    .pa;
    top:0;
    right:0;
    .w(25%);
    .pt(12%);
    z-index: 100;
}
.first_popwin{
        .none();
        z-index:103;
        position: fixed;
        top: 0;
        left: 0;
        bottom: 0;
        right: 0;
        background-color: rgba(0,0,0,.6);
        .first-popwin-rules{
            .w(5.82rem);
            .h(6.22);
            .pa();
            top: 1.5rem;
            left:0;
            right: 0;
            margin: auto;
        }
        .rules-content{
            .w(5.2rem);
            .h(4rem);
            .pa();
            top: 2.5rem;
            left: 0;
            right: 0;
            margin: auto;
            .f(0.28rem);
            .fc(#000000);
            overflow: auto;
            .l(0.3rem);
            img{
                .w(100%);
                .block;
            }
        }
        .first-popwin-know{
            .w(33%);
            .pa();
            top: 6.6rem;
            left: 50%;
            transform:translateX(-50%);
            -ms-transform:translateX(-50%);     /* IE 9 */
            -moz-transform:translateX(-50%);    /* Firefox */
            -webkit-transform:translateX(-50%); /* Safari 和 Chrome */
            -o-transform:translateX(-50%);  /* Opera */
        }
    }
    .share-popwin{
        .none();
        .pa();
        top: 0;
        left: 0;
        bottom: 0;
        right: 0;
        .pt(60px);
        background-color: rgba(0,0,0,.6);
        z-index: 999;
        img{
            .w(100%);
            .pa();
            top: 0.2rem;
            right: 0;
        }
    }
