			data.pageTitle = keywords + '【多图】-' + keywords + '新款女装价格，品牌服饰购买及搭配图片-美丽说';
			data.keywords = keywords+'，'+keywords + '新款,'+keywords + '女装,'+keywords + '搭配,'+ '图片,'+ keywords + '购买,'+keywords + '淘宝,'+keywords+'价格';
