// 轮播
.imageSlide {
  border: 0px solid black;
  position: relative;
  overflow: hidden;
  font-size: 0;
  ul {
    position: absolute;
    li {
      position: relative;
      float: left;
      display: inline;
      img {
        .w(100%);
        .h(100%);
      }
    }
  }
  .num {
    .c;
    position: absolute;
    .w (100%);
    left: 0;
    bottom: 40px;
    z-index: 10;
    b {
      .i_block;
      .w (6px);
      .h (6px);
      margin: 6px 8px 0 0;
      .bgc(#fff);
      -webkit-border-radius: 100%;
      border-radius: 100%;
    }
    .cur {
       background: transparent;
      border: 1px solid #fff;
      .w(5px);
      .h(5px);
      border-radius: 50%;
    }

  }
  .timeOver{
      background: rgba(255,255,255,.7);
      .h(30px);
      .l(30px);
      .w(100%);
      .f(12px);
      .c;
      position: absolute;
      left:0;
      bottom: 0;
      z-index: 11;
      .fc(#333333);
      span{
          .fc(#ff6666);
      }
      strong{
          .fc(#ff6666);
      }
  }
}
/*pullUp style*/
.pullUp{
    .pullUpDown > .common;.pullUpDown > .loading;
    &:before{
        .nowrap;
        .ml(-10px);
    }
    &[status="tap"]{
        background:0 none;  
    }
    &[status="stop"]{
        background:0 none;  
    }
}
.gotop{
    .w(12.5%);
    .none;
    right:20px;
    position:fixed;
    z-index: 100;
}


