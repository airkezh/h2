@import "../block.less";
body{
    .bgc(#f8f8f8);
    font-family: 'Arial';
}
.main {
  .w(100%);
  .auto;
  .f(.875em);
}
@media screen and (min-width: 640px) {
  .main {
    .w(640px);
    }
 }
.pink{
    .bgc(#ff7a9a);
    .fc(#ffffff);
}
.pink:hover{
    .fc(#fff);
}
.pink:active{
    background: rgba(255,122,154,.7);
}
.gray{
    .bgc(#cccccc);
    .fc(#333333);
}
.gray:active{
    background: rgba(204,204,204,.7);
}
.yellow{
     .bgc(#fbe603);
     .fc(#000);
}
.yellow:hover,.gray:hover{
    .fc(#000);
}
.white{
    .bgc(#fff);
    .fc(#CCCCCC)!important;
}
.white:hover{
    .fc(#CCCCCC)!important;
}
.wrapper{
    .pb(60px);
}
.wlf_wrapBox{
    .mb(10px);
    .bgc(#fff);
    .pb(10px);
}

.title,.wlf_price,.wlf_desc{
    .c
}
.title{
    .h(30px);
    .l(30px);
    .f(14px);
    .fc(#333333);
    font-weight:100;
}
.wlf_price{
    .h(35px);
    .f(18px);
    .t-through;
    .fc(#ff6666);
}
.wlf_desc{
    .auto;
    .l(14px);
    .w(90%);
    .f(11px);
    .fc(#666666);
}
      p.wlf_applyNo {
        height: 20px;
        .f(12px);
        .fc(#999);
        .c;
        span:first-child {
          margin-right: 10px;
        }
      }
.wlf_title{
   .mb(15px);
    .h(40px);
    .l(40px);
    .fc(#666666);
    .f(13px);
    font-weight: normal;
    .c;
    span{
        .block;
        .w(96%);
        .ml(4%);
         border-bottom: 1px solid rgba(51,51,51,.1);
    }
}
.shop_title{
    text-align: left;
    text-indent: 10%;
    .pr;
    color: #ff7a9a!important;
    background: url("@{pic_url}/images/m_welfare/wlf_shop_icon.png") center left no-repeat;
    background-size:16px 14px;
    background-position:4% 12px;
    .arrow{
        .pa;
        top:13px;
        right: 4%;
        .w(7px);
        .h(13px);
        color: #CCCCCC;
        background: url("@{pic_url}/images/m_welfare/wlf_shop_arrow.png") center left no-repeat;
        background-size:100% 100%;
    }
}
.wlf_linkDesc{
    .block;
    .w(85px);
    .h(30px);
    .l(30px);
    .fc(#ff7a9a);
    .c;
    .f(12px);
    .auto;
    cursor:pointer;
     //background: url("@{pic_url}/images/m_welfare/wlf_icon_arrowGR.jpg") center right no-repeat;
    // background-size:6px 10px;
}
.wlf_linkDesc:hover{
    text-decoration: underline;
}
/*product_list style*/
.goods_wall{
    .w(96%);
    .auto;
    .clearfix;
}
.productBox{
    .left;
    .w(50%);
    .link_box{
        .block;
        margin:0 5px 10px 5px;
    }
    figure{
        .pr;
        span.bs_load_img{
            .block;
            opacity:.75;
            .w(100%);
            .pt(140%);
             background-image: url("@{pic_url}/images/huodong/tuan/pre1.png");
             background-size: cover;
        }
    }
     figcaption {
            padding: 5px;
            .bgwhite;
            .f(14px);
            .proTitle{
                .h(25px);
                .l(25px);
                .f(11px);
                .fc(666666);
                .hidden;
                text-overflow: ellipsis;
                display: -webkit-box;
                -webkit-line-clamp: 1;
                -webkit-box-orient: vertical;
            }
            .proPrice{
                font-weight: normal;
                .pt(4px);
                .price{
                    .f(11px);
                    .fc(#ff7a9a);
                    .mr(10px);
                    strong{
                        font-weight: 100;
                    }
                }
                .collect{
                    .pl(13px);
                    .f(11px);
                    .fc(#999999);
                     background: url("@{pic_url}/images/m_welfare/wlf_heart.jpg") center left no-repeat;
                     background-size:9px 9px;
                }
            }
           
        }
}
.wlf_apply{
      position: fixed;
      bottom: 0;
      left: 0;
      width: 100%;
      background: #fff;
      padding-top: 10px;
      border-top: 1px solid rgba(51,51,51,.1);
      padding-bottom: 10px;
      font-size: 15px;
      .apply_btn{
          .block;
          .h(35px);
          .l(35px);
          .w(90%);
          .f(14px);
          .c;
          font-weight:100;
          margin:0 auto;
          border-radius: 4px;
      }
}
.wlf_process{
    .w(100%);
    img{
        .block;
        .auto;
        .w(80%);
    }
}
.userList{
    .clearfix;
