        .last_step{
            .w(100%);
            .pt(177px);
            .scroll_box{
                .auto;
            }
            a.prev,a.next{
                .w(38px); 
                .h(68px); 
                .pa; 
                top: 379px; 
                .block; 
                z-index: 101;
             }
            a.prev{
                left:85px; 
            }
            a.next{
                right:89px;
            }
         }
         .scroll_mark{
             .w(100%);
             .h(20px);
             .mt(35px);
             text-align: center;
             i{
                 .mr(5px);
                 display: inline-block;
                 .w(17px);
                 .h(17px);
                 background: url("http://d05.res.meilishuo.net/pic/_o/6c/ca/f39ea8b7911564a1463528e9e581_17_17.ch.png") no-repeat;
             }
