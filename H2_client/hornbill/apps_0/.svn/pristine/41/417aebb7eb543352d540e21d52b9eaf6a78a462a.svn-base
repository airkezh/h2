}
.scroll_box{
    .w(945px);
    .pr;
    .h(477px);
    .bgc(#ffffff);
    .arrow{
        .w(115px); 
        .h(115px); 
        .pa; 
        z-index: 100; 
        left: -5px; 
        top: -5px; 
        background: url('http://d03.res.meilishuo.net/pic/_o/d7/0d/bee9e3b066471b60865f82bd15d1_115_115.cj.png') no-repeat left top;}
        .scroll_bar{
            .w(100%); 
            .h(477px); 
            .pr; 
            .auto; 
            .hidden;
        ul{
            .h(477px);
          }
        li{
            .pa;
            top:0;
            left: 0;
            .w(100%); 
            .h(477px); 
            img{
                .block; 
                .w(100%); 
                .h(477px);
               }
        }
    }

}
.yybar{
    .w(710px); 
    .h(430px); 
    .bgc(#fff); 
    .pt(25px); 
    .pr;
    .close{
        .block; 
        .w(26px); 
        .h(26px); 
        .pa; 
        right:20px; 
        top: 20px; 
        background: url('http://d03.res.meilishuo.net/pic/_o/10/4e/f2a21e93ce4db5a399f3a712856b_26_26.cf.png') no-repeat;
        }
    .titleImg{
        .block;
        .auto;
    }
    .yy_nr{
        .w(560px);
        .auto; 
        .pt(23px);
        h3{
            .fc(#5a5a5a); 
            .f(12px); 
            .pb(40px);
            }
        .submit_box{
            .c; 
            .mt(50px); 
            .mb(10px);
            }
        .erro_tip{
            .c; 
            .fc(#c90028); 
            .f(16px);}
        dl{
            .clearfix; 
            .mb(20px);
            .fc(#0d0d0d); 
            .f(14px);
            dt{
                .fc(#0d0d0d); 
                .f(14px);
                .left; 
                .l(24px); 
                text-align: right; 
                .red{
                    .fc(#5a5a5a);
                    }
            }
            dd{
                .fc(#0d0d0d); 
                .f(14px);
                .w(418px);
                .left; 
                .clearfix;
                .input1{
                    .h(24px); 
                    .l(24px); 
                    .w(122px); 
                    .pl(10px); 
                    .f(14px);
                    .fc(#5a5a5a); 
                    .mr(4px);
                    border-radius: 4px; 
                    border: solid 1px #CCCCCC; 
                &::-webkit-input-placeholder { .fc(#aaabb0); }
                &::-moz-placeholder { .fc(#aaabb0);  }
                }
                .select_box{
                    .h(24px); 
                    .left; 
                    .w(122px); 
                    .pl(10px); 
                    .mr(4px); 
                    border: solid 1px #d9d9d9; 
                    border-radius: 4px;
                    .pr;
                    p{
                        .fc(#aaabb0); 
                        .l(24px); 
                        .h(24px); 
                        .f(14px); 
                        cursor: pointer; 
                        .hidden;
                        }
                    .slect_arrow{
                        .block; 
                        .w(24px); 
                        .h(24px); 
                        z-index: 0; 
                        .pa; 
                        right: 0; 
                        top: 0; 
                        background: url('http://d05.res.meilishuo.net/pic/_o/56/77/6a6a1f50e5ddce7276a67e16d9d3_24_24.ch.png') no-repeat;
                        }
                    .show_option{
                        .pa; 
                        .w(100%); 
                        .h(150px); 
                        .hidden; 
                        overflow-y: auto; 
                        .bgc(#e5e5e6); 
                        left: -2px; 
                        top: 24px; 
                        display: none;
                        li{
                            .pt(5px); 
                            .pb(5px); 
                            .l(20px); 
                            .pl(12px); 
                            .f(14px); 
                            cursor: pointer;
                        &:hover{
                            .bgc(#c90028); 
                            .fc(#fff);}
                        }
                    }
                }
                .radio_box{
                    .clearfix; 
                    .f(14px);
                    .fc(#5a5a5a);
                    .sbl{
                        .w(124px); 
                        .left; 
                        .l(24px); 
                       }
                    .sbr{
                        .w(100%); 
                        .hidden;
                        ul{
                            .w(270px);
                            float: left;
                            .clearfix;
                            li{
                                .w(135px);
                                .left;  
                                .h(24px);
                                .l(24px);
                                .mb(4px);
                                cursor: pointer;
                                i{
                                    display: inline-block; 
                                    .mr(7px); 
                                    .vm; 
                                    .w(15px); 
                                    .h(15px); 
                                    border: 1px solid #CCCCCC;
                                    border-radius: 50%;
                                &.active{
                                    .w(18px);
                                    .h(18px);
                                    background: #ff746e;
                                    border: none;
                                    }
                                }
                                input{.none;}
                            }
                        }
                    }
                }
            }
        }
    }
}
