@import "storeAtom.less";
@bds:1px solid #e6e6e6;@bdsl:1px solid #ebeaea;.nav_bar;.datepicker;
.content{border:@bds;}
;
.tabs{.clearfix();.bgc(#f7f6f6);
	.menu{.clearfix();.left();.bgc(#ffffff);
		.un_selected{.left;.w(100px);height:48px;text-align:center;cursor:pointer;background:#f7f6f6;line-height:48px;font-size:15px;border-bottom:@bdsl}
		.selected_pannel{float:left;border-top:3px solid #ff6699;border-right:@bds;.left;.w(100px);.h(45px);text-align:center;cursor:pointer;.bgc(#ffffff);.l(45px);font-size:15px;}
		.last{.w(579px)}
	;}
}
table,th,td{.tl();.pt(8px);.pl(10px);border:@bds}
th{border-left:@bds;font-family:Arial;font-style:normal;font-weight:normal}
thead{.bgc(#f7f6f6);
	tr{.h(45px)}
}
table{.w(100%);
	.first{border-left:none}
	;
	.last{border-right:none}
}
tbody tr{.h(32px);border-top:@bds;
	td{border-left:@bds;}
}
.main{.bgc(#ffffff);.p20();.clearfix();
	.table_title{font-size:15px;.mb(10px);}
	.main_latest_pannel{.main_latest_head{.bgc(#f7f6f6);}
	}
	.radio_button{.mt(20px);font-size:14px;
		input,label{vertical-align:middle;font-family:verdana}
		;
		#radio_mob{.ml(20px)}
	}
	.chart_wrap{.mt(20px);
		.date_picker{.none;
			span{.ml(10px);}
			input{.w(118px);.h(14px)}
			.ensure_time{text-align:center;.l(20px);.ml(20px);.w(50px);.h(20px);.il-block();color:(#ffffff);.bgc(#ff6597);.cursor();}
		}
		;
		.chart_tabs{.clearfix()}
		;
		ul li{.ml(10px);.h(25px);.l(25px);font-size:12px;.p05();.left();.cursor();}
		.selected{.h(23px);border-left:@bds;border-right:@bds;border-top:@bds;.pr();.bgc(#ffffff);z-index:10;}
		.chart_pannel{.pr();.p10();border:@bds;top:-2px;z-index:5;}
	}
	.part_first_table,.part_second_table{.mt(20px);}
	.mob_table{.none();}
}
.clear{clear:both;}
.main_pannel{height:100px;width:500px;background:#FFFFff;}
#mask_div{.none();.bgc(#000);filter:alpha(opacity=10);opacity:.1;left:0px;top:0px;position:fixed;_position:absolute;.w(100%);.h(100%);overflow:hidden;z-index:10000;}