<div class="pop_win none_f">
	<div class="win_content">
		<h3>当前预测最多的最强者</h3>
		<table class="rank_table">
			<% 
				this.runner_shake_init.rank = this.runner_shake_init.rank || [
					{
					vote: 1,
					name: "Angelababy",
					voteId: "runner2"
					},
					{
					vote: 1,
					name: "邓超",
					voteId: "runner4"
					},
					{
					vote: 1,
					name: "鹿晗",
					voteId: "runner1"
					}
				];
			%>
			<% this.runner_shake_init.rank.forEach(function(item,index){%>
			<% if(index>2) return; %>
			<tr>
				<td><%=index+1 %></td>
				<td><img class="head_icon" src="<%=runnerHeadInfo[item.voteId] %>"></td>
				<td><%=item.name %></td>
			</tr>
			<%});%>
		</table>
		<!-- <label class="focus_label">
			<span class="i_block_f">关注美丽说公众号 关注</span>
