.member_table_wrap{
	.w(100%);
	margin:65px auto 0;

	.table_title{
		.c;
		.auto;
		.f(20px);
		.fc(#333);
		.w(900px);
	}

	.table_box{
		.auto;
		.hidden;
		clear:both;
		.mt(30px);
		.w(900px);

		li{
			.c;
			.w(100%);
			.h(45px);
			.f(12px);
			.fc(#666);
		}

		li:nth-child(even){
			.bgc(#fff);
		}

		li:nth-child(odd){
			.bgc(#f7f7f7);
		}

		li.title{
			.f(14px);
			.bgc(#fcdbd6);
		}

		p{
			.l(15px);
			margin:15px 0;
		}
	}

	.level_rule{
		div{
			.left;
			.w(14%);
		}

		div:nth-child(2){
			.w(36%);
		}

		div:nth-child(3){
			.w(50%);
		}
	}

	.member_interests{
		div{
			.left;
			.w(15.6%);
		}

		div:nth-child(1){
			.w(22%);
		}

		div.two{
			p{
				margin:5.5px 0 2px;
			}

			p:nth-child(2){
				margin:2px 0 5.5px;
			}
		}
	}

	.beauty_detail{
		li{
			.h(90px);

			p{
				.l(15px);
				margin:37.5px 0;

				a,a:hover{
					.fc(#fe6054);
				}
			}

			p.icon{
				.w(70px);
				.h(70px);
				margin:10px auto;
			}

			.left{
				.tl;
				.hidden;
				white-space:nowrap;
            	text-overflow:ellipsis;
