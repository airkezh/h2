@import "../block.less";
@import "../biz/clearance/cl_base.less";

.slide_banner{ .pr;.h(2.4rem); .mb(.2rem); }
.imageSlide-rem;
.fallwall-rem;
.pullUp{ clear: both;.mt(1.8rem); }
.gotop{
	.w(12.5%);
	.none;
	right:20px;
	position:fixed;
	z-index: 100;
}

body{
	.w(6.4rem);
	min-width: 320px;
	.bgc(#f8f8f8);
}
img{
	.w(100%);
	.block;
}
.act_area{
	.mb(.2rem);
	.bgwhite;
	.hidden;
	&_tit{
		padding: .15rem .1rem 0.15rem .2rem;
	}
	&_txt{
		.f(.24rem);
	}
	&_desc{
		.right;
		.fc(#ff7a9a);
		.f(.2rem);
		.mt(.04rem);
	}
}
.pin_banner{
	&_cont{
		padding: 0 .05rem .4rem;
		.clearfix;
		.img_list{
			.left;
			padding: .05rem;
			.w(3.05rem);
			.h(1.5rem);
			.hidden;
		}
	}
}
.today_buy{
	.clock{
		.i_block;
		.mr(.04rem);
		.w(.14rem);
		.h(.15rem);
		background: url("http://d03.res.meilishuo.net/pic/_o/f4/5b/252ed6de6ef06059117c3d921aa8_14_15.cg.png") no-repeat;
		background-size: contain;
	}
	.pin_banner_cont{
		.block;
		.pl(.1rem);
		.pr(.1rem);
		.pr;
	}
	&_item{
		.left;
		.w(3.1rem);
		.h(3.6rem);
		.flexbox;
		.item_info{
			.hidden;
			.flex(1);
			border: 1px solid #d2d2d2;
			.c;
			.feature_img{
				.mt(.3rem);
				.mb(.16rem);
				.i_block;
				.w(2.24rem);
			}
			.feature_txt{
				.f(.26rem);
			}
			.reason{
				.w(98%);
				margin: .1rem auto .3rem;
				.f(.2rem);
			}
			.price_box{
				.o_price{
					.fc(#ff7a9a);
				}
				.n_price{
					.fc(#999);
				}
			}
			.buy_btn, .soldout_btn{
				.i_block;
				.w(1.18rem);
				.h(.4rem);
				.l(.4rem);
				.mt(.16rem);
				.white;
			}
			.buy_btn{
				.bgc(#ff7a9a);
			}
			.soldout_btn{
				.bgc(#666);
			}
		}
	}
	.today_buy_link{ .i_block; .w(3.1rem); .h(3.6rem);}
}
.pop_recommend{
	.goods_wrap{ .mb(.4rem); .h(3.5rem); .hidden;
		.goods_iscroll{ .clearfix;}
		.pop_item{
			.left;
			.w(2.21rem);
			.h(3.5rem);
			.ml(.1rem);
			.pr;
		}
		.goods_desc{
			.pa;
			bottom: .16rem;
			left: .15rem;
			.w(1.9rem);
			.h(.84rem);
			background: url("http://d02.res.meilishuo.net/pic/_o/fb/63/b056c6421792fb25e3f36acf8bea_190_84.cg.png") no-repeat;
			background-size: contain;
			background-color: rgb(197,130,91);
			opacity:0.95;
			.white;
			.c;
			.goods_tit{
				.mt(.24rem);
				.f(.21rem);
				.ellipsis;
			}
			.goods_price{
				.mt(.06rem);
				.f(.22rem);
			}
		}
		.border_type{
			.h(.38rem);
			border: 1px solid #e5e5e5;	
			border-top: none;
		}
	}
}
::-webkit-scrollbar{
	width:0;
	height:0;
}
.nav_wrap{
	.mt(.2rem);
	.bgwhite;
	.hidden;
	.nav{
		.bgwhite;
		.pt(.06rem);
	}
	.nav_title{
		padding: .2rem .1rem 0.2rem .2rem;
		border-bottom: 1px solid #d2d2d2;
	}
	.nav_txt{
		.f(.24rem);
	}
	.nav_desc{
		.right;
		.fc(#ff7a9a);
		.f(.2rem);
		.mt(.04rem);
	}
	.nav_content{
		.bgwhite;
	}
	.tab_head{
		.h(.6rem);
		.l(.6rem);
		.f(.24rem);
		.pl(.1rem);
		.pr(.1rem);
		border-bottom: 2px solid #f8f8f8;
		//.hidden;
		.tab_cont{
			.clearfix;
			.w(9.2rem);
			overflow: hidden;
			//overflow-x: scroll;
		}
		.tab_all{
			position:relative;
			z-index:100;
		}
		.tab_more{
			.bgwhite;
		}
		.tab_scroll{
			.left;
			//overflow-x: scroll;
			overflow: hidden;
			.w(4.6rem);
			position:relative;
			z-index:99;
			
		}
		.tab{
			.left;
			.w(.92rem);
			.c;
			a{
				.i_block;
			}
		}
		.active a{
			border-bottom: 2px solid red;
		}
		.tab_more{
			.left;
			.tab_arrow{
				.i_block;
				.h(.24rem);
				vertical-align: middle;
				border-left: 1px solid #ccc;
				.pl(.23rem);
				.pr(.23rem);
				.l(.24rem);

				
			}
			.tab_arrow::before{
				.i_block;
				content: '';
				.w(.2rem);
				.h(.12rem);
				background: url("http://d05.res.meilishuo.net/pic/_o/09/e9/de1135241532599942218cad279a_20_11.cg.png") no-repeat;
				background-size: contain;

			-webkit-transition: -webkit-transform .4s;
				-webkit-transform: rotate(0deg);
			}
		}
		.tab_more_show .tab_arrow:before{
			-webkit-transform: rotate(180deg);
		}
	}
	.tab_body{
		.none;
		.pa;
		background-color:rgba(255,255,255,0.95);
		.w(100%);
		z-index: 999;
    	margin-top: -.7rem;
		ul{
			.clearfix;
			.pt(.1rem);
			.pb(.1rem);
			border-bottom: 2px solid #d2d2d2;
		}
		li{
			.left;
			.w(1rem);
			.h(.4rem);
			.l(.4rem);
			.c;
		}
	}
	.range{
		.w(100%);
		ul{ .clearfix;}
		li{ .w(20%); .left; .mt(.16rem); .mb(.4rem); .c; .f(.22rem);}
		a{ 
			.block;
			border-right: 1px solid #d2d2d2;
		}
		li.active a{
			.fc(#ff7a9a);
		}
		.arrow_up, .arrow_down{
			.i_block;
			.w(.12rem);
			.h(.18rem);
			.ml(.08rem);
		}
		.arrow_up{	
			background: url("http://d05.res.meilishuo.net/pic/_o/b5/08/909d771eb1ccbd9280bd9541ef7d_12_18.ch.png") no-repeat;
			background-size: contain;
		}
		.arrow_down{
			background: url("http://d05.res.meilishuo.net/pic/_o/cd/31/9bec4a1dd9a47470a42c952dbc93_12_18.cf.png") no-repeat;
			background-size: contain;
		}
	}
}
.goods_wall{
	.ml(.05rem); .mr(.05rem);
	.clearfix;
	.bgwhite;
	.productBox{
		.left;
		.w(3.06rem);
		margin: 0 .04rem .2rem .05rem;
		.f(.22rem);
		.link_box{
			.block;
		}
		figure{
			.pr;
			z-index: 10;
			.zeroflag{ 
				z-index: 11;
				.pa;
				top: 25%;
				left: 25%;
				.w(50%);
				background: no-repeat center;
				background-size: 120/270*100%;
			}
			.ico_png{
				.pa;
				left: .08rem;
				top: 0;
				.w(.6rem);
				.h(.7rem);
				z-index: 12;
			}
			.new_png{
				background: url("http://d04.res.meilishuo.net/pic/_o/3b/b3/746fd1f5f9f78241c62f18390f28_60_70.ch.png") no-repeat;
				background-size: cover;
			}
			.hot_png{
				background: url("http://d01.res.meilishuo.net/pic/_o/be/a4/e363b46ee8c1da8e70cc108f09e6_60_70.ch.png") no-repeat;
				background-size: cover;
			}
		}
		span.bs_load_img{
			.block;
			opacity: .75;
			.w(100%);
			.pt(140%);
			background-image: url("@{pic_url}/images/huodong/tuan/pre1.png");
			background-size: cover;
		}
		.proTitle{
			.pa;
			.bgc(#ff7a9a);
			.white;
			.h(.3rem);
			.l(.3rem);
			bottom: 0;
			.w(100%);
			.c;
			.ellipsis;
		}
		.proPrice{
			.mt(.2rem);
			.c;
		}
		.discount{
			.gray;
			.ml(.24rem);
		}
		.now_price{
			.f(.24rem);
		}
		.hour, .min, .now_price{
			.fc(#ff7a9a);
		}
		.countdown{
			.mt(.14rem);
			.c;
			.gray;
		}
		.clock{
			.i_block;
			.w(.18rem);
			.h(.17rem);
			.mr(.08rem);
			background: url("http://d01.res.meilishuo.net/pic/_o/e8/de/45a32442cb993b5d09c35b09ed1c_18_17.cg.png") no-repeat;
			background-size: cover;
		}
	}
}

.shadow{
	box-shadow:7px 2px 7px #e6e6e6;
	/*兼容IE6、 7、8*/		
	filter: progid:DXImageTransform.Microsoft.Shadow(Strength=7, Direction=180, Color='#e6e6e6'); 
	/*for ie6,7,8*/
}
.scrollin{
	-webkit-transform: translate3d(0,0,0);
	.pf;
	top: -1px;
	left: 0; 
	right: 0; 
	margin: 0 auto; 
	z-index:900;
	width:100%;
}
.scrollout{
	.pr;
}