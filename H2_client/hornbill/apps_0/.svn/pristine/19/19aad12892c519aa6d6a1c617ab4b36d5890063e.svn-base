@import "sq.less";

.address_list_normal{
	background-image: url("@{pic_url}/images/wap/web/sq/icon_normal_new.png");
	background-repeat: no-repeat;
	background-position: 15px 14px;
	background-size: 14px 14px;
}
.warp{
	.f(24*@px);
	.bgc(#f0edee);
	.textarea{
		.mb(120*@px);
		.address_list{
			.pr();
			img{.w(24*@px); .h(22*@px); .pa(); top: 0*@px; padding: 22*@px;}
			.edit{right: 150*@px; }
			.del{right: 58*@px; }
			// .del{right: 58*@px; top: 22*@px;}
			.normal_text{.red();}
			.normal_add{.black();}
			.normal_text{
				.f(12); .h(66*@px); .l(66*@px); .pl(78*@px);
			}
			.user_info{
				.street{ 
					.pb(24px*@px); .pr(80*@px);
					.user_add{ .break-all();}
				}
				.hidden; .bgc(#ffffff); .pl(78*@px); .l(38*@px);
				.info_1{.pt(25*@px);
					.user_name{.w(30%); .il-block();}

				}
			}
		}
	}
	.add{
		.bgwhite; .h(120*@px); .pf; bottom: 0px; left: 0px; .w(100%);
		.save_button{
			.ml(40*@px); .mr(40*@px); .mt(20*@px);.mb(20*@px); .bgred; .h(80*@px); .border-r();
			p{
				.f(32*@px); .c; .white; .l(80*@px);
			}
		}
	}
}