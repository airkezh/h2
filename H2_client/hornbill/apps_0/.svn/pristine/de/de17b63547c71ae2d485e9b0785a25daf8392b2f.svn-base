@import "../block.less";

html{
	.f(10*@f);
}

body{
  .bgc(#ff5668);
}

img{
	.block;
	.w(100%);
}

a{
	.block;
	outline:none;
}

ul{
	list-style:none;
}

i{
	font-style:normal;
}

.body_wrap{
  /*min-height:760px;*/
  background:#fff; /* 一些不支持背景渐变的浏览器 */
  background:-moz-linear-gradient(top, #f13d2c, #ff5668);
  background:-webkit-gradient(linear, 0 0, 0 bottom, from(#f13d2c), to(#ff5668));
}

.user_wrap{
  .bgc(#fff);
  padding:10px 14px;
  margin:0 20*@w;

	.user_box{
		.hidden;
		.l(2);
		.f(1.3rem);
		.fc(#353535);

		.info_box{
			.left;
			.w(74%);

      .level,.beans{
        .pl(24px);
      }

      .level{
        background:url("http://d01.res.meilishuo.net/pic/_o/2e/9c/99c434b10d153ebb816d9693518e_32_32.cg.png") no-repeat;
        background-size:15px auto;
      }

      .beans{
        background:url("http://d02.res.meilishuo.net/pic/_o/a8/24/9ea63d82b06f7feacb21e0bea1b9_32_32.cf.png") no-repeat;
        background-size:15px auto;
      }

			i{
				.fc(#ff204f);
			}

      .login_text{
        .c;
        .block;
        .l(1.6);
        .f(1.3rem);
      }

      .login_btn{
        .c;
        .auto;
        .block;
        .w(50%);
        .f(1.4rem);
        .fc(#ff204f);
        .border-r(1px);
        border:1px solid #ff204f;
      }
		}

		.more_box{
			.c;
			.right;
      .w(26%);
			.l(1.2);
      .f(1.5rem);
			margin:8px auto;
      background:url("http://d04.res.meilishuo.net/pic/_o/8c/67/04749d074b799ebd4330026b36c5_22_40.ch.png") no-repeat;
      background-size:11px auto;
      background-position:right center;

      .more{
        border-left:1px solid #e6e6e6;
      }
		}
	}

  .go_other_btn{
    .c;
    .l(1.8);
    .f(1.4rem);
    .fc(#fff);
    .mt(10px);
    .bgc(#ff204f);
  }
}

.user_wrap_shadow{
  .auto;
  .w(600*@w);
  .pt(20/600*100%);
  background:url("http://d05.res.meilishuo.net/pic/_o/9a/1c/ff9c968f269f9d8412b6c360b711_600_20.cg.png") no-repeat;
  background-size:100%;
}

.rule_title,.plat_coupon_title,.shop_coupon_title{
  margin:30*@w 0;
}

