<div class="promise">
	<ul>
	<% this.service && this.service.forEach(function(data,k){ %>
		<li <%if(k == this.service.length - 1){%>class="last"<%}%>>
			<a href="<%= this.DOMAIN.www%>/help/return/" target="_blank" class="pro_ico"><img src="<%=data.icon_pc%>" /></a>
			<p class="pro_tle"><a href="<%= this.DOMAIN.www%>/help/return/" target="_blank"><%=data.text%></a></p>
			<p><a href="<%= this.DOMAIN.www%>/help/return/" target="_blank"><%=data.mini_text%></a></p>
		</li>
	<% }); %>
	</ul>
</div>