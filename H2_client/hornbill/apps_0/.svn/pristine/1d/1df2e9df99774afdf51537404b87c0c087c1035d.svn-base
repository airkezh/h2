.pullUp{
	.pullUpDown > .common;.pullUpDown > .loading;
	.w(.4rem);
	.h(.4rem);
	background-size: 100%;
	&:before{
		.nowrap;
		.ml(-.1rem);
	}
	&[status="tap"]{
		background:0 none;	
	}
	&[status="stop"]{
		background:0 none;	
	}
}
