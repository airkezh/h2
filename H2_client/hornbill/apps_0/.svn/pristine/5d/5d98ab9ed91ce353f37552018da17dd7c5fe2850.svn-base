			overflow: hidden;
			text-overflow:ellipsis;
		}
		.reportedPer{
			.w(90px);
			padding: 0 10px;
			white-space: nowrap;
			overflow: hidden;
			text-overflow:ellipsis;
		}
		.reportG{
			.w(170px);
			white-space: nowrap;
			overflow: hidden;
			text-overflow:ellipsis;
			padding: 0 10px;

		}
		.reportTime{
			.w(175px);
		}
		.detailsE{
			.w(70px);
			.cursor;
		}
		.reportContent{
			.f(16px);
			font-weight: bold;
			.left;
		}
		.reportDetails{
			.f(14px);
			.left;
		}
		.reportStatus{
			.w(110px);
		}
		.viewDetails{
			.fc(#FE6197);
			.f(14px);
			.left;
		}
	}
	.paging_panel{.pt(15px); .pb(5px); .c;}
	.page;
	.pageNav{.page > .pageRed; }
}
// 查看详情弹框
.detailsContain{
	position: fixed;
	top:0;
	left: 0;
	bottom: 0;
	right: 0;
	z-index: 1000;
	background-color: RGB(153, 153, 153);
	filter:alpha(opacity=50);       /* IE */
	-moz-opacity:0.5;              /* 老版Mozilla */
	-khtml-opacity:0.5;              /* 老版Safari */
	opacity: 0.5; 
}
.details{
	.bgc(#fff);
	.w(570px);
	padding-bottom: 20px;
	.pa;
	margin: 0 auto;
	z-index: 1500;
	top: 200px;
	left:50%;
	margin-left: -270px;
	.detailsTitle{
		background-color: #F7F8F9;
		padding: 20px 25px;
		border-radius: 3px;
		.titleInfomation{
			.f(16px);
			padding-right: 18px;
			.fc(#777777);
		}
		.closeIcon{
			position: relative;
			top:-20px;
			right: -443px;
			padding: 3px;
		}
	}
	// 弹框的处理过程
	.dealProcessContain{
		.h(460px);
		overflow-y: auto;
		.dealProcess{
			padding:0 25px;
			.bgc(#FFFFFF);
			.myReport{
				clear:both;
				border-bottom: 1px solid #EEEEEE;
				padding:15px 0px 15px 0px;
				.mine{
					.left;
					.f(14px);
				}
				.myReportTime{
					.right;
					.f(14px);
					.fc(#999999)
				}
			}

		}
		.reportDescription{
			clear: both;
			overflow: hidden;
			.buyers{
				.f(14px);
				padding:20px 0;
				overflow: hidden;
			}
			.reportImage{
				padding-bottom: 30px;
			}
			.imageOfBuyers{
					.mr(20px);
					.w(80px);
					.h(80px);
				}
		}
		.remark{
			padding: 20px;
			.remarkDescription{
				.f(14px);
				border-bottom: 1px solid #EEEEEE;
				padding-bottom: 10px;
				.bgc(#FFFFFF);
				z-index: 1500;
			}
			.remarkText{
				margin-top: 15px;
				border:1px solid #EEEEEE;
				padding: 10px;
				width: 490px;
			}
		}
		.addAccessory{
			padding: 10px;
			.bgc(#FE6197);
			.fc(#FFFFFF);
			.ml(20px);
		}
		// 图片上传相关样式
		.addPic_tips{
			.fc(#CCCCCC);
			.f(12px);
		}
		.article_post_error, .article_post{
			.pt(20px);
			.pl(14px);
			.pr;
			.w(430px);
		}
		.item{
			.left;
			.pb(5px);
			.pl(5px);
			img{
				.w(80px);
				.h(80px);
			}
			.cl_i_close{
				background: url("@{pic_url}/images/club/club_icons_4.png") no-repeat;
				.bp(0, -20px);
				padding: 5px;
				.h(0);
				.pa;
				top: 0px;
				right: 0px;
				.cursor;
				z-index: 10;
			}
			.upload_percentage{.w(0);.h(14px);.border-r(2px);.bgc(#f69);}
		}
		// 提交按钮
		.submitContain{
			clear: both;
			.pb(30px);
			margin: 0 20px;
			.reportSub{
				.w(100px);
				padding:15px 0px;
				background-color: #FE6187;
				.fc(#FFFFFF);
				border:0px;
				.f(14px);
				.c;
				.cursor;
				margin: 0 auto;
			}
			.ignoreChoice{
				.pt(5px);
				.fc(#FF6699);
				.f(12px);
			}
		}
	}
	
}
