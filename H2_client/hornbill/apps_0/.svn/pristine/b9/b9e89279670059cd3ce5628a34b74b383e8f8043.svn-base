@import "../block.less";
body {
  .bgc(#f8f8f8);
}

.main {
  .w(100%);
  .auto;
  overflow: hidden;
  .f(.875em);
}

.nav_wap {
  .w(100%);
  min-height: 40px;
  overflow: hidden;
}

.nav {
  ul {
    .bgc(#fff);
    .h(40px);
    letter-spacing: 2px;
    .clearfix;
  }
  ul li {
    .left;
    .w(33.333333%);
    .c;
    .mt(11px);
    .pb(11px);
    a {
      .block;
      border-right: solid 1px rgba(51,51,51,.1);
    }
    span {
      padding: 3px 5px;
      .f(13px);
    }
  }
  ul li:nth-child(3n) {
    a {
      border-right: none;
    }
  }
  ul li.curr {
    span {
      color: #ff7a9a;
      .f(15px);
    }
  }
}

.content_tip {
  .pt(40px);
  .pb(40px);
  .mt(-30px);
  img {
    .w(42%);
    .ml(29%);
  }
  .tmw_link {
    .right;
    .block;
    padding: 6px 12px;
    .bgc(#ffab33);
    .fc(#fff);
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
  }
  .clearfix;
}

