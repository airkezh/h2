/*mywlf list*/
.wlf_box{
    .w(100%);
    .h(80px);
    .bgc(#fff);
    .pr;
    .mb(10px);
    padding:10px;
    -webkit-box-sizing: border-box;
    a{
      display: inline-block;
    }
    .wlf_img{
        .block;
        .pa;
        .w(60px);
        .h(60px);
        left:10px;
        top:10px;
         background-image: url("@{pic_url}/images/huodong/tuan/pre1.png");
         background-size: cover;
    }
    .text_content{
        margin-left:70px;
        margin-right: 28%;
        h3{
            padding: 10px 0 5px 0;
            .fc(#333333);
            .f(12px);
        }
        p{
            .fc(#999999);
            .f(9px);
        }
    }
    .wlf_state{
        .block;
        .h(40px);
        .w(28%);
        .l(40px);
        .f(13px);
        .fc(#cccccc);
        .pa;
        .c;
        border-left:1px solid rgba(51,51,51,.1);
        top:20px;
        right:0;
        a{
            .h(25px);
            .l(25px);
        }
        .bor{
            .f(11px);
            .fc(#666666);
            border: 1px solid #666666;
            padding: 0 10px;
            border-radius: 4px;
        }
        .gray{
            .fc(#cccccc);
        }
        .pink{
            .fc(#ff7a9a);
        }
        .red{
            .fc(#cccccc);
        }
    }
}

// 轮播
.imageSlide {
  border: 0px solid black;
  position: relative;
  overflow: hidden;
  font-size: 0;
  ul {
    position: absolute;
    li {
      position: relative;
      float: left;
      display: inline;
      img {
        .w(100%);
        .h(100%);
      }
    }
  }
  .num {
    .c;
    position: absolute;
    .w (100%);
    left: 0;
    bottom: 10px;
    z-index: 10;
    b {
      .i_block;
      .w (6px);
      .h (6px);
      margin: 6px 8px 0 0;
      .bgc(#fff);
      -webkit-border-radius: 100%;
      border-radius: 100%;
    }
    .cur {
      background: transparent;
      border: 1px solid #fff;
      .w(5px);
      .h(5px);
      border-radius: 50%;
    }

  }
}
/*新增固定导航，滚动到顶部样式 wch add 15.6.26*/
.shadow{
    box-shadow:7px 2px 7px #e6e6e6;
    /*兼容IE6、 7、8*/      
    filter: progid:DXImageTransform.Microsoft.Shadow(Strength=7, Direction=180, Color='#e6e6e6'); 
    /*for ie6,7,8*/
}
.scrollin{
    .pf;top:-1px;left: 0; right: 0; margin: 0 auto; z-index:100;
}
.scrollout{
    .pr;
}
.gotop{
    .w(12.5%);
    .none;
    right:20px;
    position:fixed;
    z-index: 100;
    img{
        .block;
        .w(100%);
    }
}
/*福利社二期精彩报告*/
.reportWrap{
    .bgc(#ffffff);
    .pb(30px);
    .noReport{
        text-align: center;
        .f(14px);
        .fc(#ff7a9a);
        .l(30px);
    }
    .reportList{
        .w(90%);
        .auto;
        .reportItems{
            .auto;
            .pb(15px);
            border-bottom: 1px solid #eaeaea;
            .imgs{
                .clearfix;
                img{
                    float: left;
                    .w(32%);
                    .ml(2%);
                }
            }
             .userInfo{
                    position: relative;
                    .pt(14px);
                    .pb(14px);
                    .h(25px);
                    .userImg{
                        position: absolute;
                        .w(25px);
                        .h(25px);
                        top:14px;
                        border-radius: 50%;
                    }
                    .userName{
                        position: absolute;
                        left:35px;
                        .f(11px);
                        .fc(#333333);
                        i{
                            .f(9px);
                            .fc(#999999);
                        }
                    }
                     .pulishTime{
                        position: absolute;
                        left:35px;
                        top:30px;
                        .f(9px);
                        .fc(#999999);
                    }
                   
                }
                 .report_title{
                        .pr;
                        .h(35px);
                        .l(35px);
                        .f(13px);
                        .fc(#333333);
                    }
                  .isPerfect{
                    text-indent: 25px;
                    img{
                      position:absolute;
                      .w(20px);
                      .h(20px);
                      top:7px;
                      left:0;
                    }
                  }
                    .report_con{
                        .f(11px);
                        .fc(#666666);
                        .l(15px);
                    }
        }
   }
}
#reportNo{
    .auto;
    .w(90%);
    .h(25px);
    .l(25px);
    .f(12px);
    .fc(#999999);
    font-weight: 100;
}
/*pullUp style*/
.pullUp{
    .pullUpDown > .common;.pullUpDown > .loading;
    &:before{
        .nowrap;
        .ml(-10px);
    }
    &[status="tap"]{
        background:0 none;  
    }
    &[status="stop"]{
        background:0 none;  
    }
}
.gotop{
    .w(12.5%);
    .none;
    right:20px;
    position:fixed;
    z-index: 100;
}





