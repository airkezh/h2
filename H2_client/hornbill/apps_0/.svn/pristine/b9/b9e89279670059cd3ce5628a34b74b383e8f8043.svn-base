.welfareBox {
  .w(100%);
  .h(420px);
  .mb(10px);
  .link_box {
    .block;
  }
  figure {
    .h(320px);
    .pr;
    overflow: hidden;
    span {
      .w(100%);
      .h(100%);
    }
    span.bs_load_img {
      .pa;
      .block;
      opacity: .75;
      background-image: url("@{pic_url}/images/huodong/tuan/pre1.png");
      background-size: cover;
      div {
        .h(100%);
        img {
          .border-r(0 0 0 0);
        }
      }
    }
    span.wlf_forwards {
      .pa;
      background-image: url("@{pic_url}/images/m_welfare/wlf_forwards.jpg");
      background-size: 100% 100%;
    }
    .wlf_apply {
      .w(100%);
      .h(30px);
      .pa;
      bottom: 0px;
      .bgc(#fff);
      opacity: .70;
      text-align: center;
      p.timeOver {
        .f(13px);
        .fc(#333333);
        .h(30px);
        .l(30px);
        span,strong {
          .fc(#ff6666);
          font-weight: 100;
        }
      }

    }
  }
  div.wlf_applyNo {
    height: 100px;
    text-align: center;
    .bgc(#ffffff);
    h3{
      .f(13px);
      .fc(#333333);
      .pt(12px);
      .pb(10px);
      overflow:hidden;
      white-space: nowrap;
    }
    span{
      .h(20px);
      .f(10px);
      .fc(#999999);
    }
    span:first-child {
      margin-right: 10px;
    }
    .nowApply{
        .mt(10px);
        .auto;
        .block;
        .w(26%);
        .h(25px);
        .l(25px);
        .f(11px);
        .fc(#666666);
        border-radius: 3px;
    }
    .yellow{
         .bgc(#fce44e);
    }
    .yellow:active{
          background: rgba(252,228,78,.4);
    }
    .gray{
        .bgc(#cccccc);
    }
    .gray:active{
         background: rgba(204,204,204,.4);
    }
    
  }
}
