			.audio_info { .h(74px);.right;.l(74px);
				.control_btn{.w(58px);.h(74px); .left;.pr;.cursor;}
				.start, .pause{ .w(16px); .h(30px); .pa;top:22px;left:22px;}
				.audio_duration{ .left; .f(24px);.mr(10px);}
			}
		}
	}
}
