	.activity_11tips12{background: url("@{pic_url}/images/biz/salePrice/float_12t.png") no-repeat;}
	.activity_11tips23{background: url("@{pic_url}/images/biz/salePrice/float_23t.png") no-repeat;}
	.activity_121423{background: url("@{pic_url}/images/biz/promotion/sale_141212/ad_12last.png") no-repeat;}
	.activity_12{background: url("@{pic_url}/images/biz/promotion/sale_141212/ad_12yr.png") no-repeat;}
