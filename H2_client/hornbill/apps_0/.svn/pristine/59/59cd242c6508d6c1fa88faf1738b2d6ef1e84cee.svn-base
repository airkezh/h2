/*header*/
.logo(){
	margin-top:25px;
	.w(106px); .h(36px); .mr(13px); background: url("http://d03.res.meilishuo.net/pic/_o/07/0c/5882e941e2c0d330549989f5699e_106_36.ch.png"); _filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='http://d03.res.meilishuo.net/pic/_o/07/0c/5882e941e2c0d330549989f5699e_106_36.ch.png', sizingMethod='scale'); _background-image:none; .cursor; .hidden;
	.header(){ z-index:460; left: 50%; .ml(-57px); .pa; .mt(3px); }
	.simple(){ .block;}
}

.header_20130607(){
	.hoverNav{.left;
		&.hover{
			a{background-image: url("@{pic_url}/images/header/@{mls_ico}");.bp(20px,-440px);}	
			.r_l { border-left: none; .pl(6px);}
			.r_r { border-right: none; .pr(6px); }
		}
	}
	.header{
		&_nav{.none;.red;.pa;left:50%;.ml(-436px);.w(880px);.pb(26px);.pt(4px);//border-top:2px solid #f69;
			background: #fff url("@{pic_url}/images/header/header_nav_bg.jpg") top right no-repeat;
			.box-shadow-r(0px,20px,20px,rgba(34,25,25,0.3));
			h2{.f(14px);.tunder;.w(80px);.l(16px);
				a{.red;}
			}
			.list{.ml(30px);.mr(80px);.pt(16px);
				&0{.mb(6px);}
				&1{
					h2{.left;}
				}
				&2{.ml(80px);.l(14px);
					a{.mb(4px);
						.itemLine(#ccc, 10px, 10px, solid);
					}
				}
				&.line{.border-tc(dotted);.mt(12px);}
			}
		}
	}
}

.header(){
	.header_20130607;
	#navbar{width: 100%; z-index: 400;}
	/*** 头部css .newheader**/ 
	.item_right, .item_right2{ background:#ed3577; .right; .h(30px); .pa; .w(6px); top:0; }

	.nav_new {.bp(right,center);.w(948px);.auto;
		li { .h(30px); .l(30px); .left; .hidden; 
			a { .white; .left; padding: 0 6px; 
				&:hover{.bgc(#ed3577);}
				//.item_right{*margin-top:-8px;}
			}
			.new { background: url("@{pic_url}/images/header/new_20131220.png") no-repeat;.pl(18px); .ml(3px);
				_filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='@{pic_url}/images/header/new_20131220.png',sizingMethod='scale'); _background-image:none;
			}
			.new_ico { background: url("@{pic_url}/images/header/brand_i.png") no-repeat 0 -113px;.pl(12px); }
			.r_l { .f(14px); .fb; border-left: 1px solid #FE8CB2; .pl(5px);
				&:hover{border-left: none; .pl(6px);}
			}
			.r_r { border-right: 1px solid #FA5B90; .pr(5px);
				&:hover{border-right: none; .pr(6px);}
			}
			.r_d { .pr; }
			.r_d, .r_d:hover { .h(30px); padding: 0 6px; border:none; .bgc(#ed3577); }
			.r_l{
				//.item_right{*margin-top:-8px;}
			}
			.wlc_ico {padding:0px;background: url("@{pic_url}/images/header/@{mls_ico}") no-repeat -72px -509px; .i_block; .h(30px); .w(26px); }
		}
	}
	.shining { background: #fff; .border-r(2px); .red; .ml(3px); padding: 1px 3px; }

	.home{.f(14px); .fb;}
	.h_ico { .yellow; .pa; .h(16px); .l(16px); top: 40px;left:36px; padding: 5px 8px; .w(160px); z-index: 405; }
	//nav follow
	.nav_follow {
		.gray; .l(20px); .w(150px); 
		.pink_pop { .red; background:url("@{pic_url}/images/header/@{mls_ico}") no-repeat -172px -144px; padding:1px 4px 1px 6px; }
	}
	.nav_down { z-index:450;.w(130px); .h(85px); .p10; box-shadow: 0 1px 3px rgba(212, 212, 212, 0.8); -webkit-box-shadow: 0 1px 3px rgba(212, 212, 212, 0.8); .pa; .mt(4px); .ml(0); .bgwhite; }
	.widescreen(){
		.bgc(#f69); left: 0; .auto; .pr; 
		.header_b{
			.h(30px); .w(960px); .auto; .pr;
		}
	}

	.headertop(){
		.header_bg { .bgc(#f2f0f0);
			.header_top{
				.h(45px); padding: 4px 0 3px; .auto; .w(960px);
				.logo_new{ .logo;.logo>.header;}
			}
		}
		.menu_leo {
			.bgc(red);
			z-index:450;.pr;.inline;.box-linear; .border-1c; .h(28px); .right; .mt(8px);.mr(6px);
			li { border-left: 1px solid #fff; border-right: 1px solid #ccc; .left; .h(28px); .l(28px); .hidden;
				a { .block; .pl(8px); .pr(8px);
					&:hover { background: #F2F0F0; }
				}
				.h_share { .fb; .red; }
				.redarrow { background: url("@{pic_url}/images/header/@{mls_ico}") no-repeat -20px -70px; _background-position: -20px -82px; .i_block; .h(6px); *height: 9px; _height: 22px; .hidden; .w(10px); }
				.s_face { .left; .h(16px); margin: 6px 5px 0 0; .w(16px);
					img{ .h(16px); .w(16px); }
				}
			}
			#message { .cursor; }
		}
		.search;.search > .top;	
		.daily_mobile_info { .h(40px); .hidden;
			.daily_mobile { .h(33px); .hidden; .pr; .ml(6px); .mt(8px); }
			ul { .pa;.border-1(#ccc); .pr(8px); .box-linear(); }
			li { .l(28px); .pl(38px); .block; background:url("@{pic_url}/images/header/bg_daily_mobile_info_l.png") no-repeat 0 0; }
			.mobileHW_go { padding: 0 6px; .ml(6px); background:url("@{pic_url}/images/header/bg_daily_mobile_info_r.png") no-repeat center center; }
		}
	}
}
