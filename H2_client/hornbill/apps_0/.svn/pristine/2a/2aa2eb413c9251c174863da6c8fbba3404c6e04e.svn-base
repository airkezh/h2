@import "../block.less";
@import "../window.less";
@import "../block/common_sale.less";
@import "shoppingshow.import.less";
@import "yy_sale.import.less";
body { .bgc(@bodybg); }
.sec_nav { .auto; .w(948px); }
.page;
.pageNav1{ .page > .pageRed; .c; margin: 30px auto; }
.poster_icons;
.face_pop { .window > .head_float; }
.share_group { .share_group_btn; }
.tab_tle { .tabDetail; }
.sale_icons;
samp { font-family: Arial; }

.buy_btn, .define { .bgc(#f69); .white; } 
.add_cart { .bgc(#f93); .white; }
.non_buy { .bgc(#d9d9d9); }

.detail { .w(948px); .auto; .mt(20px); 
	.top_bnr { .h(120px); .w(948px); .hidden; .block; }
	.sale { .right; .mt(20px); .w(370px); 
		.s_tle { .f(16px); .fb; .l(26px); }
		.text_info { .l(20px); .mt(3px); 
			.quot_l, .quot_r { .h(16px); .pl(15px); }
			.quot_l { .bp(-123px,0); }
			.quot_r { .bp(-156px,0); }
		}
		.activity_desc { .red; .mt(10px);
			em{ background: url("@{pic_url}/images/doota/prom_activity_desc_1.png") no-repeat; 
				.i_block; .w(20px); .h(16px);.mb(-3px);
			}
		}
		.buy_info { .h(60px); .mt(20px); .w(100%); .pr;
			.arrow { .bp(-293px,0); .block; .h(60px); .w(24px); .pa; left: -24px; top:0; }
			.price { .white; .bgred; .p10_0; .h(40px); .pl(5px); .w(273px); .pa; left:0;
				dt { .left; .f(36px); .l(40px); .mr(6px); }
				dd { .left; .l(18px); .pt(2px);
					span { .block; .t-through; }
				}
			}
			.discount { .border-1(#f69); .c;.red; .f(16px); .h(48px); .l(48px); .w(90px); .pt(5px); .pb(5px);.pa;right:0;
				samp { .f(26px); .pr(2px); .pr; top: 2px; }
			}
		}
		.buy_btn, .add_cart, .define { .cursor; .f(16px); .h(40px); .l(40px);.i_block;.c; .w(142px); }
		.stock { .ml(20px); .l(20px); }
		.info_box { .border-1(#e6e6e6); border-top:none; .bgc(#f7f7f7); .pr; padding: 20px 30px 25px;
			.status { border-top: 1px dotted #7f7f7f; border-bottom: 1px dotted #7f7f7f; 
				.c; .h(26px); .l(26px); .pt(16px); .pb(16px); .mt(20px);
				.mid_clock { .bp(-268px,-67px); .pl(20px); .pt(7px); .pb(7px); }
				.countdown { .inline;
					samp { .f(24px); .red; .pl(2px); .pr(2px); .pr; top: 3px; }
				}
			}
			.btn_box { .h(40px); .w(100%); }
			.userBox { .pt(15px); .pb(20px); .hidden; .w(100%);
				.user_buy { .left; .l(20px); .w(140px); }
				.user_from { .right; .h(34px); .mt(5px); .w(168px); .hidden;
					li { .w(100%); .h(34px); .hidden;
						.face { .mr(10px); .avatar32; .border-1(#e8e8e8);
							img { .avatar32; }
						}
						.text { .ellipsis; .h(14px); .l(14px); }
					}
				}
			}
			.bot_box { .h(18px); .l(18px); .w(100%); .pr;
				.loveBox { .left; .w(150px); 
					.love_ico { .bp(-297px,-70px); .h(20px); .pl(18px); }
					.like_btn { .cursor; }
					.love_msg { background: url("@{pic_url}/images/doota/like_pro.png") no-repeat;
						.red; .left; .c; .none; .f(12px); .h(31px); .l(26px); .w(130px); .pa; left: -6px; top: -35px;
					}
				}
				.share { .right; .h(18px); .tr; .w(110px);
					a { .ml(2px); }
				}
			}
			.promptBox { .bgwhite; .border-1(#f69); .none; border-top: none; .w(368px); 
				.pa; left: -1px; _left: -31px; top:0;z-index:9;
				.text { .bgc(#ffebf2); .block; .l(16px); .w(348px);  padding: 8px 10px; 
					.close_z { .mt(2px); }
					.msg_ico { .bp(-2px,-115px); .left; .h(16px); .mr(10px); .w(16px); }
				}
				.sel_box { padding: 20px 30px;
					.msg_ {
						&over, &limit, &price { .red; .pb(15px); }
					}
					dl { .hidden; .pb(3px); .w(100%);
						dt { .left; .mt(3px); .h(16px); .w(75px); .hidden; }
						dd { .left; .w(233px);
							li { .left; .mr(10px); .l(18px); padding: 1px 1px 10px;
								a { .border-1(#d2d2d2); .block; .c; .break-word; .pl(6px); .pr(6px); min-width: 10px; }
								a.active { border: 2px solid #f69; margin: -1px; }
							}
						}
					}
					.numBox { .border-1(#d2d2d2); .left; .h(19px); .w(76px); 
						.minus, .plus { .cursor; .h(19px); .w(19px); .hidden; }
						.minus { .bp(-48px,-72px); .left; border-right: 1px solid #d2d2d2; }
						.plus { .bp(-76px,-72px); .right; border-left: 1px solid #d2d2d2; }
						.nums {.border-1(#fff);.c;.dgray;.left;.hidden; .h(12px); .l(14px); .w(32px); padding:3px 0 0;  }
					}
					.buy_btn { .mt(12px); }
				}
			}

		}
	}
	.picture { .left; .block; .c; .hidden; .mt(20px); .h(500px); .w(536px); .pr;
		.big_pic { .h(500px); .w(536px); .block; .c; .vm; display:table-cell;*font-size:450px;
			.big_img { max-width: 536px; max-height: 500px; .vm;  }
			.mark{ .pa; left: 5px; top: 0px;
				span{ .left; .mr(1px); }
			}
		}
	}
}
.comm_tle { .commTle(); }
.promiseBox;
.maylike;
.common_title;

.main { .mt(14px); .w(948px); .auto;
	.wrap { .wrapBox(); 
		.all { .c; .bgc(#f5f5f5); .h(32px); .l(32px); .mt(-1px); }
		.not_fame { .c; .f(14px); .l(100px); .pt(20px); }
		.fame {
			.starBox { .left; .pl(50px); .pt(46px); .w(350px);
				.point { .f(30px); .red; .pr(10px); }
				.big_star, .big_star b { .left; .h(20px); }
				.big_star { .bp(-190px,-21px); .mt(2px); .w(90px);
					b { .bp(-190px,0); }
				}
			}
			.score { .left; .pt(10px); .w(265px);
				li { .h(14px); .hidden; .mt(10px); 
					.txt { .left; .w(76px); }
					.score_line, .score_line b { .left; .h(14px); .border-r(2px); }
					.score_line { .bgc(#e6e6e6); .mr(10px); .w(110px); 
						b { .bgc(#fcc63b); }
					}
				}
			}
		}
		.fame_list { border-top: 1px solid #e6e6e6; .pl(20px); .pr(20px); .mt(20px); 
			li { border-bottom: 1px dotted #7f7f7f; .pt(30px); .pb(26px); .hidden; .w(100%);
				.avatar48_f img { .border-r(5px); }
				p { .ml(66px); }
				.txt_one { .gray; }
				.txt { .l(20px); .mt(10px);
					strong { .left; .w(68px); }
					.con { .left; .w(770px); }
				}
				.f_skin { .ml(20px); }
				.jing { .white; .bgc(#f93); .f(14px); .border-r(5px); .right; .mt(-3px); .mr(10px); padding: 2px 6px; }
			}
		}
	}
}

.tab_match {
	.tabNew;
	.w(982px);.auto;
}
.cmn_title{
	.l(48px);.h(48px);.f(16px);border-bottom: 1px solid #e6e6e6;overflow: visible;
	h3{.red;border-bottom: 3px solid;margin: 0;padding: 0 20px;.l(46px);}
}
.shop-promote{
	.w(920px);.auto;padding: 20px;.mb(2px);.bgc(#f9f9f9);.border-1(#e7e7e7);border-top-width: 0;
	i{font-style: normal;}
	p{.i_block;
		i{.fc(@red);}
	}
	.item{.mb(10px);}

	.icon{.i_block;.w(16px);.h(16px);.l(16px);.mr(10px);.fc(#fff);.c;}
	.icon-mail{.bgc(#94d564);}
	.icon-reduce{.bgc(#fcc15b);}
	.icon-discount{.bgc(#ff9cc0);}

	.note{.ml(20px);.fc(#999);.ellipsis;}
}

.main {
	.detail_tle { .red; .f(16px); .mt(20px); }
	.evaLink, .shoppingLink, .dealLink { .bgc(#f5f5f5); .cursor; .c; .h(32px); .l(32px); .mt(-1px); }
	.twitter_comment_hd .r_info .jing{.fc(#FFF);.bgc(#F93);.border-r(5px);padding: 2px 6px;}
	.comments .list>li .l_con{.l(20px);}
}
