@import "sq.less";
body{
	.bgc(#ffe8ab);
	background-image: url("@{pic_url}/images/wap/sq/back.png");
	background-repeat: no-repeat;
	background-size: cover;
	.fc(#7e4812);
	.w(100%);
	.friend-title{
		.w(70%);
		.f(28*@px);
		.l(36*@px);
		.pt(20%);
		.auto;
	}
	.friend-btn{
		.w(100%);
		img{
			.w(42000/620%);
			.auto;
			.block;
		}
		.pt(20%);
	}
	.friend-link{
		text-align: center;
		.w(100%);
		.pt(10%);
		a{
			.f(28*@px);
			.fc(#7e4812);
		}
	}
}