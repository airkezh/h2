<%# head.html%>
<%
var query   = this._Request_query,
    mSubstr = _extFn.mSubstr,

    shopGoodsCate     = this.shop_goods_category || {},
    shopCategoryDatas = shopGoodsCate.datas,
    topNavs = shopCategoryDatas && shopCategoryDatas[1] && shopCategoryDatas[1].cats || [],

    shop_in         = this.shop_in || '',
    featureGoods    = this.feature_goods = this.feature_goods || {},
    defaultShopHead = this.PICTURE_URL + 'images/shop/head-img.jpg',
    shop_head_img   = shop_in.shop_head_img ? shop_in.shop_head_img : defaultShopHead,
    goodsInfo       = this.goods_info || {},
    shopId          = goodsInfo.shop_id;

function justifyTxt(str , endTag){
    endTag = endTag || 'span'
    return str.split('').join(' ') + '<'+endTag+'></'+endTag+'>'
}
var cur_time  = (new Date).getTime() / 1000

    //评价条数
var shopCommentsCount = goodsInfo.comment.totalUser || 0,
    //晒单数
    shoppingShowCount = goodsInfo.count_shoppingShow || 0,

    shopDisAll = this.satisfy_rate;
var reportCheck =  this.reportCheck;

/* 好评率 */
if ( !goodsInfo.satisfy_rate ) {
    this.hasNoSatisfyRatio = true
    goodsInfo.satisfy_rate = '--'
}

function getTimeRemain(end_time){
    if (end_time && end_time > cur_time) return end_time - cur_time
    else return ''
}

/**
    注意：后端传的价格都是字符串，如果要进行比较的时候需要手动转(比如下面的 preheatPrice)
*/
var nowPriceMin, nowPriceMax, nowPrice, oriPrice,
    oriPriceMin = goodsInfo.original_price_min,
    oriPriceMax = goodsInfo.original_price_max;

if ( goodsInfo.campaign_price_min ) {
    nowPriceMin = goodsInfo.campaign_price_min;
    nowPriceMax = goodsInfo.campaign_price_max;
} else {
    nowPriceMin = goodsInfo.original_price_min;
    nowPriceMax = goodsInfo.original_price_max;
}

if ( nowPriceMin == nowPriceMax ) {
    nowPrice = nowPriceMin
} else {
    nowPrice = nowPriceMin + '-' + nowPriceMax
}

if ( oriPriceMin == oriPriceMax ) {
    oriPrice = oriPriceMin
} else {
    oriPrice = oriPriceMin + '-' + oriPriceMax
}

/* 0 表示不在大促，1 表示预热，2 表示大促 */
var promoteStatus  = goodsInfo.is_bigpromote,
    promoteInfo    = this.promote_info,
    promoteContent = this.promote_info.info || {},
    promoteTime    = {},
    bigPromoteClass, preheatPrice, isBigPromote, promotePhase;

promoteTime[ promoteInfo.preheat_time ] = 1;
promoteTime[ promoteInfo.stime ] = 2;
promoteTime[ promoteInfo.etime ] = 0;

promotePhase = this.ViewTools.timeStuff( promoteTime, '' );
switch( promotePhase ) {
    case 1:
        bigPromoteClass = 'big-promote big-promote-preheat';
        preheatPrice = goodsInfo.preheat_price_min || 0;
        isBigPromote = promoteStatus != 0 && parseFloat( nowPriceMin ) >= parseFloat( preheatPrice ) && goodsInfo.platform_type != 5;
        break;
    case 2:
        bigPromoteClass = 'big-promote big-promote-now';
        preheatPrice = nowPriceMin;
        isBigPromote = promoteStatus != 0 && parseFloat( nowPriceMin ) >= parseFloat( preheatPrice ) && goodsInfo.platform_type != 5;
        break;
    default:
        isBigPromote = false;
        bigPromoteClass = preheatPrice = '';
        break;
}

/* 上面 isBigPromote 的代码无用了 */
var promoteMsg = goodsInfo.promot_msg;

if ( promoteMsg ) {
    promoteMsg = promoteMsg.replace( /(￥\d+(?:.\d+)?)/, '<i class="num">$1</i>' )
}
%>

<%if( !this.is_iPad ) {%>
<div id="shop-hd" class="shop-hd">
    <%if ( shop_in != '' ) { %>
    <a class="top-banner" href="/shop/<%=shopId%>" target="_blank" style="background-image:url(<%=shop_head_img%>)">
    </a>
    <%}%>
    <%= requireFn('doota/nav.html')({
            categoryId: this._Request_query.category_id,
            shopId: shopId,
            categories: topNavs
        });
    %>
</div>
<%}%>

<div class="main <%= bigPromoteClass %>">

    <%if ( 1 == goodsInfo.buyer_verify ) {%>
    <div class="you_shop">
        <h3 class="tle_ico"><a href="/help/return/#d_5" target="_blank">优品认证</a></h3>
        <p class="f14_f"><a href="/help/return/#d_5" target="_blank">美丽说已对商品进行详细抽检，质量合格，可放心购买</a></p>
    </div>
    <%}%>

    <div class="item-box" data-tid="<%= goodsInfo.twitter_id %>">
        <div class="content twitter" twitter_id="<%= goodsInfo.twitter_id%>">
            <div class="item-sale">
                <h3 class="item-title"><% if(goodsInfo.count_shoppingShow) { %><a href="#" id="js-shopping-show" class="label-show">[晒单] </a><% } %><%= goodsInfo.goods_title%></h3>
                <% if (goodsInfo.goods_subtitle) {%>
                <h4 class="item-sub-title"><%= goodsInfo.goods_subtitle %></h4>
                <%}%>

                <div class="item-price-info">
                    <span class="price-now">￥<i id="price-now" class="num" data-price-min="<%= nowPriceMin %>" data-price-max="<%= nowPriceMax %>"><%= nowPrice %></i></span>
                    <%if(1 == goodsInfo.in_activity ){%>
                    <span class="price-original">￥<i id="price-original" class="num" data-price-min="<%= oriPriceMin %>" data-price-max="<%= oriPriceMax %>"><%= oriPrice %></i></span>
                    <%}%>
                    <%
                    goodsInfo.label.forEach(function(label) {
                    if(label.text != '免运费' || (label.text == '免运费' && (goodsInfo.freight_free || !goodsInfo.freight))) {
                    %>
                    <span class="label-text js-label" style="background-color:<%= label.color %>" data-text="<%= label.text%>"><%= label.text %></span>
                    <%}});%>
                </div>

                <!-- big promote -->
                <div class="big-promote-notice">
                    <p style="background-color:#<%= promoteContent.bgcolor%>;">
                        <i class="logo" style="background-image:url(<%= promoteContent.icon%>);"></i>
                        <span>
                            <%= promoteMsg %>
                        </span>
                    </p>
                    <a href="<%= promoteContent.url %>" target="_blank"><%= promoteContent.word %>&nbsp;&gt;&gt;</a>
                </div>
                <!-- big promote end -->

                <%
                var promptRemain = getTimeRemain( goodsInfo.end_time ),
                labelText    = goodsInfo.end_time_text == '团购' ? '团' : '限';
                %>
                <ul class="item-promote-infos js-shop-promote">
                    <% if (promptRemain && 1 == goodsInfo.shelf ){%>
                    <li class="js-promote-item">
                        <i class="label-mark-limit"><%=labelText%></i>
                        距离恢复原价仅剩<span class="countdown" data-remain="<%= promptRemain %>"><i class="day countdown-time">&nbsp;</i>天<i class="hour countdown-time">&nbsp;</i>小时<i class="minute countdown-time">&nbsp;</i>分<i class="second countdown-time">&nbsp;</i>秒</span>
                    </li>
                    <%}%>
                    <% if(goodsInfo.ext_info) { %>
                    <li class="js-promote-item">
                        <i class="label-mark-limit">预</i>
                        预售发货时间<span><i class="countdown-time"><%= goodsInfo.ext_info.info.mon %></i>月<i class="countdown-time"><%= goodsInfo.ext_info.info.day %></i>日</span>
                    </li>
                    <% } %>

                </ul>

                <%
                    var promoteRoomInfos = goodsInfo.promoteRoomInfos;
                    if(promoteRoomInfos) {
                %>
                <a class="big-promote-room" href="<%= promoteRoomInfos.url %>" target="_blank"><%= promoteRoomInfos.notice %>&nbsp;&gt;&gt;</a>
                <% } %>

                <div class="item-freight">
                    <label>运 费</label>
                    <%= shop_in && shop_in.location%><span class="gray_f pl6_f">至</span><a id="js-dest-city" class="pl6_f pr6_f city-name" href="#"><span class="js-city-name"><%= goodsInfo.freight_city || '北京' %></span><i class="arrow-down"></i></a>
                    <span id="js-freight-text"><% if(goodsInfo.freight_free) { %>全场免运费<% } else if(goodsInfo.freight) {%> 运费: <%= goodsInfo.freight %>元<% if(goodsInfo.freight_quota){ %> (满<%= goodsInfo.freight_quota %>元包邮) <% } } else {%>免运费<%}%></span>
                </div>

                <ul class="item-data">
                    <li>销量
                    <span class="item-data-wrap">
                        <%= goodsInfo.sales%>件
                    </span>
                    </li>
                    <li class="item-data-middle">好评率
                    <span class="item-data-wrap">
                        <a href="#" id="js-comment"><%= goodsInfo.satisfy_rate %></a> (<%= goodsInfo.comment.totalUser
                        %>人)
                    </span>
                    </li>
                    <li>喜欢
                    <span class="item-data-wrap">
                        <%= goodsInfo.twitter_stat.count_like%>人
                    </span>
                    </li>
                </ul>

                <%if(goodsInfo.promo != ''){%>
                <p class="s_tle red_f"><%= goodsInfo.promo%></p>
                <%}%>

                <ul class="sku_meta">
                    <%if(goodsInfo.is_preheat) {%>
                    <li class="sku-item">
                        <span class="sort">活动价</span>
                        <span class="price-normal">￥<i id="price-preheat" class="num" data-price-min="<%= goodsInfo.preheat_price_min%>" data-price-max="<%= goodsInfo.preheat_price_max %>"><%= goodsInfo.preheat_price_min == goodsInfo.preheat_price_max ? goodsInfo.preheat_price_min : (goodsInfo.preheat_price_min + '-' + goodsInfo.preheat_price_max)%></i></span><span class="js-promotion-desc" data-time="<%= goodsInfo.start_time %>"></span>
                    </li>
                    <%}%>

                    <% if(goodsInfo.campaign_phone_price_min) {%>
                    <li class="sku-item mob-exclusive">
                        <span class="sort">手机专享</span>
                        <span class="price">¥<i id="price-phone" class="num" data-price-min="<%= goodsInfo.campaign_phone_price_min %>" data-price-max="<%= goodsInfo.campaign_phone_price_max %>"><%= goodsInfo.campaign_phone_price_min == goodsInfo.campaign_phone_price_max ? goodsInfo.campaign_phone_price_min : (goodsInfo.campaign_phone_price_min + '-' + goodsInfo.campaign_phone_price_max)%></i></span>
                        <div class="dropdown-wrap">
                            <a class="red_f js-mob-exclusive-btn dropdown-wrap-btn" href="javascript: void 0;"><% if(goodsInfo.is_preheat == 1) {%>扫码享916优惠价<%} else {%>扫码立享专享价<%}%><i class="arrow-ico"></i></a>
                            <div class="js-mob-exclusive-panel dropdown-panel">
                                <img src="<%= goodsInfo.qr_img %>" width="170">
                            </div>
                        </div>
                    </li>
                    <% } %>
                </ul>
                <div class="sku_info">
                    <div class="skin">
                        <%if(1 == goodsInfo.shelf ){%>
                        <p class="text none_f"><span class="close_z"></span><span class="msg_ico"></span>请选择您要的商品信息</p>

                        <%
                        if( goodsInfo.color ) {
                            var goodsColor    = goodsInfo.color,
                                index         = 1,
                                itemColors    = [],
                                hasImageColor = false,
                                renderHtml    = [],
                                c, itemColor, metaTitle;

                            for ( c in goodsColor ) {
                                if ( goodsColor.hasOwnProperty( c ) ) {
                                    itemColor = goodsColor[ c ];
                                    metaTitle = itemColor.meta_title;
                                    if ( itemColor.color_image) {
                                        hasImageColor = true;
                                        renderHtml.push('<li class="img_list"><a href="#" class="js-item item-color-img" data-type="color" data-id="' + itemColor.meta_id + '" data-index="' + (index++) + '"><img src="' + itemColor.color_image + '" bsrc="' + itemColor.image + '" title="' + itemColor.meta_name + '" alt="' + itemColor.meta_name + '"><i class="icon-outofstock-mask"></i></a></li>');
                                    } else {
                                        renderHtml.push('<li ><a href="#" class="js-item item-color-text" data-type="color" data-id="' + itemColor.meta_id + '" data-index="' + (index++) + '">' + itemColor.meta_name + '</a></li>');
                                    }
                                }
                            }
                        %>
                        <dl class="prop">
                            <dt>
                                <%= justifyTxt( metaTitle )%>
                            </dt>
                            <dd>
                                <ul id="colorList" class="item-colorlist <%= hasImageColor ? 'has-image-color' : '' %>">
                                <%= renderHtml.join('') %>
                                </ul>
                            </dd>
                        </dl>
                        <%}%>
                        <%
                        if(goodsInfo.size) {
                        var goodsSize = goodsInfo.size;
                        var index = 1;
                        %>
                        <dl class="prop">
                            <dt><% for(var c in goodsSize) { %>
                                <%= justifyTxt(goodsSize[c].meta_title)%>
                                <% break;}%></dt>
                            <dd>
                                <ul id="sizeList" class="item-size-types">
                                    <%
                                    for(var s in goodsSize) {
                                    var itemSize = goodsSize[s];
                                    %>
                                    <li><a href="#" class="js-item item-size-type" data-type="size" data-id="<%= itemSize.meta_id %>" data-index="<%= index++ %>"><%= itemSize.meta_name %></a></li>
                                    <%}%>
                                </ul>
                            </dd>
                        </dl>
                        <%}%>
                        <dl class="amount">
                            <dt>数 量</dt>
                            <dd>
                                <div class="item-stock js-stock">
                                    <input class="item-stock-num js-stock-num" title="请输入购买数量" type="text" value="1" <%if(goodsInfo.coupon_seller){%> readonly<%}%>>
                                <span class="item-stock-btns">
                                    <span class="item-stock-plus js-stock-plus <%if(goodsInfo.coupon_seller){%>hidden<%}%>"><i class="icon-up"></i></span>
                                    <span class="item-stock-minus js-stock-minus <%if(goodsInfo.coupon_seller){%>hidden<%}%>"><i class="icon-down"></i></span>
                                </span>
                                <span class="item-stock-reserve js-stock-reserve">
                                    <% if(goodsInfo.platform_type == 1) {%>
                                    (预订款 <a href="/help/korea_shop#h_03" target="_blank" class="red_f">查看说明</a>)
                                    <% } else {%>
                                    (库存<span>-</span>件)
                                    <%}%>
                                </span>
                                </div>
                            </dd>
                        </dl>
                        <% if(this._serviceIM){ %>
                        <dl class="mt10_f">
                            <dt>客 服</dt>
                            <dd>
                                <% if(this._serviceIM){%>
                                <a class="goqq knilmi" href="<%= this.ViewTools.callIM(this._serviceIM) %>" target="_blank"><em class="qqico">&nbsp;</em>联系客服</a>
                                <% } %>
                            </dd>
                        </dl>
                        <% } %>
                        <% if ( shop_in.is_shop_realshot || shop_in.is_goods_realshot ) { %>
                        <dl class="mt10_f">
                            <dt>承 诺</dt>
                            <dd>
                                <i class="icon-realshot"></i><span class="item-real-shot">实拍认证</span>
                            </dd>
                        </dl>
                        <% } %>

                        <div class="msg_box">
                            <p class="msg_limit"><span class="msg_ico"></span>对不起！每人最多限购100件！</p>
                            <p class="msg_zero"><span class="msg_ico"></span>对不起，这件宝贝已经卖光啦！</p>
                            <p class="msg_over"><span class="msg_ico"></span>您所填写的宝贝数量超过库存！</p>
                        </div>
                        <%}%>
                        <div class="btn_box">
                            <% if(goodsInfo && goodsInfo.purchased){ %>
                            <span class="msg_shelf">MM你已经购买过该优惠券啦!</span>
                            <% } else if (goodsInfo && 1 == goodsInfo.shelf) { %>
                            <% if(goodsInfo.seckill_status > 0) {%>
                            <a class="go-to-client" href="/client/?frm=daeh">此宝贝只能手机上购买，赶紧去下载客户端吧!&nbsp;&gt;&gt;</a>
                            <% } else {%>
                            <a class="define" title="确定">确定</a>
                            <div class="same_btn">
                                <a class="add_cart" title="加入购物车"><em class="cartico">&nbsp;</em>加入购物车</a>
                                <a class="buy_btn" title="立即购买"><em class="rmbico">&nbsp;</em>立即购买</a>
                            </div>
                            <%}%>
                            <%}else{%>
                            <% if(!this.goods_info.is_zero){ %>
                            <span class="msg_shelf">宝贝已下架!</span>
                            <% } else { %>
                            <span class="msg_shelf"><a href="/biz/fouryears/zero/#welcome">请回活动换购</a></span>
                            <% }%>
                            <%}%>
                        </div>
                    </div>
                </div>

                <%
                var twitterStat = goodsInfo.twitter_stat,
                count_like  = twitterStat && twitterStat.count_like,
                beauty_me = goodsInfo.beauty_me
                %>
                <div class="item-spread">
                    <% if (2 == beauty_me){ %>
                    <a class="love_msg">这是你自己分享的哦!</a>
                    <% } %>
                    <div class="item-like-wrap">
                        <a class="item-like-btn js-like-btn" data-liked="<%= 1==goodsInfo.beauty_me?1:0%>" isshowlike="<%= 2==beauty_me?0:1 %>" like_twitter_id="<%= goodsInfo.twitter_id %>">
                            <%if (1 == beauty_me) {%>
                            已喜欢
                            <%}else{%>
                            <i class="icon-like"></i>喜欢
                            <%}%>
                            <i class="item-like-num"><%= count_like?'('+count_like+')' :'' %></i>
                        </a>
                        <span class="addOneLike">+1</span>
                    </div>
                    <a class="item-share-btn js-item-share-btn">
                        <i class="icon-share"></i>分享
                    </a>
                    <a id="twitter_magazi" class="add-in-magazine-btn">
                        <i class="icon-magazine" title="收进杂志"></i>
                    </a>
                </div>
                <div class="report"><a class="reportL" href=
                    <% if(reportCheck.success!=0){ %>
                    "/reportGoods?event_id=<%= goodsInfo.twitter_id%>"
                    <%}else{%>
                    "javascript:void(0)"
                    <%};%>
                     >举报</a></div>
                <% if(this.isLogin && reportCheck.success==0){ %>
                    <span class="warn">
                        <%= reportCheck.error_info %>
                    </span>
                <%}%>
                <% if(goodsInfo.banner) {%>
                <a href="<%= goodsInfo.banner_url %>" target="_blank"><img class="ad-banner" src="<%= goodsInfo.banner %>"></a>
                <% } %>
            </div>

            <div id="picture" class="item-pic">
                <div class="item-pic-origin" style="max-height:<%= goodsInfo.image.max_height%>px;">
                    <%= requireFn('mark.html')({
                    'marks' : goodsInfo.marks
                    }); %>
                    <img class="j-big-pic twitter_pic" alt="<%= this.winTitle %>" src="<%= goodsInfo.image.image[0]%>"/>
                    <% if ( goodsInfo.count_latestSaleNum ) {%>
                    <div class="js-pop-tip item-pop-tip" data-num="<%= goodsInfo.count_latestSaleNum || 0 %>"><i class="icon-people"></i><i class="num"><%= goodsInfo.count_latestSaleNum%></i> 人最近已购买</div>
                    <% } %>
                </div>
                <div class="item-pic-thumb js-pic-thumb">
                    <% goodsInfo && goodsInfo.image.small_image.forEach(function( img, i ) { %>
                    <span><img src="<%= img %>" bsrc="<%= goodsInfo.image.image[i]%>" width="68"></span>
                    <% }); %>
                </div>
            </div>
        </div>
        <div class="sidebar">
            <% if( shop_in ) {
                var _tmpClassName = 'shop ';
                if ( shop_in.is_cs == 1 ) {
                    _tmpClassName += 'competitive-shop'
                } else if ( shop_in.is_star == 1 ) {
                    _tmpClassName += 'star-shop'
                }
            %>
            <div class="<%= _tmpClassName %>">
                <a class="shop-avatar" href="/shop/rating/?shop_id=<%=shop_in.shop_id%>&frm=rate_to_rateall" target="_blank"><img width="48" src="<%=shop_in.shop_face_img%>"></a>
                <i class="icon-shop"></i>
                <div class="shop-wrap">
                    <a href="/shop/<%=shop_in.shop_id%>?pos=guangNick" target="_blank" class="shop-name"><%=shop_in.shop_nick%></a>
                    <div class="shop-follow-state js-follow-shop">
                        <a class="shop-follow-btn <%if(shop_in.isFollowing){%>is-followed<%} else {%>is-not-followed<%}%>">
                            <%if(shop_in.isFollowing){%>
                            <i class="icon-check"></i>已关注
                            <%}else{%>
                            <i class="icon-plus">+</i>关注
                            <%}%>
                        </a>
                        <span class="shop-fans-wrap"><i class="shop-fans js-fans"><%= shop_in.followerNum %></i>粉丝</span>
                    </div>
                </div>
                <ul class="shop-assess">
                    <%
                    var trend = shop_in.points_trend;
                    if(trend) {
                    [
                        [ '描述', 'accord', 'first' ],
                        [ '质量', 'quality', 'middle' ],
                        [ '服务', 'attitude', 'middle' ],
                        [ '发货', 'fast', 'last' ]
                    ].forEach(function(data, i) {
                    var prop = data[1],
                        mark, avg, content, stateClass;

                    if ( !trend[prop] ) return

                    mark = trend[prop].mark
                    avg  = trend[prop].avg

                    if ( mark == 1 ) {
                        stateClass = 'up'
                    } else if ( mark == 0 ) {
                        stateClass = 'fair'
                    } else {
                        stateClass = 'down'
                    }

                    if ( !avg || avg == '0.0' ) {
                        content = '--'
                    } else {
                        content = '<i class="arrow-ico">--</i>' + avg
                    }
                    %>
                    <li class="shop-assess-<%=data[2]%>">
                        <%=data[0]%>
                        <a href="/shop/rating/?shop_id=<%=shop_in.shop_id%>&frm=rate_to_rateall" class="shop-point <%=stateClass%>" target="_blank"><%=content%></a>
                    </li>
                    <%
                    })
                    }
                    %>
                </ul>
                <ul class="shop-info">
                    <li>所在地区：<%= shop_in.location%></li>
                    <li>商品数量：<%=shop_in.goods_count%></li>
                    <li>销售数量：<%=shop_in.goods_sales%></li>
                    <li>店铺资质：
                        <i class="icon-approve" title="美丽说认证 <%=shop_in.shop_ctime%>"></i>
						<i class="icon-money<%=shop_in.shop_margin%>" title="已缴纳<%=shop_in.shop_margin%>元保证金"></i>
                    </li>
                    <li>联系客服：<a class="knilmi" href="<%= this.ViewTools.callIM(this._serviceIM) %>" target="_blank"><i class="icon-contact"></i></a></li>
                </ul>

                <a href="/shop/<%=shop_in.shop_id%>?frm=rate_to_shop" class="into-shop-btn" target="_blank"><i class="into-shop-ico"></i>进入店铺</a>
            </div>
            <% } %>

            <% if(this.maybe_like && this.maybe_like.length) {%>
            <div class="maybe-like">
                <h3 class="hd">你可能还喜欢</h3>
                <ul>
                    <% this.maybe_like.forEach(function(data, i) { %>
                    <li class="<% if(i%2) { %>even<% } else {%>odd<%}%>">
                        <a href="<%= data.link_url%>" target="_blank" title="<%= data.goods_title %>"><img src="<%= data.image %>"></a>
                        <p class="price">￥<%= data.price %></p>
                    </li>
                    <% }) %>
