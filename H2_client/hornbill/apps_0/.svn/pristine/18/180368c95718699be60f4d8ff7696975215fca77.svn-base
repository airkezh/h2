<script type="text/html" id="pagingNav">
    <div class="pageNav1">
        <? if(page > 0){?>
        <a index="-1" class="pagePrev" href="javascript:void(0)" data-action="prev">&lt;上一页</a>
        <?};?>
        <?
		var left_num = page >5 ? page - 5: 0 , right_num = left_num + 10;
        if ( right_num > pageLen) right_num = pageLen;
        ?>
        <?if (left_num >= 1){?>
        <a index="1" href="javascript:void(0)" class="pageitem <?if(page == 1){?>currentpage<?}?>" data-action="jump" data-page="1"><?= 1?></a>
        <span>...</span>
        <?}?>
        <?for (var i = left_num ;i < right_num;i++ ){?>
        <a index="<?= i?>" href="javascript:void(0)" class="pageitem <?if(page == i){?>currentpage<?}?>" data-action="jump" data-page="<?= i + 1 ?>"><?= i+1?></a>
        <?}?>
        <?if (right_num < pageLen) {?>
        <span>...</span>
        <a index="<?= pageLen-1?>" href="javascript:void(0)" class="pageitem <?if(page == pageLen-1){?>currentpage<?}?>" data-action="jump" data-page="<?= pageLen?>"><?= pageLen?></a>
        <?}?>
        <? if(page != pageLen - 1){?>
        <a class="pageNext" href="javascript:void(0)" index="1" data-action="next">下一页&gt;</a>
        <?}?>
    </div>
</script>
<script type="text/html" id="tmpl_shop_coupons">
<li>
    <i class="label-mark-coupon">券</i>
    <ol class="item-promotion">
        <?
            var isMulti = coupons.length > 1;
            $.each( coupons, function( i, coupon ) {
        ?>
        <li class="<?= isMulti ? 'item-promotion-multi' : 'item-promotion-single'?>"><?=coupon.text?>
            <? if(coupon.coupon_apply) { ?>
            <div class="dropdown-wrap js-coupon coupon-wrap">
                <a class="red_f dropdown-wrap-btn js-coupon-btn" href="javascript: void 0;">立即领取&nbsp;&#9662;</a>
                <div class="js-coupon-panel dropdown-panel label-arrow-top">
                    <div class="hd">领券下单，更多优惠等你拿</div>
                    <ul class="bd">
                    </ul>
                    <span class="info js-info red_f"></span>
                </div>
            </div>
            <? } else if(coupon.link != '') { ?>
            <a class="red_f" href="<?=coupon.link?>" target="_blank"> <?=coupon.link_text?>》</a>
            <?}?>
        </li>
        <?});?>
    </ol>
</li>
</script>
<script type="text/html" id="tmpl_shop_promote">
<?
    var marketToolTypes = {
        'mail': '邮',
        'reduce': '减',
        'discount': '折'
    };

    $.each( datas, function( i, v ) {
        var type = v.type;
?>
    <li>
        <i class="label-mark-<?= type ?>"><?= marketToolTypes[type] ?></i>
        <a class="js-promote-<?=type?>-btn item-promote-btn" href="#"><?= v.brief?>&nbsp;<i>&#9662;</i></a>
        <div class="js-promote-<?=type?>-panel item-promote-panel label-arrow-top">
            <?= v.text ?>
            <p class="note">时间：<?= v.note ?></p>
        </div>
    </li>
<?});?>
</script>

<script>
    fml.vars.count_discuss = "<%= goodsInfo.twitter_stat.discuss_num%>";
    fml.vars.count_dis = {'meaning':"<%= shopCommentsCount %>",'all':"<%= shopCommentsCount %>"};
    fml.vars.count_deal = "<%= goodsInfo.sales || 0 %>";
    fml.vars.count_shs = "<%= shoppingShowCount %>";
    fml.vars.goods = {"id" : "<%=goodsInfo.goods_id %>" , "twitter_id": "<%= goodsInfo.twitter_id%>" ,"pid" :"<%= goodsInfo.pid || 0%>"};
    fml.vars.goods_stock = <%= JSON.stringify(goodsInfo.stock)%>;
    fml.vars.sku = <%= JSON.stringify(goodsInfo.sku) %>;
    fml.vars.hasColor = <%= !!goodsInfo.color %>;
    fml.vars.hasSize  = <%= !!goodsInfo.size %>;
    fml.vars.repertory = <%= goodsInfo.repertory || 0 %>;
    fml.vars.goods_prop = <%= JSON.stringify(goodsInfo.prop)%>;
    fml.vars.shop_id = <%= shopId %>;
    fml.vars.user_id = "<%= this.userInfo.user_id %>";
    fml.vars.satisfy_rate = "<%= goodsInfo.satisfy_rate %>";
    fml.vars.goodsInfo = {
        title: "<%= goodsInfo.goods_title %>",
        picSrc: "<%= goodsInfo.image.image[0] %>",
        twitterId: "<%= goodsInfo.twitter_id %>"
    };
    fml.vars.shareInfo = <%= JSON.stringify(this.share_info) %>;
    fml.vars.hasShoppingShow = <%= !!shoppingShowCount %>;
</script>
