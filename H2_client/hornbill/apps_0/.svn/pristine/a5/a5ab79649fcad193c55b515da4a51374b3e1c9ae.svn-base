@import "base.less";
body{.f(.14rem);}
.container{.pt(4.39rem);.pb(.4rem);.pr;}
.coupon_wrapper,.coupon_wrapper_none{.auto;.w(5.11rem);.h(2.7rem);position: relative;z-index: 1;}
.coupon_wrapper{background: url('http://d02.res.meilishuo.net/pic/_o/ab/3a/cd9e8a3a17336e5d052d4a058879_511_270.cf.png') no-repeat center center;background-size: 100%;}
.coupon_wrapper_none{background: url('http://d03.res.meilishuo.net/pic/_o/f9/a9/fcb435c4e6642bdf16cf9c9ffce2_511_270.cg.png') no-repeat center center;background-size: 100%;}
.coupon_num{.f(1rem);.ml(1.78rem);.w(2.72rem);.h(1.68rem);.l(1.68rem);.white;.pt(.62rem);.c;opacity:0.99;}
.coupon_unit{.f(.4rem);}
.content{.auto;.bgwhite;.border-r(.08rem);.w(5.8rem);.f(.28rem);.l(.36rem);.c;.mt(-1.26rem);.pt(1.26rem);.pb(.46rem);}
