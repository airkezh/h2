@import "../block.less";

body{ .bgc(#fff); }
.title{ .f(.26rem);.fc(#6F6F6F);padding: .23rem 0 .23rem .14rem; }
.classify{
	.w(100%);.hidden;
	.effect{
		.w(1.5rem);.h(1.8rem);.left;.mb(.1rem);.ml(.1rem);
		a{ .block; }
		.img{ .w(1.16rem);.h(1.16rem);margin: .19rem .17rem .04rem .17rem; }
		p{ .f(.22rem);.fc(#666);.c; }
	}
	.effect:nth-child(4n+1){ .ml(.05rem); }
}
