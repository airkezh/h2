.good_detail_wrap{
    .w(300px);
    padding-top: 20px;
    padding-right: 20px;
    float: right;
    .good_detail{
        background: #f7f7f7;
        padding: 15px 20px;
        .contact_wrap{
            padding-bottom: 15px;
            position: relative;
            color: #666666;
            font-size: 12px;
            font-weight: bold;
            line-height: 22px;
            span{
                color: #ff6699;
                a{color: #ff6699;}
            }
            .contact_btn{
                display: block;
                width:75px;
                height: 20px;
                position: absolute;
                top: 0px;
                right: 0px;
                border: 1px solid #ff6699;
                line-height: 20px;
                text-align: center;
                background: #ffffff;
                color: #ff6699;
                cursor: pointer;

            }
        }
        .good_info_wrap{
            border-top: 1px dashed #e4e4e4;
            padding: 16px 0px;
            position: relative;
            .clearfix;
            .good_thumb{
                a{
                    width: 70px;
                    height: 70px;
                    overflow: hidden;
                    display: block;
                }
                float: left;
                padding-top: 3px;
                img{
                    width: 70px;
                }
            }
            .good_info{
                width: 165px;
                float: left;
                margin-left: 20px;
                .good_name{
                    color: #666666;
                    line-height: 18px;
                }
                .good_list{
                    padding-top: 5px;
                    p{
                        line-height: 18px;
                        color: #999999;
                    }
                }
            }
        }
        .good_detailinfo_wrap{
            border-top: 1px dashed #e4e4e4;
            padding: 16px 0px;
            position: relative;
            color: #999999;
            line-height: 25px;
            .itemlist{
                height: 25px;
                line-height: 25px;
                position: relative;
                .no{
                    float: right;
                    font-weight: bold;
                }
                .red{
                    color: #ff6699;
                }
            }
            .itemblank{
                height: 20px;
            }
        }
        .refund_no_wrap{
            border-top: 1px dashed #e4e4e4;
            padding-top: 16px;
            position: relative;
            color: #999999;
            line-height: 25px;
            span{
                color: #666666;
                font-weight: normal;
            }
        }
    }
}